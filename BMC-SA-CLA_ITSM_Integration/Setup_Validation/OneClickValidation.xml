<process draft="false" dev-studio-version="7.8.02">
  <process-def name=":BMC-SA-CLA_ITSM_Integration:Setup_Validation:OneClickValidation">
    <parameters>
      <input global="false" required="true">
        <description>hostname:port of AutomationProduct.</description>
        <from>hostinfo</from>
        <to>automationHostDetails</to>
      </input>
      <output global="false" required="true">
        <to>output</to>
        <from>output</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:BMC-SA-CLA_ITSM_Integration:Setup_Validation:Test AO-DB Connectivity</process-name>
        <parameters>
          <output>
            <from>aoConfigScenario</from>
            <to>aoConfigScenario</to>
          </output>
        </parameters>
        <meta>
          <documentation>
            <description>Connectivity check with database used to support store and forward mechanism.</description>
          </documentation>
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>info</level>
            <parameters>
              <input>
                <value>BMC-SA-CLA_ITSM_Integration:IN OneClickValidation=&gt;</value>
              </input>
              <input>
                <value></value>
              </input>
            </parameters>
          </log>
        </before-execution>
        <location>:call-process[1]</location>
      </call-process>
      <call-process>
        <process-name>:BMC-SA-CLA_ITSM_Integration:Setup_Validation:TestRemedyConnectivity</process-name>
        <parameters>
          <output>
            <from>arConnectionScenario</from>
            <to>arConnectionScenario</to>
          </output>
          <output>
            <from>arInterfaceScenario</from>
            <to>arInterfaceScenario</to>
          </output>
        </parameters>
        <meta>
          <documentation>
            <description>Connectivity and interface check on Remedy related forms</description>
          </documentation>
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[2]</location>
      </call-process>
      <call-process>
        <process-name>:BMC-SA-CLA_ITSM_Integration:Setup_Validation:TestBladeConnectivity</process-name>
        <parameters>
          <input>
            <from>automationHostDetails</from>
            <to>automationHostDetails</to>
          </input>
          <output>
            <from>bladeConnectivityScenario</from>
            <to>bladeConnectivityScenario</to>
          </output>
        </parameters>
        <meta>
          <documentation>
            <description>Connectivity check with Blade. And that appropriate role is applied to Blade user.</description>
          </documentation>
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[3]</location>
      </call-process>
      <assign>
        <assignment>
          <output>
            <to>output</to>
            <value>
              <input />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="aoConfigScenario">
                <input global="false">
                  <from>aoConfigScenario</from>
                </input>
              </replacement-token>
              <replacement-token name="arConnectionScenario">
                <input global="false">
                  <from>arConnectionScenario</from>
                </input>
              </replacement-token>
              <replacement-token name="arInterfaceScenario">
                <input global="false">
                  <from>arInterfaceScenario</from>
                </input>
              </replacement-token>
              <replacement-token name="bladeConnectivityScenario">
                <input global="false">
                  <from>bladeConnectivityScenario</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <OneClickValidation>${aoConfigScenario}${arConnectionScenario}${arInterfaceScenario}${bladeConnectivityScenario}</OneClickValidation>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <from>arConnectionScenario</from>
              </input>
              <input>
                <from>arInterfaceScenario</from>
              </input>
              <input>
                <from>bladeConnectivityScenario</from>
              </input>
            </parameters>
          </log>
        </before-execution>
        <after-execution>
          <log>
            <level>info</level>
            <parameters>
              <input>
                <value>BMC-SA-CLA_ITSM_Integration:OUT OneClickValidation=&gt;</value>
              </input>
              <input>
                <from>output</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <compensation name=":BMC-SA-CLA_ITSM_Integration:Setup_Validation:OneClickValidation:999999999" />
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This is invoked to check overall health status of products working together for this solution. It checks connectivity and interface availiability of Remedy,Blade from AO. It also checks connectivity with db used for store and forward mechanism. In case basic connectivity fails with any of these products from AO, then a standalone utility is invoked that too tries to connect with Remedy and Blade. Output contains results of all the scenarios executed.It is invoked from BSA UI.</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>43c25f3b-77e5-4ddb-97db-84b56566e37d</process-id>
      <author>AoAdmin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>AoAdmin</last-modified-by>
      <last-modified>1473144329856</last-modified>
    </meta>
  </process-def>
  <compensation-def name=":BMC-SA-CLA_ITSM_Integration:Setup_Validation:OneClickValidation:999999999">
    <meta>
      <peer-location>
        <location>this</location>
      </peer-location>
    </meta>
    <parameters>
      <output global="false" required="false">
        <to>output</to>
        <from>output11</from>
      </output>
    </parameters>
    <sequence />
  </compensation-def>
</process>