                </xslt-stylesheet>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//text()]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <input global="true">
                  <configuration>:BMC-SA-CLA_ITSM_Integration:Defaults:separator</configuration>
                </input>
                <output>
                  <to>seperator</to>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>remedyResponse</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>taskIDReceivedFromRemedy</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens />
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0"><xsl:output indent="yes" /><xsl:template match="/"><result><xsl:for-each select="//*"><xsl:choose><xsl:when test="contains(name(.), &quot;TaskID&quot;)"><xsl:copy-of select="." /></xsl:when></xsl:choose></xsl:for-each></result></xsl:template></xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//text()]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <input>
                  <concat>
                    <input>
                      <value>hostname=</value>
                    </input>
                    <input required="false">
                      <from>Hostname</from>
                    </input>
                    <input required="false">
                      <from>seperator</from>
                    </input>
                    <input>
                      <value>jobid=</value>
                    </input>
                    <input required="false">
                      <from>JobId</from>
                    </input>
                    <input required="false">
                      <from>seperator</from>
                    </input>
                    <input>
                      <value>scheduleid=</value>
                    </input>
                    <input required="false">
                      <from>ScheduleId</from>
                    </input>
                    <input required="false">
                      <from>seperator</from>
                    </input>
                    <input>
                      <value>changeID=</value>
                    </input>
                    <input required="false">
                      <from>changeIDReceivedFromRemedy</from>
                    </input>
                    <input required="false">
                      <from>seperator</from>
                    </input>
                    <input>
                      <value>taskID=</value>
                    </input>
                    <input required="false">
                      <from>taskIDReceivedFromRemedy</from>
                    </input>
                    <input required="false">
                      <from>seperator</from>
                    </input>
                    <input>
                      <value>JobVersion=</value>
                    </input>
                    <input required="false">
                      <from>JobVersion</from>
                    </input>
                    <input required="false">
                      <from>seperator</from>
                    </input>
                  </concat>
                </input>
                <output>
                  <to>inputParams</to>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input global="true">
                    <configuration>:BMC-SA-CLA_ITSM_Integration:Persistence:Status</configuration>
                  </input>
                </document>
                <result>
                  <output>
                    <to>status_Ready</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//mapping[@name='Ready']/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <input>
                  <concat>
                    <input>
                      <value>'</value>
                    </input>
                    <input required="false">
                      <from>inputParams</from>
                    </input>
                    <input>
                      <value>','</value>
                    </input>
                    <input required="false">
                      <from>status_Ready</from>
                    </input>
                    <input>
                      <value>','Remedy','</value>
                    </input>
                    <input required="false">
                      <from>JobId</from>
                    </input>
                    <input>
                      <value>',</value>
                    </input>
                    <input required="false">
                      <from>currentTime</from>
                    </input>
                  </concat>
                </input>
                <output>
                  <to>item</to>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
            <assign>
              <assignment>
                <input global="true">
