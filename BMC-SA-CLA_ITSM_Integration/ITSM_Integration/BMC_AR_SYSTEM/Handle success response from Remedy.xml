          </input>
          <input>
            <from>persistenceID</from>
            <to>item</to>
          </input>
          <output>
            <from>list</from>
            <to>completedEntries</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence />
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>changeID</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[not(boolean(string(.)))]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <utility>
              <date>
                <now />
              </date>
              <output global="false">
                <to>currentTime</to>
              </output>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:utility[1]</location>
            </utility>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>remedyResponse</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>changeIDReceivedFromRemedy</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens />
