              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>dbTableColumns</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>columnsToUpdate</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[concat(//mapping[@name='INPUTPARAMETERS'], ",", //mapping[@name='STATUS'], ",", //mapping[@name='ORIGIN'], ",", //mapping[@name='JOBID'], ",", //mapping[@name='CREATETIME'])]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <input global="true">
                  <configuration>:BMC-SA-CLA_ITSM_Integration:Persistence:Tables:INCOMINGMESSAGES:Name</configuration>
                </input>
                <output>
                  <to>incomingMessageTableName</to>
