<process draft="false" dev-studio-version="7.6.01.02">
  <process-def name=":AutoPilot-OA-IP-Address-Management:Utilities:CLI:Build UNC Path">
    <parameters>
      <input global="false" required="true">
        <description>Remote UNC File</description>
        <from>Remote UNC File</from>
        <to>Remote UNC File</to>
      </input>
      <output global="false" required="false">
        <description>UNC Path</description>
        <to>UNC Path</to>
        <from>UNC Path</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <utility>
        <string>
          <regex>
            <input>
              <from>Remote UNC File</from>
            </input>
            <expression>
              <search case-sensitive="false">:\\</search>
              <replace replace-all="true">\$\\</replace>
            </expression>
          </regex>
        </string>
        <output global="false">
          <to>UNC Path</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:utility[1]</location>
      </utility>
      <assign>
        <assignment>
          <output>
            <to>UNC Path</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="UNC Path">
                <input global="false">
                  <from>UNC Path</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <unc-path>
      <xsl:text disable-output-escaping="no">\\$[UNC Path]</xsl:text>
    </unc-path>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Strips Leading and Trailing Quotes from a string</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>14551c89-f8c7-45a0-8321-eac82ae3e49e</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1293185286213</last-modified>
    </meta>
  </process-def>
</process>