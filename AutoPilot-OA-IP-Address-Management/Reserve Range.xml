<process draft="false" dev-studio-version="7.6.01.04">
  <process-def name=":AutoPilot-OA-IP-Address-Management:Reserve Range">
    <parameters>
      <input global="false" required="true">
        <description>Configuration name from module configuration.
Valid values: VitalQIP.</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <from>range address</from>
        <to>range address</to>
      </input>
      <input global="false" required="true">
        <from>pools</from>
        <to>pools</to>
      </input>
      <input global="false" required="false">
        <description>Scope can have twos possible values
1. Any string : It is a organization name same as pod name. 
2. null : in this case we will use default organization.</description>
        <from>scope</from>
        <to>scope</to>
      </input>
      <input global="false" required="false">
        <from>network bits</from>
        <to>network bits</to>
      </input>
      <output global="false" required="false">
        <description>operation status.
valid values: true, false.</description>
        <to>success status</to>
        <from>success</from>
      </output>
      <output global="false" required="false">
        <description>incase of any error, error message goes here.  </description>
        <to>error message</to>
        <from>error message</from>
      </output>
      <output global="false" required="false">
        <description>List of pool ids.
&lt;pool-ids&gt;
     &lt;pool-id&gt;a.b.c.d&lt;/pool-id&gt;
     &lt;pool-id&gt;p.q.r.s&lt;/pool-id&gt;
     &lt;pool-id&gt;l.m.n.o&lt;/pool-id&gt;
&lt;pool-ids&gt;</description>
        <to>pool ids</to>
        <from>pool ids</from>
      </output>
      <output global="false" required="false">
        <description>incase of few pools fails to reserve, the detailes information regarding those pools will be returned as a info message.</description>
        <to>info message</to>
        <from>info-message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-IP-Address-Management:Utilities:Extract Configuration Data</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <output>
            <from>user name</from>
            <to>user name</to>
          </output>
          <output>
            <from>password</from>
            <to>password</to>
          </output>
          <output>
            <from>organization</from>
            <to>organization</to>
          </output>
          <output>
            <from>adapter name</from>
            <to>adapter name</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>error message</to>
                  <value>This IPAM solution s not supported</value>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>success</to>
                  <value>false</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>configuration name</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[text()="VitalQIP"]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>info-message</to>
                  <value></value>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>pool ids</to>
                  <value>
                    <pool-ids>
</pool-ids>
                  </value>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>success</to>
                  <value>false</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
            <for-each>
              <document>
                <input>
                  <from>pools</from>
                </input>
              </document>
              <result>
                <output>
                  <to>pool</to>
                </output>
              </result>
              <xpath-expression>
                <xpath-replacement-tokens />
                <xpath><![CDATA[//pool]]></xpath>
              </xpath-expression>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>true</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <sequence>
                <assign>
                  <assignment>
                    <document>
                      <input>
                        <from>pool</from>
                      </input>
                    </document>
                    <result>
                      <output>
                        <to>pool-address</to>
                      </output>
                    </result>
                    <xpath-expression>
                      <xpath-replacement-tokens />
                      <xpath><![CDATA[//pool-address/text()]]></xpath>
                    </xpath-expression>
                  </assignment>
                  <assignment>
                    <document>
                      <input>
                        <from>pool</from>
                      </input>
                    </document>
                    <result>
                      <output>
                        <to>pool-mask</to>
                      </output>
                    </result>
                    <xpath-expression>
                      <xpath-replacement-tokens />
                      <xpath><![CDATA[//pool-mask/text()]]></xpath>
                    </xpath-expression>
                  </assignment>
                  <meta>
                    <documentation />
                    <checkpoint>false</checkpoint>
                  </meta>
                  <location>:switch[1]:sequence[2]:assign[2]</location>
                </assign>
                <call-process>
                  <process-name>:AutoPilot-OA-IP-Address-Management:Reserve Pool</process-name>
                  <parameters>
                    <input>
                      <from>configuration name</from>
                      <to>configuration name</to>
                    </input>
                    <input>
                      <from>pool-address</from>
                      <to>pool address</to>
                    </input>
                    <input>
                      <from>pool-mask</from>
                      <to>pool mask</to>
                    </input>
                    <input>
                      <from>scope</from>
                      <to>scope</to>
                    </input>
                    <input>
                      <from>range address</from>
                      <to>range address</to>
                    </input>
                    <input>
                      <from>network bits</from>
                      <to>network bits</to>
                    </input>
                    <input>
                      <value>false</value>
                      <to>pool operation</to>
                    </input>
                    <output>
                      <from>success status</from>
                      <to>reserve success</to>
                    </output>
                    <output>
                      <from>error message</from>
                      <to>reserve error</to>
                    </output>
                    <output>
                      <from>pool id</from>
                      <to>reserve id</to>
                    </output>
                  </parameters>
                  <meta>
                    <documentation />
                    <run-mode-handling>
                      <run-mode>OCP</run-mode>
                      <allow-step-into>false</allow-step-into>
                    </run-mode-handling>
                    <checkpoint>false</checkpoint>
                  </meta>
                  <location>:switch[1]:sequence[2]:call-process[1]</location>
                </call-process>
                <switch>
                  <meta>
                    <documentation />
                    <run-mode-handling>
                      <run-mode>OCP</run-mode>
                      <allow-step-into>true</allow-step-into>
                    </run-mode-handling>
                    <checkpoint>false</checkpoint>
                  </meta>
                  <execute-all-applicable-cases>false</execute-all-applicable-cases>
                  <default>
                    <sequence>
                      <utility>
                        <string>
                          <concat>
                            <input>
                              <from>info-message</from>
                            </input>
                            <input>
                              <concat>
                                <input>
                                  <value>Failed to reserve a pool: </value>
                                </input>
                              </concat>
                            </input>
                            <input>
                              <from>pool-address</from>
                            </input>
                            <input>
                              <concat>
                                <input>
                                  <value>. Error:</value>
                                </input>
                              </concat>
                            </input>
                            <input>
                              <from>reserve error</from>
                            </input>
                            <input>
                              <concat>
                                <input>
                                  <value> </value>
                                </input>
                              </concat>
                            </input>
                          </concat>
                        </string>
                        <output global="false">
                          <to>info-message</to>
                        </output>
                        <meta>
                          <documentation />
                          <checkpoint>false</checkpoint>
                        </meta>
                        <location>:switch[1]:sequence[2]:switch[1]:sequence[1]:utility[1]</location>
                      </utility>
                    </sequence>
                  </default>
                  <case>
                    <condition>
                      <document>
                        <input>
                          <from>reserve success</from>
                        </input>
                      </document>
                      <xpath-expression>
                        <xpath-replacement-tokens />
                        <xpath><![CDATA[text() = 'true']]></xpath>
                      </xpath-expression>
                    </condition>
                    <sequence>
                      <assign>
                        <assignment>
                          <document>
                            <input>
                              <from>pool ids</from>
                            </input>
                          </document>
                          <result>
                            <output>
                              <to>pool ids</to>
                            </output>
                          </result>
                          <xslt-stylesheet>
                            <stylesheet-replacement-tokens>
                              <replacement-token name="pool-id">
                                <input global="false">
                                  <from>reserve id</from>
                                </input>
                              </replacement-token>
                              <replacement-token name="scope">
                                <input global="false">
                                  <from>scope</from>
                                </input>
                              </replacement-token>
                              <replacement-token name="network">
                                <input global="false">
                                  <from>range address</from>
                                </input>
                              </replacement-token>
                            </stylesheet-replacement-tokens>
                            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <pool-ids>
      <xsl:copy-of select="//pool-ids/*" />
      <xsl:if test="string-length(&quot;${scope}&quot;) &gt; 0">
        <pool-id>
          <xsl:text disable-output-escaping="no">${pool-id}:-:${network}:-:${scope}</xsl:text>
        </pool-id>
      </xsl:if>
      <xsl:if test="string-length(&quot;${scope}&quot;) = 0">
        <pool-id>
          <xsl:text disable-output-escaping="no">${pool-id}:-:${network}</xsl:text>
        </pool-id>
      </xsl:if>
    </pool-ids>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                          </xslt-stylesheet>
                        </assignment>
                        <assignment>
                          <output>
                            <to>success</to>
                            <value>true</value>
                          </output>
                        </assignment>
                        <meta>
                          <documentation />
                          <checkpoint>false</checkpoint>
                        </meta>
                        <location>:switch[1]:sequence[2]:switch[1]:sequence[2]:assign[1]</location>
                      </assign>
                    </sequence>
                  </case>
                  <location>:switch[1]:sequence[2]:switch[1]</location>
                </switch>
              </sequence>
              <location>:switch[1]:sequence[2]:for-each[1]</location>
            </for-each>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>f88239ad-e875-44ba-8776-53bb5c7652d8</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1298978133514</last-modified>
    </meta>
  </process-def>
</process>