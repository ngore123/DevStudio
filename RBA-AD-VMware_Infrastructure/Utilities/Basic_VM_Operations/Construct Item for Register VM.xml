<process draft="false" dev-studio-version="7.6.02.05">
  <process-def name=":RBA-AD-VMware_Infrastructure:Utilities:Basic_VM_Operations:Construct Item for Register VM">
    <parameters>
      <input global="false" required="true">
        <description>Name of the folder inside which the virtual machine should be registered.
 
Valid value:  String
</description>
        <from>folder</from>
        <to>folder</to>
      </input>
      <input global="false" required="true">
        <description>Datastore path to the virtual machine.

For example: [datastorename]containername/VMXNameOfVirtualMachine.vmx</description>
        <from>path</from>
        <to>path</to>
      </input>
      <input global="false" required="false">
        <description>The name to be assigned to the virtual machine. If this parameter is not set, the displayName configuration parameter of the virtual machine is used.

Valid values: String</description>
        <from>name</from>
        <to>name</to>
      </input>
      <input global="false" required="false">
        <description>Flag to specify whether or not the virtual machine should be marked as a template.

Valid values: true, false</description>
        <from>as template</from>
        <to>as template</to>
      </input>
      <input global="false" required="false">
        <description>The resource pool to which the virtual machine should be attached. If imported as a template, this parameter is not set.</description>
        <from>resource pool</from>
        <to>resource pool</to>
      </input>
      <input global="false" required="false">
        <description>The target host on which the virtual machine will run. For a stand-alone host or a cluster with DRS, the parameter can be omitted, and the system selects a default. </description>
        <from>host system</from>
        <to>host system</to>
      </input>
      <output global="false" required="false">
        <to>items</to>
        <from>items</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="folder">
                <input global="false">
                  <from>folder</from>
                </input>
              </replacement-token>
              <replacement-token name="path">
                <input global="false">
                  <from>path</from>
                </input>
              </replacement-token>
              <replacement-token name="name">
                <input global="false">
                  <from>name</from>
                </input>
              </replacement-token>
              <replacement-token name="as template">
                <input global="false">
                  <from>as template</from>
                </input>
              </replacement-token>
              <replacement-token name="resource pool">
                <input global="false">
                  <from>resource pool</from>
                </input>
              </replacement-token>
              <replacement-token name="host system">
                <input global="false">
                  <from>host system</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <items>
      <item>
        <parameters>
          <folder>${folder}</folder>
          <path>${path}</path>
          <name>
            <xsl:if test="string-length('${name}') &gt;0">${name}</xsl:if>
          </name>
          <as-template>
            <xsl:choose>
              <xsl:when test="string-length('${as template}') &gt;0">${as template}</xsl:when>
              <xsl:otherwise>
                <xsl:text disable-output-escaping="no">false</xsl:text>
              </xsl:otherwise>
            </xsl:choose>
          </as-template>
          <resource-pool>
            <xsl:if test="string-length('${resource pool}') &gt;0">${resource pool}</xsl:if>
          </resource-pool>
          <host-system>
            <xsl:if test="string-length('${host system}') &gt;0">${host system}</xsl:if>
          </host-system>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Constructs &lt;items&gt; for Register VM operation</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>0963ca49-8f61-4ea4-802e-be6834999e44</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1356513219021</last-modified>
    </meta>
  </process-def>
</process>