<process draft="false" dev-studio-version="7.6.01.02">
  <process-def name=":RBA-AD-VMware_Infrastructure:Utilities:Advanced_VM_Operations:Construct Item for Delete VM Snapshot">
    <parameters>
      <input global="false" required="true">
        <description>virtual machine
	Virtual machine name used to make the method call.</description>
        <from>virtual machine</from>
        <to>virtual machine</to>
      </input>
      <input global="false" required="true">
        <description>snapshot name
		       Name of the Snapshot to be deleted.</description>
        <from>snapshot name</from>
        <to>snapshot name</to>
      </input>
      <input global="false" required="true">
        <description>remove children [Optional]
	Flag to specify removal of the entire snapshot subtree or not.

Valid Values: true/false
</description>
        <from>remove children</from>
        <to>remove children</to>
      </input>
      <output global="false" required="false">
        <to>items</to>
        <from>items</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="snapshot name">
                <input global="false">
                  <from>snapshot name</from>
                </input>
              </replacement-token>
              <replacement-token name="virtual machine">
                <input global="false">
                  <from>virtual machine</from>
                </input>
              </replacement-token>
              <replacement-token name="remove children">
                <input global="false">
                  <from>remove children</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <items>
      <item>
        <parameters>
          <virtual-machine>${virtual machine}</virtual-machine>
          <name>${snapshot name}</name>
          <remove-children>${remove children}</remove-children>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
