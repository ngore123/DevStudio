<process draft="false" dev-studio-version="7.6.01.02">
  <process-def name=":RBA-AD-VMware_Infrastructure:Utilities:VM_Configuration_Operations:Construct Item for Update Ethernet Adapter Configuration">
    <parameters>
      <input global="false" required="true">
        <description>virtual machine
	Virtual machine name</description>
        <from>virtual machine</from>
        <to>virtual machine</to>
      </input>
      <input global="false" required="false">
        <description>host system [optional]
	The host system associated with the DV Port Group and DV Switch.</description>
        <from>host system</from>
        <to>host system</to>
      </input>
      <input global="false" required="false">
        <description>Device type attribute is optional. 
Default value is VirtualPCNet32

Supported Values: VirtualE1000, VirtualPCNet32, VirtualVmxnet
</description>
        <from>device type</from>
        <to>device type</to>
      </input>
      <input global="false" required="false">
        <description>backing type [Optional]

Valid values: VirtualEthernetCardDistributedVirtualPortBackingInfo, VirtualEthernetCardNetworkBackingInfo</description>
        <from>backing type</from>
        <to>backing type</to>
      </input>
      <input global="false" required="true">
        <description>Name of the Ethernet Adapter to be udpated.
</description>
        <from>ethernet adapter</from>
        <to>ethernet adapter</to>
      </input>
      <input global="false" required="false">
        <description>backing device name [optional]
	The name of the device on the host system
</description>
        <from>backing device name</from>
        <to>backing device name</to>
      </input>
      <input global="false" required="false">
        <description>backing dv switch [optional]
	The name of distributed virtual switch.</description>
        <from>backing dv switch</from>
        <to>backing dv switch</to>
      </input>
      <input global="false" required="false">
        <description>backing dv portgroup [optional]
	The name of distributed virtual portgroup</description>
        <from>backing dv portgroup</from>
        <to>backing dv portgroup</to>
      </input>
      <input global="false" required="false">
        <description>Information about restrictions on removing this device while a virtual machine is running

Takes in boolean value either true or false</description>
        <from>start connected</from>
        <to>start connected</to>
      </input>
      <input global="false" required="false">
        <description>Information about restrictions on removing this device while a virtual machine is running

Takes in boolean value either true or false</description>
        <from>allow guest control</from>
        <to>allow guest control</to>
      </input>
      <input global="false" required="false">
        <description>Information about restrictions on removing this device while a virtual machine is running

Takes in boolean value either true or false</description>
        <from>connected</from>
        <to>connected</to>
      </input>
      <input global="false" required="false">
        <description>MAC address type. 

Valid values for address type are: manual 

Statically assigned MAC address: generated 

Automatically generated MAC address: assigned 

MAC address assigned by VirtualCenter.
</description>
        <from>address type</from>
        <to>address type</to>
      </input>
      <input global="false" required="false">
        <description>MAC address assigned to the virtual network adapter. Clients can set this property to any of the allowed address types. The server might override the specified value for "Generated" or "Assigned" if it does not fall in the right ranges or is determined to be a duplicate.
</description>
        <from>mac address</from>
        <to>mac address</to>
      </input>
      <input global="false" required="false">
        <description>Indicates whether wake-on-LAN is enabled on this virtual network adapter. Clients can set this property to selectively enable or disable wake-on-LAN.
</description>
        <from>wake on lan enabled</from>
        <to>wake on lan enabled</to>
      </input>
      <output global="false" required="false">
        <to>items</to>
        <from>items</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
