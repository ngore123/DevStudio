<process draft="false" dev-studio-version="7.6.01.02">
  <process-def name=":RBA-AD-VMware_Infrastructure:Utilities:Construct Connection Parameters">
    <parameters>
      <input global="false" required="false">
        <description>alternate username    [Optional]
            The username use to handle the request.</description>
        <from>alternate username</from>
        <to>alternate username</to>
      </input>
      <input global="false" required="false">
        <description>alternate password    [Optional]
            The password use to handle the request</description>
        <from>alternate password</from>
        <to>alternate password</to>
      </input>
      <input global="false" required="false">
        <description>alternate soap URL
	The Virtual Center Server SOAP URL used to execute the request.</description>
        <from>alternate soap URL</from>
        <to>alternate soap URL</to>
      </input>
      <input global="false" required="false">
        <description>alternate transport
	The Virtual Center Server trasnport protocol used to execute the request.</description>
        <from>alternate transport</from>
        <to>alternate transport</to>
      </input>
      <output global="false" required="false">
        <to>connection parameters</to>
        <from>connection parameters</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>connection parameters</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
