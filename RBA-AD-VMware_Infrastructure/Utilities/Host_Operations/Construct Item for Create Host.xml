<process draft="false" dev-studio-version="7.6.01.02">
  <process-def name=":RBA-AD-VMware_Infrastructure:Utilities:Host_Operations:Construct Item for Create Host">
    <parameters>
      <input global="false" required="false">
        <description>host folder
	Folder used to make the method call.</description>
        <from>host folder</from>
        <to>host folder</to>
      </input>
      <input global="false" required="true">
        <description>Name of the datacenter where host has to be created.</description>
        <from>datacenter</from>
        <to>datacenter</to>
      </input>
      <input global="false" required="true">
        <description>host
       The DNS name of the host. </description>
        <from>host</from>
        <to>host</to>
      </input>
      <input global="false" required="false">
        <description>port [optional]
      The port number for the connection. If this is not specified, the default port number (port 902) is used. If this is a reconnect, the port number is unchanged. </description>
        <from>port</from>
        <to>port</to>
      </input>
      <input global="false" required="true">
        <description>host username
           The administration account on the host.</description>
        <from>host username</from>
        <to>host username</to>
      </input>
      <input global="false" required="true">
        <description>host password
         The password for the administration account.</description>
        <from>host password</from>
        <to>host password</to>
      </input>
      <input global="false" required="true">
        <description>force
       If this flag is set to "true", then the connection succeeds even if the host is already being managed by another VirtualCenter server. The original VirtualCenter server loses connection to the host. 

Valid values: true, false</description>
        <from>force</from>
        <to>force</to>
      </input>
      <input global="false" required="false">
        <description>vim account name [optional]
        The username to be used for accessing the virtual machine files on the disk.</description>
        <from>vim account name</from>
        <to>vim account name</to>
      </input>
      <input global="false" required="false">
        <description>vim account password [optional]
        The password to be used with the vimAccountName property for accessing the virtual machine files on the disk.</description>
        <from>vim account password</from>
        <to>vim account password</to>
      </input>
      <input global="false" required="false">
        <description>vm folder [optional]
         Folder in which to store the existing virtual machines on the host. If this folder is not specified, a default folder is chosen (and possibly created) by the VirtualCenter. This folder exists (or is possibly created) on the VirtualCenter server and is called "Discovered VM". </description>
        <from>vm folder</from>
        <to>vm folder</to>
      </input>
      <input global="false" required="true">
        <description>add connected
         Flag to specify whether or not the host should be connected as soon as it is added. The creation operation fails if a connection attempt is made and fails. 

valid values: true, false</description>
        <from>add connected</from>
        <to>add connected</to>
      </input>
      <output global="false" required="false">
        <to>items</to>
        <from>items</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="host name">
                <input global="false">
                  <from>host</from>
                </input>
              </replacement-token>
              <replacement-token name="port">
                <input global="false">
                  <from>port</from>
                </input>
              </replacement-token>
              <replacement-token name="host username">
                <input global="false">
                  <from>host username</from>
                </input>
              </replacement-token>
              <replacement-token name="host password">
                <input global="false">
                  <from>host password</from>
                </input>
              </replacement-token>
              <replacement-token name="vm folder">
                <input global="false">
                  <from>vm folder</from>
                </input>
              </replacement-token>
              <replacement-token name="force">
                <input global="false">
                  <from>force</from>
                </input>
              </replacement-token>
              <replacement-token name="vim account name">
                <input global="false">
                  <from>vim account name</from>
                </input>
              </replacement-token>
              <replacement-token name="vim account password">
                <input global="false">
                  <from>vim account password</from>
                </input>
              </replacement-token>
              <replacement-token name="host folder">
                <input global="false">
                  <from>host folder</from>
                </input>
              </replacement-token>
              <replacement-token name="add connected">
                <input global="false">
                  <from>add connected</from>
                </input>
              </replacement-token>
              <replacement-token name="datacenter">
                <input global="false">
                  <from>datacenter</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <items>
      <item>
        <parameters>
          <folder>${host folder}</folder>
          <datacenter>${datacenter}</datacenter>
          <spec>
            <host-name>${host name}</host-name>
            <port>${port}</port>
            <user-name>${host username}</user-name>
            <password>${host password}</password>
            <force>${force}</force>
            <vim-account-name>${vim account name}</vim-account-name>
            <vim-account-password>${vim account password}</vim-account-password>
            <folder>${vm folder}</folder>
          </spec>
          <add-connected>${add connected}</add-connected>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Constructs &lt;items&gt; for Create Host operation</description>
        <version>1.0</version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
