<process draft="false" dev-studio-version="7.6.01.02">
  <process-def name=":RBA-AD-VMware_Infrastructure:Utilities:Host_Operations:Construct Item for Add Internet Scsi Send Targets">
    <parameters>
      <input global="false" required="true">
        <description>DNS name of the host.

Valid value:  String</description>
        <from>host system</from>
        <to>host system</to>
      </input>
      <input global="false" required="true">
        <description>The device of the Internet SCSI HBA adapter. 

Valid value: String</description>
        <from>iScsi hba device</from>
        <to>iScsi hba device</to>
      </input>
      <input global="false" required="true">
        <description>An array of iSCSI send targets. Takes input as XML with following parameters:

&lt;targets-list&gt;
&lt;!-- One or more targets --&gt;
	&lt;targets&gt;
		&lt;address&gt;&lt;/address&gt;
		&lt;port&gt;&lt;/port&gt;
 	&lt;/targets&gt;
&lt;/targets-list&gt;</description>
        <from>targets list</from>
        <to>targets list</to>
      </input>
      <output global="false" required="false">
        <to>items</to>
        <from>items</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="host system">
                <input global="false">
                  <from>host system</from>
                </input>
              </replacement-token>
              <replacement-token name="iScsi hba device">
                <input global="false">
                  <from>iScsi hba device</from>
                </input>
              </replacement-token>
              <replacement-token name="targets list">
                <input global="false">
                  <from>targets list</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <items>
      <item>
        <parameters>
          <host-system>${host system}</host-system>
          <iScsi-hba-device>${iScsi hba device}</iScsi-hba-device>
          ${targets list}
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
