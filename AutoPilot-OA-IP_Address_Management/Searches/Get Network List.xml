<process draft="false" dev-studio-version="7.6.02.05">
  <process-def name=":AutoPilot-OA-IP_Address_Management:Searches:Get Network List">
    <parameters>
      <input global="false" required="true">
        <description>String Value : Name of the configuration name 

Valid Value :Infoblox</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] 
	Specify the network or subnetwork.</description>
        <from>network</from>
        <to>network</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] 
	Specify the mask for the network.</description>
        <from>network mask</from>
        <to>network mask</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] 
	Specify the network view in which the network is located. By default, all network views are searched (except when contain_address is specified).

This parameter is applicable for Infoblox only</description>
        <from>network view</from>
        <to>network view</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] 
	Specify the network container, in CIDR format, to which this network belongs.
	e.g. CIDR Format : 10.0.0.0/24</description>
        <from>network container</from>
        <to>network container</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] 
	Specify an IPv4 address in string format. If specified, all other search attributes are ignored, except for network_view. When a value for this attribute is provided, network_view defaults to the system-defined default network view, unless specified. The returned network is the smallest network enclosing this IPv4 address.


This parameter is applicable for Infoblox only</description>
        <from>contains address</from>
        <to>contains address</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] 
	Specify the type of network for search. Valid values ipv6, ipv4
Default :ipv4</description>
        <from>network type</from>
        <to>network type</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] Specify the extensible attributes for the network. E.g. Building, site etc.

key[Optional]
	Specify the key for the attribute

value[Optional]
	Specify the value for the attribute


&lt;extensible-attributes&gt;
				&lt;attribute&gt;
					&lt;key&gt;Building&lt;/key&gt;
					&lt;value&gt;sample&lt;/value&gt;
				&lt;/attribute&gt;
&lt;/extensible-attributes&gt;

This parameter is applicable for Infoblox only</description>
        <from>extensible attributes</from>
        <to>extensible attributes</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
   Specify the user comment for the network</description>
        <from>comment</from>
        <to>comment</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]:
			Host name or IP address of InfoBlox. This tag is required while providing dynamic targets.
</description>
        <from>hostname</from>
        <to>hostname</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]:Username required to connect InfoBlox. This tag is required while providing dynamic targets.</description>
        <from>username</from>
        <to>username</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]:
			The password associated with the "username" provided. Encrypted password can be provided by providing an "encryption-type" attribute. This tag is required while providing dynamic targets. 
			Valid values for encryption-type are: Plain, Base64
			Default value: Plain

		</description>
        <from>password</from>
        <to>password</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]: specified only when password parameter is mentioned and its a required when dynamic targeting
You can encrypt the password by specifying the encryption type .
Valid values:  Base64,Plain.
Note : should be specified when password is provided</description>
        <from>encryption-type</from>
        <to>encryption-type</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
			Specifies the time, in seconds, after which the attempt to connect to the InfoBlox fails
			Default value in seconds: 60</description>
        <from>timeout-secs</from>
        <to>timeout-secs</to>
      </input>
      <output global="false" required="false">
        <description>String Value - A process specific explanation for the value of success status.  Contains the error message in case of a failure scenario.
</description>
        <to>status message</to>
        <from>status message</from>
      </output>
      <output global="false" required="false">
        <description>String Value - Indicates whether or not the process completed successfully. 

Valid values: success or failure</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="false">
        <description>XML value - Contains required output, in case of success</description>
        <to>output</to>
        <from>output</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-IP_Address_Management:Utilities:Extract Configuration Data</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <output>
            <from>adapter</from>
            <to>adapter</to>
          </output>
          <output>
            <from>version</from>
            <to>version</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>failure</value>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>configuration name</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>status message</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[concat("Unknown configuration name: ", .)]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>configuration name</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[string(.) ='Infoblox']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>configuration name</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>items</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens>
                    <replacement-token name="host">
                      <input global="false">
                        <from>hostname</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="username">
                      <input global="false">
                        <from>username</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="e-type">
                      <input global="false">
                        <from>encryption-type</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="password">
                      <input global="false">
                        <from>password</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="timeout secs">
                      <input global="false">
                        <from>timeout-secs</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="network">
                      <input global="false">
                        <from>network</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="mask">
                      <input global="false">
                        <from>network mask</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="comment">
                      <input global="false">
                        <from>comment</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="contains address">
                      <input global="false">
                        <from>contains address</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="view">
                      <input global="false">
                        <from>network view</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="extn attributes">
                      <input global="false">
                        <from>extensible attributes</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="container">
                      <input global="false">
                        <from>network container</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="type">
                      <input global="false">
                        <from>network type</from>
                      </input>
                    </replacement-token>
                  </stylesheet-replacement-tokens>
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <items>
      <item>
        <xsl:if test="string-length('${username}') &gt; 0">
          <target>
            <host>
              <xsl:value-of select="'${host}'" disable-output-escaping="no" />
            </host>
            <user-name>
              <xsl:value-of select="'${username}'" disable-output-escaping="no" />
            </user-name>
            <password>
              <xsl:attribute name="encryption-type">
                <xsl:value-of select="'${e-type}'" disable-output-escaping="no" />
              </xsl:attribute>
              <xsl:value-of select="'${password}'" disable-output-escaping="no" />
            </password>
            <xsl:if test="string-length('${timeout secs}')&gt;0">
              <timeout-secs>
                <xsl:value-of select="'${timeout secs}'" disable-output-escaping="no" />
              </timeout-secs>
            </xsl:if>
          </target>
        </xsl:if>
        <parameters>
          <xsl:if test="string-length('${network}')&gt;0">
            <network>
              <xsl:value-of select="'${network}'" disable-output-escaping="no" />
            </network>
          </xsl:if>
          <xsl:if test="string-length('${mask}')&gt;0">
            <mask>
              <xsl:value-of select="'${mask}'" disable-output-escaping="no" />
            </mask>
          </xsl:if>
          <xsl:if test="string-length('${comment}') &gt;0">
            <comment>
              <xsl:value-of select="'${comment}'" disable-output-escaping="no" />
            </comment>
          </xsl:if>
          <xsl:if test="string-length('${view}')&gt;0">
            <network-view>
              <xsl:value-of select="'${view}'" disable-output-escaping="no" />
            </network-view>
          </xsl:if>
          <xsl:if test="string-length('${contains address}')&gt;0">
            <contains-address>
              <xsl:value-of select="'${contains address}'" disable-output-escaping="no" />
            </contains-address>
          </xsl:if>
          <xsl:if test="string-length('${container}')&gt;0">
            <network-container>
              <xsl:value-of select="'${container}'" disable-output-escaping="no" />
            </network-container>
          </xsl:if>
          <xsl:if test="string-length('${type}')&gt;0">
            <network-type>
              <xsl:value-of select="'${type}'" disable-output-escaping="no" />
            </network-type>
          </xsl:if>
          ${extn attributes}
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
            <call-process>
              <process-name>:AO-AD-Infoblox-NIOS:Find Items</process-name>
              <parameters>
                <input>
                  <from>adapter</from>
                  <to>adapter name</to>
                </input>
                <input>
                  <value>retrieve-network</value>
                  <to>item type</to>
                </input>
                <input>
                  <from>items</from>
                  <to>items</to>
                </input>
                <input>
                  <from>version</from>
                  <to>version</to>
                </input>
                <output>
                  <from>adapter response</from>
                  <to>adapter response</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:call-process[1]</location>
            </call-process>
            <switch>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>true</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <execute-all-applicable-cases>false</execute-all-applicable-cases>
              <default>
                <sequence>
                  <assign>
                    <assignment>
                      <output>
                        <to>success status</to>
                        <value>failure</value>
                      </output>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>adapter response</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>status message</to>
                        </output>
                      </result>
                      <xpath-expression>
                        <xpath-replacement-tokens />
                        <xpath><![CDATA[string(//error)]]></xpath>
                      </xpath-expression>
                    </assignment>
                    <meta>
                      <documentation />
                      <checkpoint>false</checkpoint>
                    </meta>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[1]:assign[1]</location>
                  </assign>
                </sequence>
              </default>
              <case>
                <condition>
                  <document>
                    <input>
                      <from>adapter response</from>
                    </input>
                  </document>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//responses/retrieve-network-response/metadata/status !='error']]></xpath>
                  </xpath-expression>
                </condition>
                <sequence>
                  <assign>
                    <assignment>
                      <output>
                        <to>success status</to>
                        <value>success</value>
                      </output>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>adapter response</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>output</to>
                        </output>
                      </result>
                      <xpath-expression>
                        <xpath-replacement-tokens />
                        <xpath><![CDATA[//output]]></xpath>
                      </xpath-expression>
                    </assignment>
                    <meta>
                      <documentation />
                      <checkpoint>false</checkpoint>
                    </meta>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[2]:assign[1]</location>
                  </assign>
                </sequence>
              </case>
              <location>:switch[1]:sequence[2]:switch[1]</location>
            </switch>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This process retrives network list in a given network</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>275730b1-e810-4d16-acc8-0d0c6a1f8538</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1343384200382</last-modified>
    </meta>
  </process-def>
</process>