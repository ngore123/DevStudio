<process draft="false" dev-studio-version="7.6.02.05">
  <process-def name=":AutoPilot-OA-IP_Address_Management:Searches:Get Addresses By Status">
    <parameters>
      <input global="false" required="true">
        <description>String Value : Name of the configuration name 

Valid Value :Infoblox</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>Specify the network or subnetwork.</description>
        <from>network</from>
        <to>network</to>
      </input>
      <input global="false" required="true">
        <description>	Specify the mask for the network.</description>
        <from>network mask</from>
        <to>network mask</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] 
	Specify the network view in which the network is located.

This parameter is applicable for Infoblox only</description>
        <from>network view</from>
        <to>network view</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
The filter type of the IPAM address. Valid values are "used", "unused" and "all". 
Default value is "all" for IPV4.
For IPv6 only supported valid value is "used".
</description>
        <from>status</from>
        <to>status</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]:
			Host name or IP address of InfoBlox. This tag is required while providing dynamic targets.
</description>
        <from>hostname</from>
        <to>hostname</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]:Username required to connect InfoBlox. This tag is required while providing dynamic targets.</description>
        <from>username</from>
        <to>username</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]:
			The password associated with the "username" provided. Encrypted password can be provided by providing an "encryption-type" attribute. This tag is required while providing dynamic targets. 
			Valid values for encryption-type are: Plain, Base64
			Default value: Plain

		</description>
        <from>password</from>
        <to>password</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]: specified only when password parameter is mentioned and its a required when dynamic targeting
You can encrypt the password by specifying the encryption type .
Valid values:  Base64,Plain.
Note : should be specified when password is provided</description>
        <from>encryption-type</from>
        <to>encryption-type</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
			Specifies the time, in seconds, after which the attempt to connect to the InfoBlox fails
			Default value in seconds: 60</description>
        <from>timeout-secs</from>
        <to>timeout-secs</to>
      </input>
      <output global="false" required="false">
        <description>String Value - Indicates whether or not the process completed successfully. 

Valid values: success or failure</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="false">
        <description>String Value - A process specific explanation for the value of success status.  Contains the error message in case of a failure scenario.</description>
        <to>status message</to>
        <from>status message</from>
      </output>
      <output global="false" required="false">
        <description>XML value - Contains required output, in case of success</description>
        <to>output</to>
        <from>output</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-IP_Address_Management:Utilities:Extract Configuration Data</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <output>
            <from>adapter</from>
            <to>adapter</to>
          </output>
          <output>
            <from>version</from>
            <to>version</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>failure</value>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>configuration name</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>status message</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[concat("Unknown configuration name: ", .)]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>configuration name</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[string(.) ='Infoblox']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>configuration name</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>items</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens>
                    <replacement-token name="host">
                      <input global="false">
                        <from>hostname</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="username">
                      <input global="false">
                        <from>username</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="e-type">
                      <input global="false">
                        <from>encryption-type</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="password">
                      <input global="false">
                        <from>password</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="timeout secs">
                      <input global="false">
                        <from>timeout-secs</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="network">
                      <input global="false">
                        <from>network</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="mask">
                      <input global="false">
                        <from>network mask</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="view">
                      <input global="false">
                        <from>network view</from>
                      </input>
                    </replacement-token>
                    <replacement-token name="status">
                      <input global="false">
                        <from>status</from>
                      </input>
                    </replacement-token>
                  </stylesheet-replacement-tokens>
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <items>
      <item>
        <xsl:if test="string-length('${username}') &gt; 0">
          <target>
            <host>
              <xsl:value-of select="'${host}'" disable-output-escaping="no" />
            </host>
            <user-name>
              <xsl:value-of select="'${username}'" disable-output-escaping="no" />
            </user-name>
            <password>
              <xsl:attribute name="encryption-type">
                <xsl:value-of select="'${e-type}'" disable-output-escaping="no" />
              </xsl:attribute>
              <xsl:value-of select="'${password}'" disable-output-escaping="no" />
            </password>
            <xsl:if test="string-length('${timeout secs}')&gt;0">
              <timeout-secs>
                <xsl:value-of select="'${timeout secs}'" disable-output-escaping="no" />
              </timeout-secs>
            </xsl:if>
          </target>
        </xsl:if>
        <parameters>
          <xsl:if test="string-length('${network}')&gt;0">
            <network>
              <xsl:value-of select="'${network}'" disable-output-escaping="no" />
            </network>
          </xsl:if>
          <xsl:if test="string-length('${mask}')&gt;0">
            <mask>
              <xsl:value-of select="'${mask}'" disable-output-escaping="no" />
            </mask>
          </xsl:if>
          <xsl:if test="string-length('${view}')&gt;0">
            <network-view>
              <xsl:value-of select="'${view}'" disable-output-escaping="no" />
            </network-view>
          </xsl:if>
          <xsl:if test="string-length('${status}')&gt;0">
            <status>
              <xsl:value-of select="'${status}'" disable-output-escaping="no" />
            </status>
          </xsl:if>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
            <call-process>
              <process-name>:AO-AD-Infoblox-NIOS:Find Items</process-name>
              <parameters>
                <input>
                  <from>adapter</from>
                  <to>adapter name</to>
                </input>
                <input>
                  <value>retrieve-addresses</value>
                  <to>item type</to>
                </input>
                <input>
                  <from>items</from>
                  <to>items</to>
                </input>
                <input>
                  <from>version</from>
                  <to>version</to>
                </input>
                <output>
                  <from>adapter response</from>
                  <to>adapter response</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:call-process[1]</location>
            </call-process>
            <switch>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>true</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <execute-all-applicable-cases>false</execute-all-applicable-cases>
              <default>
                <sequence>
                  <assign>
                    <assignment>
                      <output>
                        <to>success status</to>
                        <value>failure</value>
                      </output>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>adapter response</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>error</to>
                        </output>
                      </result>
                      <xslt-stylesheet>
                        <stylesheet-replacement-tokens />
                        <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <xsl:if test="not(contains(//line[@ index =&quot;1&quot;], &quot;Response Code:0&quot;))">
      <error-message>
        <xsl:value-of select="//line[@ index =&quot;2&quot;]" disable-output-escaping="no" />
      </error-message>
    </xsl:if>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                      </xslt-stylesheet>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>error</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>status message</to>
                        </output>
                      </result>
                      <xpath-expression>
                        <xpath-replacement-tokens />
                        <xpath><![CDATA[//error-message/text()]]></xpath>
                      </xpath-expression>
                    </assignment>
                    <meta>
                      <documentation />
                      <checkpoint>false</checkpoint>
                    </meta>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[1]:assign[1]</location>
                  </assign>
                </sequence>
              </default>
              <case>
                <condition>
                  <document>
                    <input>
                      <from>adapter response</from>
                    </input>
                  </document>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//responses/retrieve-addresses-response/metadata/status !='error'  and //line/text()='Response Code:0']]></xpath>
                  </xpath-expression>
                </condition>
                <sequence>
                  <assign>
                    <assignment>
                      <output>
                        <to>success status</to>
                        <value>success</value>
                      </output>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>adapter response</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>extracted output</to>
                        </output>
                      </result>
                      <xslt-stylesheet>
                        <stylesheet-replacement-tokens />
                        <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <output>
      <xsl:if test="string-length(//unused-addresses)&gt;0">
        <unused-addresses>
          <xsl:copy-of select="//unused-addresses/*" />
        </unused-addresses>
      </xsl:if>
      <xsl:if test="string-length(//used-addresses)&gt;0">
        <used-addresses>
          <xsl:copy-of select="//used-addresses/*" />
        </used-addresses>
      </xsl:if>
      <xsl:if test="string-length(//ip-addresses)&gt;0">
        <all-addresses>
          <xsl:copy-of select="//ip-addresses/*" />
        </all-addresses>
      </xsl:if>
    </output>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                      </xslt-stylesheet>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>extracted output</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>output</to>
                        </output>
                      </result>
                      <xslt-stylesheet>
                        <stylesheet-replacement-tokens>
                          <replacement-token name="network type">
                            <input global="false">
                              <from>network</from>
                            </input>
                          </replacement-token>
                        </stylesheet-replacement-tokens>
                        <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="ip-address">
    <xsl:copy-of select="." />
  </xsl:template>
  <xsl:template match="unused-addresses">
    <output>
      <unused-addresses>
        <xsl:apply-templates select="ip-address" />
      </unused-addresses>
    </output>
  </xsl:template>
  <xsl:template match="used-addresses">
    <output>
      <xsl:if test="not(contains('${network type}', '::')) and not(contains('${network type}', ':') )">
        <used-addresses>
          <xsl:apply-templates select="ip-address[ position() &gt;1][position()&lt;last()]" />
        </used-addresses>
      </xsl:if>
      <xsl:if test="contains('${network type}', '::') or contains('${network type}', ':') ">
        <used-addresses>
          <xsl:apply-templates select="ip-address" />
        </used-addresses>
      </xsl:if>
    </output>
  </xsl:template>
  <xsl:template match="all-addresses">
    <output>
      <xsl:if test="not(contains('${network type}', '::')) and not(contains('${network type}', ':') )">
        <all-addresses>
          <xsl:apply-templates select="ip-address[ position() &gt;1][position()&lt;last()]" />
        </all-addresses>
      </xsl:if>
      <xsl:if test="contains('${network type}', '::') or contains('${network type}', ':')">
        <all-addresses>
          <xsl:apply-templates select="ip-address" />
        </all-addresses>
      </xsl:if>
    </output>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                      </xslt-stylesheet>
                    </assignment>
                    <meta>
                      <documentation />
                      <checkpoint>false</checkpoint>
                    </meta>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[2]:assign[1]</location>
                  </assign>
                </sequence>
              </case>
              <location>:switch[1]:sequence[2]:switch[1]</location>
            </switch>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflows retrives ip addresses by the given status under Infoblox .</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
