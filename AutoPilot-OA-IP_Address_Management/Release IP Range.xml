<process draft="false" dev-studio-version="7.6.02.05">
  <process-def name=":AutoPilot-OA-IP_Address_Management:Release IP Range">
    <parameters>
      <input global="false" required="true">
        <description>String Value : Name of the configuration name 

Valid Value :Infoblox</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>Specify the network in CIDR format.</description>
        <from>network</from>
        <to>network</to>
      </input>
      <input global="false" required="false">
        <description>Specify the start address of the IP range.</description>
        <from>start address</from>
        <to>start address</to>
      </input>
      <input global="false" required="false">
        <description>Specify the end address of the IP range.</description>
        <from>end adrress</from>
        <to>end adrress</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
	 Specify the start prefix of the IPv6 range. This parameter is only valid for IPv6 range.

This parameter is applicable for Infoblox only</description>
        <from>ipv6 start prefix</from>
        <to>ipv6 start prefix</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
	 Specify the end prefix of the IPv6 range. This parameter is only valid for IPv6 range.
This parameter is applicable for Infoblox only</description>
        <from>ipv6 end prefix</from>
        <to>ipv6 end prefix</to>
      </input>
      <input global="false" required="false">
        <description>[Optional] Specify the user comment for the deleted range.
This parameter is applicable for Infoblox only,This parameter is only valid for IPv4 range.</description>
        <from>comment</from>
        <to>comment</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
  Specify the failover association.This parameter is only valid for IPv4 range.
This parameter is applicable for Infoblox only</description>
        <from>failover association</from>
        <to>failover association</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
  Specify the IP address of the grid member.This parameter is only valid for IPv4 range.
This parameter is applicable for Infoblox only</description>
        <from>grid member address</from>
        <to>grid member address</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
	Specify the server association type.
	Valid values for IPv4 Range: NONE,MEMBER,FAILOVER,MS_SERVER
	Valid values for IPv6 Range: NONE,MEMBER
This parameter is applicable for Infoblox only</description>
        <from>server association type</from>
        <to>server association type</to>
      </input>
      <input global="false" required="false">
        <description>[Optional]
	Specifies additional custom attributes defined in Infoblox Grid Manager application.
	Sample format to define an attribute:
&lt;extensible-attributes&gt;
	&lt;attribute&gt;
		&lt;key&gt;Site&lt;/key&gt;
		&lt;value&gt;value2&lt;/value&gt;
	&lt;/attribute&gt;
&lt;/extensible-attributes&gt;
key[Optional]
	Specify the key for the attribute

value[Optional]
	Specify the value for the attribute
This parameter is applicable for Infoblox only</description>
        <from>extensible attributes</from>
        <to>extensible attributes</to>
      </input>
      <input global="false" required="false">
        <description>[Conditional]:
