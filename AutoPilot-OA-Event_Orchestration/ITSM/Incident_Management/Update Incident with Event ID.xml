<process draft="false" dev-studio-version="8.2.00">
  <process-def name=":AutoPilot-OA-Event_Orchestration:ITSM:Incident_Management:Update Incident with Event ID">
    <parameters>
      <input global="false" required="true">
        <from>itsm type</from>
        <to>itsm type</to>
      </input>
      <input global="false" required="true">
        <from>event source type</from>
        <to>event source type</to>
      </input>
      <input global="false" required="true">
        <from>all configurations</from>
        <to>all configurations</to>
      </input>
      <input global="false" required="true">
        <from>event data</from>
        <to>event data</to>
      </input>
      <input global="false" required="true">
        <from>incident number</from>
        <to>incident number</to>
      </input>
      <input global="false" required="false">
        <from>itsm authentication token</from>
        <to>itsm authentication token</to>
      </input>
      <output global="false" required="false">
        <to>status</to>
        <from>status</from>
      </output>
      <output global="false" required="false">
        <to>error message</to>
        <from>error message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>status</to>
                  <value>failure</value>
                </output>
              </assignment>
              <assignment>
                <input>
                  <concat>
                    <input>
                      <value>Unsupported ITSM type: </value>
                    </input>
                    <input required="false">
                      <from>itsm type</from>
                    </input>
                  </concat>
                </input>
                <output>
                  <to>error message</to>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>itsm type</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[. = 'BMC_AR_System']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>all configurations</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>adapter name</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens>
                    <replacement-token name="itsm type">
                      <input global="false">
                        <from>itsm type</from>
                      </input>
                    </replacement-token>
                  </stylesheet-replacement-tokens>
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <value>
      <xsl:choose>
        <xsl:when test="'${itsm type}' = 'BMC_AR_System' and //Defaults/UseRemedyJavaAPI/text() = 'false'">
          <xsl:value-of select="//Adapters/RESTAdapter/text()" disable-output-escaping="no" />
        </xsl:when>
        <xsl:otherwise>
          <xsl:value-of select="//Adapters/ITSMActorAdapter/text()" disable-output-escaping="no" />
        </xsl:otherwise>
      </xsl:choose>
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>event data</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>event id</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//metaData/eventId/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>all configurations</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>rest url</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens>
                    <replacement-token name="itsm type">
                      <input global="false">
                        <from>itsm type</from>
                      </input>
                    </replacement-token>
                  </xpath-replacement-tokens>
                  <xpath><![CDATA[//*[name() = '${itsm type}']/WebServiceConnectionData/RestURL/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
            <call-process>
              <process-name>:AutoPilot-OA-ITSM_Automation:Incident_Management:Update Incident with Event ID</process-name>
              <parameters>
                <input>
                  <from>adapter name</from>
                  <to>adapter name</to>
                </input>
                <input>
                  <from>incident number</from>
                  <to>incident number</to>
                </input>
                <input>
                  <from>event id</from>
                  <to>event id</to>
                </input>
                <input>
                  <from>rest url</from>
                  <to>url</to>
                </input>
                <input>
                  <from>itsm authentication token</from>
                  <to>itsm authentication token</to>
                </input>
                <output>
                  <from>status</from>
                  <to>status</to>
                </output>
                <output>
                  <from>error message</from>
                  <to>error message</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:call-process[1]</location>
            </call-process>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
        <consumer>OCP</consumer>
        <consumer>SOAP</consumer>
      </expose-to>
      <documentation>
        <description>Given an Incident Number and an Event ID, update the mc_ueid field (in BMC_AR_System)</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>2614cc60-3e4f-482d-b9bc-6da3fc9f78ac</process-id>
      <author>Ranganath Samudrala</author>
      <category>Event Orchestration</category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1573955026187</last-modified>
    </meta>
  </process-def>
</process>