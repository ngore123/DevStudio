<process draft="false" dev-studio-version="8.2.00">
  <process-def name=":AutoPilot-OA-Event_Orchestration:Utilities:Login">
    <parameters>
      <input global="false" required="true">
        <description>Target adapter name</description>
        <from>adapter name</from>
        <to>adapter name</to>
      </input>
      <input global="false" required="true">
        <description>ITSM's web service URL end point. For BMC_AR_System, it will be REST URL end point.</description>
        <from>url</from>
        <to>url</to>
      </input>
      <input global="false" required="true">
        <description>User ID allowed to perform ITSM operations.</description>
        <from>user name</from>
        <to>user name</to>
      </input>
      <input global="false" required="true">
        <description>Password related to ITSM user.</description>
        <from>password</from>
        <to>password</to>
      </input>
      <input global="false" required="true">
        <description>At the moment, only BMC_AR_System.</description>
        <from>itsm type</from>
        <to>itsm type</to>
      </input>
      <output global="false" required="false">
        <description>Authentication token, if login was successful.</description>
        <to>authentication token</to>
        <from>authentication token</from>
      </output>
      <output global="false" required="false">
        <description>success/failure</description>
        <to>status</to>
        <from>status</from>
      </output>
      <output global="false" required="false">
        <description>HTTP error code. If ITSM type is not supported, then, a 500 is returned.</description>
        <to>status code</to>
        <from>status code</from>
      </output>
      <output global="false" required="false">
        <description>error message in case of failure.</description>
        <to>error message</to>
        <from>error message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>authentication token</to>
            <value></value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>status</to>
            <value>success</value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>status code</to>
            <value>200</value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>error message</to>
            <value></value>
          </output>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>status</to>
                  <value>failure</value>
                </output>
              </assignment>
              <assignment>
                <input>
                  <concat>
                    <input>
                      <value>Unsupported ITSM type: '</value>
                    </input>
                    <input required="false">
                      <from>itsm type</from>
                    </input>
                    <input>
                      <value>'</value>
                    </input>
                  </concat>
                </input>
                <output>
                  <to>error message</to>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>status code</to>
                  <value>500</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>itsm type</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[. = 'BMC_AR_System']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <call-process>
              <process-name>:BMC-AD-Remedy_REST:Utilities:Login</process-name>
              <parameters>
                <input>
                  <from>adapter name</from>
                  <to>adapter name</to>
                </input>
                <input>
                  <from>url</from>
                  <to>url</to>
                </input>
                <input>
                  <from>user name</from>
                  <to>user name</to>
                </input>
                <input>
                  <from>password</from>
                  <to>password</to>
                </input>
                <output>
                  <from>authorization token</from>
                  <to>authentication token</to>
                </output>
                <output>
                  <from>status</from>
                  <to>status</to>
                </output>
                <output>
                  <from>status code</from>
                  <to>status code</to>
                </output>
                <output>
                  <from>error message</from>
                  <to>error message</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:call-process[1]</location>
            </call-process>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
        <consumer>OCP</consumer>
        <consumer>SOAP</consumer>
      </expose-to>
      <documentation>
        <description>Login into target ITSM. For now only BMC_AR_System</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>44637f4b-e6e4-4fe4-8ded-c2641deec53e</process-id>
      <author>Ranganath Samudrala</author>
      <category>Event Orchestration</category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1569846289750</last-modified>
    </meta>
  </process-def>
</process>