<process draft="false" dev-studio-version="8.1.00">
  <process-def name=":AutoPilot-OA-Event_Orchestration:Utilities:Wait N Seconds">
    <parameters>
      <input global="false" required="true">
        <from>num seconds</from>
        <to>num seconds</to>
      </input>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-Common_Utilities:XML_Utilities:Create For Loop XML</process-name>
        <parameters>
          <input>
            <from>num seconds</from>
            <to>loopcount</to>
          </input>
          <input>
            <value></value>
            <to>root element name</to>
          </input>
          <input>
            <value></value>
            <to>child element name</to>
          </input>
          <output>
            <from>loop</from>
            <to>loop</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <for-each>
        <document>
          <input>
            <from>loop</from>
          </input>
        </document>
        <result>
          <output>
            <to>wait loop</to>
          </output>
        </result>
        <xpath-expression>
          <xpath-replacement-tokens />
          <xpath><![CDATA[//loop]]></xpath>
        </xpath-expression>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <sequence>
          <pause>
            <meta>
              <documentation />
              <checkpoint>false</checkpoint>
            </meta>
            <duration>1000</duration>
            <location>:pause[1]</location>
          </pause>
        </sequence>
        <location>:for-each[1]</location>
      </for-each>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
        <consumer>OCP</consumer>
        <consumer>SOAP</consumer>
      </expose-to>
      <documentation>
        <description>Wait for N seconds</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>c7b2b7eb-09d7-4ede-91e2-61d0f6527856</process-id>
      <author>Ranganath Samudrala</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>rsamudrala</last-modified-by>
      <last-modified>1528308101096</last-modified>
    </meta>
  </process-def>
</process>