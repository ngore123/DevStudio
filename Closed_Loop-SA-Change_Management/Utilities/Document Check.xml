<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":Closed_Loop-SA-Change_Management:Utilities:Document Check">
    <parameters>
      <input global="false" required="true">
        <description>A string or xml document.</description>
        <from>input</from>
        <to>input</to>
      </input>
      <output global="false" required="true">
        <description>String Value - Indicates whether or not the process completed successfully. 

Valid values: 0-success,1-failure
</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String Value â€“ A verbose message indicating the success or failure of the process</description>
        <to>status message</to>
        <from>status message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>0</value>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>status message</to>
                  <value>A correctly formed XML document was submitted.</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>input</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[name(.)!='Configuration-Items']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>1</value>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>status message</to>
                  <value>An incorrectly formed document was submitted.</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>The workflow tests for a well formed xml document.</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
