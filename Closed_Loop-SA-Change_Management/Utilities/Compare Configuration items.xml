<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":Closed_Loop-SA-Change_Management:Utilities:Compare Configuration items">
    <parameters>
      <input global="false" required="false">
        <description>An XML document

Example:

&lt;Configuration-Items&gt;
  &lt;Configuration-Item&gt;
    &lt;name&gt;Persius1&lt;/name&gt;
     &lt;/Configuration-Item&gt;
  &lt;Configuration-Item&gt;
    &lt;name&gt;Papillon1&lt;/name&gt;
     &lt;/Configuration-Item&gt;
&lt;/Configuration-Items&gt;</description>
        <from>remedy configuration items</from>
        <to>remedy configuration items</to>
      </input>
      <input global="false" required="false">
        <description>An XML document

Example:

&lt;Configuration-Items&gt;
  &lt;Configuration-Item&gt;
    &lt;name&gt;Persius1&lt;/name&gt;
     &lt;/Configuration-Item&gt;
  &lt;Configuration-Item&gt;
    &lt;name&gt;Papillon1&lt;/name&gt;
     &lt;/Configuration-Item&gt;
&lt;/Configuration-Items&gt;</description>
        <from>input configuration items</from>
        <to>input configuration items</to>
      </input>
      <output global="false" required="false">
        <description>An XML document

Example:

&lt;Configuration-Items&gt;
  &lt;Configuration-Item&gt;
    &lt;name&gt;Persius1&lt;/name&gt;
     &lt;/Configuration-Item&gt;
  &lt;Configuration-Item&gt;
    &lt;name&gt;Papillon1&lt;/name&gt;
     &lt;/Configuration-Item&gt;
&lt;/Configuration-Items&gt;</description>
        <to>configuration items</to>
        <from>configuration item collection</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>remedy configuration items</from>
            </input>
          </document>
          <result>
            <output>
              <to>configuration item collection</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens />
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <configuration-items />
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <for-each>
        <document>
          <input>
            <from>remedy configuration items</from>
          </input>
        </document>
        <result>
          <output>
            <to>remedy configuration item</to>
          </output>
        </result>
        <xpath-expression>
          <xpath-replacement-tokens />
          <xpath><![CDATA[//Configuration-Items/*]]></xpath>
        </xpath-expression>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>**************OUTER LOOP****************</value>
              </input>
            </parameters>
          </log>
        </before-execution>
        <sequence>
          <assign>
            <assignment>
              <document>
                <input>
                  <from>remedy configuration item</from>
                </input>
              </document>
              <result>
                <output>
                  <to>remedy configuration item name</to>
                </output>
              </result>
              <xpath-expression>
                <xpath-replacement-tokens />
                <xpath><![CDATA[//name/text()]]></xpath>
              </xpath-expression>
            </assignment>
            <meta>
              <documentation />
              <checkpoint>false</checkpoint>
            </meta>
            <location>:assign[2]</location>
          </assign>
          <assign>
            <assignment>
              <output>
                <to>flag status</to>
                <value>0</value>
              </output>
            </assignment>
            <meta>
              <documentation />
              <checkpoint>false</checkpoint>
            </meta>
            <location>:assign[3]</location>
          </assign>
          <for-each>
            <document>
              <input>
                <from>input configuration items</from>
              </input>
            </document>
            <result>
              <output>
                <to>input configuration item</to>
              </output>
            </result>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[//Configuration-Items/*]]></xpath>
            </xpath-expression>
            <meta>
              <documentation />
              <run-mode-handling>
                <run-mode>OCP</run-mode>
                <allow-step-into>true</allow-step-into>
              </run-mode-handling>
              <checkpoint>false</checkpoint>
            </meta>
            <before-execution>
              <log>
                <level>debug</level>
                <parameters>
                  <input>
                    <value>+++++++++++INNER LOOP+++++++++++++++++++</value>
                  </input>
                </parameters>
              </log>
            </before-execution>
            <sequence>
              <assign>
                <assignment>
                  <document>
                    <input>
                      <from>input configuration item</from>
                    </input>
                  </document>
                  <result>
                    <output>
                      <to>input configuration item name</to>
                    </output>
                  </result>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//name/text()]]></xpath>
                  </xpath-expression>
                </assignment>
                <meta>
                  <documentation />
                  <checkpoint>false</checkpoint>
                </meta>
                <location>:assign[4]</location>
              </assign>
              <switch>
                <meta>
                  <documentation />
                  <run-mode-handling>
                    <run-mode>OCP</run-mode>
                    <allow-step-into>true</allow-step-into>
                  </run-mode-handling>
                  <checkpoint>false</checkpoint>
                </meta>
                <execute-all-applicable-cases>false</execute-all-applicable-cases>
                <default>
                  <sequence />
                </default>
                <case>
                  <condition>
                    <document>
                      <input>
                        <from>input configuration item</from>
                      </input>
                    </document>
                    <xpath-expression>
                      <xpath-replacement-tokens>
                        <replacement-token name="input configuration item">
                          <input global="false">
                            <from>input configuration item name</from>
                          </input>
                        </replacement-token>
                        <replacement-token name="remedy configuration item">
                          <input global="false">
                            <from>remedy configuration item name</from>
                          </input>
                        </replacement-token>
                      </xpath-replacement-tokens>
                      <xpath><![CDATA[string('${input configuration item}') = string('${remedy configuration item}')]]></xpath>
                    </xpath-expression>
                  </condition>
                  <sequence>
                    <assign>
                      <assignment>
                        <output>
                          <to>flag status</to>
                          <value>1</value>
                        </output>
                      </assignment>
                      <meta>
                        <documentation />
                        <checkpoint>false</checkpoint>
                      </meta>
                      <after-execution>
                        <log>
                          <level>debug</level>
                          <parameters>
                            <input>
                              <value>!!!!!!!!!!! Flag 0 !!!!!!!!!!!!!!!!!!</value>
                            </input>
                            <input>
                              <from>input configuration item name</from>
                            </input>
                            <input>
                              <from>remedy configuration item name</from>
                            </input>
                            <input>
                              <from>flag status</from>
                            </input>
                          </parameters>
                        </log>
                      </after-execution>
                      <location>:switch[1]:sequence[2]:assign[1]</location>
                    </assign>
                  </sequence>
                </case>
                <location>:switch[1]</location>
              </switch>
            </sequence>
            <location>:for-each[2]</location>
          </for-each>
          <switch>
            <meta>
              <documentation />
              <run-mode-handling>
                <run-mode>OCP</run-mode>
                <allow-step-into>true</allow-step-into>
              </run-mode-handling>
              <checkpoint>false</checkpoint>
            </meta>
            <execute-all-applicable-cases>false</execute-all-applicable-cases>
            <default>
              <sequence />
            </default>
            <case>
              <condition>
                <document>
                  <input>
                    <from>flag status</from>
                  </input>
                </document>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[text() = '0']]></xpath>
                </xpath-expression>
              </condition>
              <sequence>
                <assign>
                  <assignment>
                    <document>
                      <input>
                        <from>configuration item collection</from>
                      </input>
                    </document>
                    <result>
                      <output>
                        <to>configuration item collection</to>
                      </output>
                    </result>
                    <xslt-stylesheet>
                      <stylesheet-replacement-tokens>
                        <replacement-token name="name">
                          <input global="false">
                            <from>remedy configuration item name</from>
                          </input>
                        </replacement-token>
                      </stylesheet-replacement-tokens>
                      <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" cdata-section-elements="location" />
  <xsl:template match="/">
    <Configuration-Items>
      <xsl:copy-of select="./node()/*" />
      <Configuration-Item>
        <name>${name}</name>
      </Configuration-Item>
    </Configuration-Items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                    </xslt-stylesheet>
                  </assignment>
                  <meta>
                    <documentation />
                    <checkpoint>false</checkpoint>
                  </meta>
                  <before-execution>
                    <log>
                      <level>debug</level>
                      <parameters>
                        <input>
                          <value>%%%%% Input to collection %%%%%%%%</value>
                        </input>
                        <input>
                          <from>input configuration item name</from>
                        </input>
                      </parameters>
                    </log>
                  </before-execution>
                  <after-execution>
                    <log>
                      <level>debug</level>
                      <parameters>
                        <input>
                          <value>%%%%%%% Collection %%%%%%%%%</value>
                        </input>
                        <input>
                          <from>configuration item collection</from>
                        </input>
                      </parameters>
                    </log>
                  </after-execution>
                  <location>:switch[2]:sequence[2]:assign[1]</location>
                </assign>
              </sequence>
            </case>
            <location>:switch[2]</location>
          </switch>
        </sequence>
        <location>:for-each[1]</location>
      </for-each>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow compares two xml document lists.</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>19261ba4-92ea-3c70-a338-ae1be655f162</process-id>
      <author></author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by></last-modified-by>
      <last-modified>1283245740496</last-modified>
    </meta>
  </process-def>
</process>