<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":Closed_Loop-SA-Change_Management:Utilities:Calculate Change Dates">
    <parameters>
      <input global="false" required="true">
        <description>String Value - epoch time

Example: 1226432349</description>
        <from>scheduled start date</from>
        <to>scheduled start date</to>
      </input>
      <input global="false" required="true">
        <description>String Value - day

Example:

month
week
day
hour</description>
        <from>time factor</from>
        <to>time factor</to>
      </input>
      <output global="false" required="false">
        <description>String Value - epoch time

Example: 1226432349</description>
        <to>scheduled start date</to>
        <from>scheduled start date</from>
      </output>
      <output global="false" required="false">
        <description>String Value - epoch time

Example: 1226432349</description>
        <to>scheduled end date</to>
        <from>scheduled end date</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>time factor</from>
            </input>
          </document>
          <result>
            <output>
              <to>time value</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="time factor">
                <input global="false">
                  <from>time factor</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <value>
      <xsl:if test="'${time factor}' = &quot;month&quot;">
        <xsl:text disable-output-escaping="no">2629743</xsl:text>
      </xsl:if>
      <xsl:if test="'${time factor}' = &quot;week&quot;">
        <xsl:text disable-output-escaping="no">604800</xsl:text>
      </xsl:if>
      <xsl:if test="'${time factor}' = &quot;day&quot;">
        <xsl:text disable-output-escaping="no">86400</xsl:text>
      </xsl:if>
      <xsl:if test="'${time factor}' = &quot;hour&quot;">
        <xsl:text disable-output-escaping="no">3600</xsl:text>
      </xsl:if>
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <utility>
        <number>
          <add>
            <input>
              <from>scheduled start date</from>
            </input>
            <input>
              <from>time value</from>
            </input>
          </add>
        </number>
        <output global="false">
          <to>scheduled end date</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>Show dates</value>
              </input>
              <input>
                <from>scheduled start date</from>
              </input>
              <input>
                <from>scheduled end date</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:utility[1]</location>
      </utility>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow will create a scheduled end date from a given scheduled start date, when an input variable of hour, day, week or month has been supplied.</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>c8e27045-a5d7-3376-8978-4dc0f4506a2f</process-id>
      <author></author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by></last-modified-by>
      <last-modified>1283245739034</last-modified>
    </meta>
  </process-def>
</process>