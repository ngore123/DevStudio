<process draft="false" dev-studio-version="7.6.02.03">
  <process-def name=":AutoPilot-OA-Work_Order_Management:Searches:Get Work Orders by Requested By">
    <parameters>
      <input global="false" required="true">
        <description>String Value - The name of a configuration item defined in a configuration module. 

Example: BMC_AR_System</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String Value - First Name 

Example: Adapter</description>
        <from>first name</from>
        <to>first name</to>
      </input>
      <input global="false" required="true">
        <description>String Value - Last name 

Example: User
</description>
        <from>last name</from>
        <to>last name</to>
      </input>
      <output global="false" required="false">
        <description>An XML document that contains detail for one or more work orders.</description>
        <to>work orders</to>
        <from>work orders</from>
      </output>
      <output global="false" required="true">
        <description>String Value - Indicates whether or not the process completed successfully. 

Valid values: success, failure</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String Value - A process specific explanation for the value of success status.</description>
        <to>status message</to>
        <from>status message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>first name</from>
            </input>
          </document>
          <result>
            <output>
              <to>formed query</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="first name">
                <input global="false">
                  <from>first name</from>
                </input>
              </replacement-token>
              <replacement-token name="last name">
                <input global="false">
                  <from>last name</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" cdata-section-elements="value " />
  <xsl:template match="/">
    <value>
      <xsl:text disable-output-escaping="no">'1000000019' = "$[first name]" and '1000000018' = "$[last name]"</xsl:text>
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <call-process>
        <process-name>:AutoPilot-OA-Work_Order_Management:Searches:Get Work Orders</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <input>
            <value>1000000019</value>
            <to>key 1</to>
          </input>
          <input>
            <from>first name</from>
            <to>value 1</to>
          </input>
          <input>
            <value>1000000018</value>
            <to>key 2</to>
          </input>
          <input>
            <from>last name</from>
            <to>value 2</to>
          </input>
          <input>
            <from>formed query</from>
            <to>query</to>
          </input>
          <output>
            <from>work orders</from>
            <to>work orders</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>status message</from>
            <to>status message</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <assign>
        <assignment>
          <document>
            <input>
              <from>success status</from>
            </input>
          </document>
          <result>
            <output>
              <to>status message</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="first">
                <input global="false">
                  <from>first name</from>
                </input>
              </replacement-token>
              <replacement-token name="last">
                <input global="false">
                  <from>last name</from>
                </input>
              </replacement-token>
              <replacement-token name="status message">
                <input global="false">
                  <from>status message</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <value>
      <xsl:choose>
        <xsl:when test="string(.) = &quot;success&quot;">
          <xsl:text disable-output-escaping="no">Query result successfuly fetched for ${first} ${last}</xsl:text>
        </xsl:when>
        <xsl:when test="string(.) = &quot;failure&quot;">
          <xsl:text disable-output-escaping="no">Query failed for ${first} ${last}</xsl:text>
        </xsl:when>
        <xsl:otherwise>${status message}</xsl:otherwise>
      </xsl:choose>
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[2]</location>
      </assign>
    </sequence>
    <meta>
