        <xsl:when test="string-length(//workorder) &gt; 0">
          <xsl:copy-of select="//workorder" />
        </xsl:when>
        <xsl:otherwise>${workorder}</xsl:otherwise>
      </xsl:choose>
    </WorkOrders>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
