<process draft="false" dev-studio-version="7.6.03">
  <process-def name=":AutoPilot-OA-Change_Management:Searches:Get Changes in Date Range">
    <parameters>
      <input global="false" required="true">
        <description>String Value - The name of a configuration entry defined in a configuration module. 

Example: BMC_AR_System</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String Value - The name of the date field to search (low end of inclusive range)

Valid Values: 
planned-start-date 
planned-finish-date 
actual-start-date 
actual-finish-date</description>
        <from>query field name 1</from>
        <to>query field name 1</to>
      </input>
      <input global="false" required="true">
        <description>String Value - Inclusive start date to search (epoch) 

Example: 1262322060</description>
        <from>field name date 1</from>
        <to>field name date 1</to>
      </input>
      <input global="false" required="true">
        <description>String Value - The name of the date field to search (high end of inclusive range) 

Valid Values: 
planned-start-date 
planned-finish-date 
actual-start-date 
actual-finish-date</description>
        <from>query field name 2</from>
        <to>query field name 2</to>
      </input>
      <input global="false" required="true">
        <description>String Value - Inclusive end date to search (epoch) 

Example: 1262322060</description>
        <from>field name date 2</from>
        <to>field name date 2</to>
      </input>
      <output global="false" required="false">
        <description>An XML document that contains detail for one or more incidents </description>
        <to>changes</to>
        <from>changes</from>
      </output>
      <output global="false" required="true">
        <description>String Value - Indicates whether or not the process completed successfully. 

Valid values: success, failure
</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String Value - A process specific explanation for the value of success status.</description>
        <to>status message</to>
        <from>status message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-Change_Management:Utilities:Build Date Query</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <input>
            <from>query field name 1</from>
            <to>query field name 1</to>
          </input>
          <input>
            <from>field name date 1</from>
            <to>field name date 1</to>
          </input>
          <input>
            <from>query field name 2</from>
            <to>query field name 2</to>
          </input>
          <input>
            <from>field name date 2</from>
            <to>field name date 2</to>
          </input>
          <output>
            <from>changes</from>
            <to>changes</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>status message</from>
            <to>status message</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow finds change records for a specified inclusive date range.

This is not supported by HP ServiceCenter,BMC  Remedyforce</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>7c4969c5-7d57-3bae-8a8a-0a374b028734</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1421315344390</last-modified>
    </meta>
  </process-def>
</process>