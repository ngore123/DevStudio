        <from>status message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-Change_Management:Setup_Validation:ServiceNow:Get Service Now Configuration Name</process-name>
        <unique-id>7e75ca13-a384-432e-82c6-a434a0bc3732</unique-id>
