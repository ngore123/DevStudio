        <xsl:copy-of select="./node()/*[local-name() != 'priority']" />
        <priority>
          <xsl:value-of select="'${name}'" disable-output-escaping="no" />
        </priority>
      </xsl:if>
      <xsl:if test="string-length('${name}')=0">
