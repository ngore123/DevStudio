<process draft="false" dev-studio-version="7.9.01.002">
  <process-def name=":ESA_ITSM:Check ITSM Adapter">
    <parameters>
      <input global="false" required="false">
        <description>The output/result of the ITSM Adapter ran previously</description>
        <from>Adapter_Result</from>
        <to>Adapter_Result</to>
      </input>
      <output global="false" required="false">
        <description>Return Code meanings:
0 = Success
1 = Error</description>
        <to>ReturnCode</to>
        <from>ReturnCode</from>
      </output>
      <output global="false" required="false">
        <description>ITSM Error Message</description>
        <to>ITSM_ErrorMessage</to>
        <from>ITSM_ErrorMessage</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>Adapter_Result</from>
            </input>
          </document>
          <result>
            <output>
              <to>Adapter_Error</to>
            </output>
          </result>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[string(//entry/error)]]></xpath>
          </xpath-expression>
        </assignment>
        <assignment>
          <document>
            <input>
              <from>Adapter_Error</from>
            </input>
          </document>
          <result>
            <output>
              <to>Adapter_Error_lg</to>
            </output>
          </result>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[string-length(.)]]></xpath>
          </xpath-expression>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>ReturnCode</to>
                  <value>0</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>Adapter_Error_lg</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[number(.) > 0]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>ReturnCode</to>
                  <value>1</value>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>Adapter_Error</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>ITSM_ErrorMessage</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[substring-after(., "[AppendedText=")]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>ITSM_ErrorMessage</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>ITSM_ErrorMessage</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[substring-before(., "]")]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>ITSM_ErrorMessage</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>ITSM_ErrorMessage_lg</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[string-length(.)]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
            <switch>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>true</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <execute-all-applicable-cases>false</execute-all-applicable-cases>
              <default>
                <sequence>
                  <assign>
                    <assignment>
                      <input>
                        <from>ITSM_ErrorMessage</from>
                      </input>
                      <output>
                        <to>ITSM_ErrorMessage</to>
                      </output>
                    </assignment>
                    <meta>
                      <documentation />
                      <checkpoint>false</checkpoint>
                    </meta>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[1]:assign[1]</location>
                  </assign>
                </sequence>
              </default>
              <case>
                <condition>
                  <document>
                    <input>
                      <from>ITSM_ErrorMessage_lg</from>
                    </input>
                  </document>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[number(.) = 0]]></xpath>
                  </xpath-expression>
                </condition>
                <sequence>
                  <assign>
                    <assignment>
                      <input>
                        <from>Adapter_Error</from>
                      </input>
                      <output>
                        <to>ITSM_ErrorMessage</to>
                      </output>
                    </assignment>
                    <meta>
                      <documentation />
                      <checkpoint>false</checkpoint>
                    </meta>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[2]:assign[1]</location>
                  </assign>
                </sequence>
              </case>
              <location>:switch[1]:sequence[2]:switch[1]</location>
            </switch>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>eccef76a-0c98-489d-9319-475b485ab4f4</process-id>
      <author>Yannic Legermann</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1580893310933</last-modified>
    </meta>
  </process-def>
</process>