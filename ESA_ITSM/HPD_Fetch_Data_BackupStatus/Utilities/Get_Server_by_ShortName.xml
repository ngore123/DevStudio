  <process-def name=":ESA_ITSM:HPD_Fetch_Data_BackupStatus:Utilities:Get_Server_by_ShortName">
    <parameters>
      <input global="false" required="false">
        <description>in lowercase</description>
        <from>ServerNameShort</from>
        <to>ServerNameShort</to>
      </input>
      <output global="false" required="false">
        <to>ServerName</to>
        <from>ServerName</from>
      </output>
      <output global="false" required="false">
        <to>count_Server</to>
        <from>count_Server</from>
      </output>
      <output global="false" required="false">
        <to>exists</to>
        <from>exists</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>ServerNameShort</from>
            </input>
          </document>
          <result>
            <output>
              <to>ServerNameShort</to>
            </output>
          </result>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[lower-case(.)]]></xpath>
          </xpath-expression>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
