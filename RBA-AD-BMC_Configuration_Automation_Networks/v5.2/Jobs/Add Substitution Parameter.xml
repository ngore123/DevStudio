<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":RBA-AD-BMC_Configuration_Automation_Networks:v5.2:Jobs:Add Substitution Parameter">
    <parameters>
      <input global="false" required="false">
        <description>adapter name    [Optional]
            The name of the adapter to use to handle the request. 

Default value: BCANActor</description>
        <from>adapter name</from>
        <to>adapter name</to>
      </input>
      <input global="false" required="true">
        <description>Sample items xml for operation Add Substitution Parameter:

&lt;items&gt;
	&lt;item&gt;
		&lt;parameters&gt;
			&lt;substitution-parameter&gt;
				&lt;key&gt;a&lt;/key&gt;
				&lt;name&gt;a&lt;/name&gt;
				&lt;value&gt;a&lt;/value&gt;
				&lt;dynamic-fields&gt;
				    &lt;element&gt;
						&lt;name&gt;a&lt;/name&gt;
						&lt;values&gt;
							&lt;element&gt;foo&lt;/element&gt;
							&lt;element&gt;bar&lt;/element&gt;
						&lt;/values&gt;
				    &lt;/element&gt;
				    &lt;element&gt;
						&lt;name&gt;b&lt;/name&gt;
						&lt;values&gt;
				            &lt;element&gt;foo&lt;/element&gt;
							&lt;element&gt;bar&lt;/element&gt;
						&lt;/values&gt;
				    &lt;/element&gt;
				&lt;/dynamic-fields&gt;
				&lt;sensitive&gt;true&lt;/sensitive&gt;
			&lt;/substitution-parameter&gt;
		&lt;/parameters&gt;
	&lt;/item&gt;
&lt;/items&gt;</description>
        <from>items</from>
        <to>items</to>
      </input>
      <input global="false" required="false">
        <description>connection parameters [optional]
        The XML containing the metadata needed by the adapter to connect to the target.

Value takes the form:

&lt;connection-parameters&gt;
	&lt;soap-url&gt;&lt;/soap-url&gt;
 	&lt;user-name&gt;&lt;/user-name&gt;
	&lt;password&gt;&lt;/password&gt;
	&lt;soap-version&gt;&lt;/soap-version&gt;
&lt;/connection-parameters&gt;</description>
        <from>connection parameters</from>
        <to>connection parameters</to>
      </input>
      <output global="false" required="false">
        <to>adapter response</to>
        <from>adapter response</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:RBA-AD-BMC_Configuration_Automation_Networks:Utilities:Create Adapter Request</process-name>
        <parameters>
          <input>
            <from>items</from>
            <to>items</to>
          </input>
          <input>
            <value>substitution-parameter</value>
            <to>entity</to>
          </input>
          <input>
            <value>add-substitution-parameter</value>
            <to>operation-name</to>
          </input>
          <input>
            <from>connection parameters</from>
            <to>connection parameters</to>
          </input>
          <output>
            <from>request</from>
            <to>adapter request</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <call-process>
        <process-name>:RBA-AD-BMC_Configuration_Automation_Networks:Utilities:Execute Adapter Request</process-name>
        <parameters>
          <input>
            <from>adapter name</from>
            <to>adapter name</to>
          </input>
          <input>
            <from>adapter request</from>
            <to>adapter request</to>
          </input>
          <output>
            <from>adapter response</from>
            <to>adapter response</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[2]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Substitution parameters may be used in configuration Templates, Rules and other areas to support re-usability.
The Add Substitution Parameter operation adds a global substitution parameter.</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>5c9f60e2-2a25-3581-ba30-94d8da27542b</process-id>
      <author>admin</author>
