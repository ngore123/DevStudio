              &lt;action-sequence-number&gt;1&lt;/action-sequence-number&gt;
            &lt;/element&gt;
          &lt;/action-runtime-param-values&gt;
          &lt;run-at-time&gt;Now or when approved&lt;/run-at-time&gt;
          &lt;runtime-span-values&gt;
            &lt;element&gt;
              &lt;realm-name&gt;default&lt;/realm-name&gt;
              &lt;span-name&gt;DeviceType.Cisco IOS Switch/Router&lt;/span-name&gt;
              &lt;span-prompt&gt;A&lt;/span-prompt&gt;
              &lt;span-type&gt;Group&lt;/span-type&gt;
            &lt;/element&gt;
          &lt;/runtime-span-values&gt;
          &lt;shared-runtime-parameter-values&gt;
            &lt;element&gt;
              &lt;name&gt;res_LGPD_Pool_Name&lt;/name&gt;
              &lt;value&gt;hello&lt;/value&gt;
            &lt;/element&gt;
            &lt;element&gt;
              &lt;name&gt;res_LGPD_Server_HostName&lt;/name&gt;
              &lt;value&gt;hello2&lt;/value&gt;
            &lt;/element&gt;
            &lt;element&gt;
              &lt;name&gt;res_LGPD_Server_Address&lt;/name&gt;
              &lt;value&gt;hello3&lt;/value&gt;
            &lt;/element&gt;
            &lt;element&gt;
              &lt;name&gt;res_LGPD_Server_Port&lt;/name&gt;
              &lt;value&gt;hello4&lt;/value&gt;
            &lt;/element&gt;
          &lt;/shared-runtime-parameter-values&gt;
        &lt;/job-params&gt;
        &lt;wait-for-completion&gt;true&lt;/wait-for-completion&gt;
      &lt;/parameters&gt;
	&lt;/item&gt;
&lt;/items&gt; 
 

