<process draft="false" dev-studio-version="7.6.02.05">
  <process-def name=":AO-AD-VMware_VCloud:Utilities:Template_Management_Operations:Construct Item for Import Ovf As vApp Template">
    <parameters>
      <input global="false" required="true">
        <description>Id of the Catalog where vApp Template will be added</description>
        <from>catalog id</from>
        <to>catalog id</to>
      </input>
      <input global="false" required="true">
        <description>vdc id of the vApp Container</description>
        <from>vdc id</from>
        <to>vdc id</to>
      </input>
      <input global="false" required="true">
        <description>name of the created vApp Template
Default value : New Template</description>
        <from>vapp template name</from>
        <to>vapp template name</to>
      </input>
      <input global="false" required="false">
        <description>Description of the created vApp Template</description>
        <from>description</from>
        <to>description</to>
      </input>
      <input global="false" required="true">
        <description>Path to the OVF Descriptor File</description>
        <from>ovf descriptor file</from>
        <to>ovf descriptor file</to>
      </input>
      <output global="false" required="false">
        <to>items</to>
        <from>items</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="vdc  id">
                <input global="false">
                  <from>vdc id</from>
                </input>
              </replacement-token>
              <replacement-token name="ovf descriptor file">
                <input global="false">
                  <from>ovf descriptor file</from>
                </input>
              </replacement-token>
              <replacement-token name="vapp template name">
                <input global="false">
                  <from>vapp template name</from>
                </input>
              </replacement-token>
              <replacement-token name="description">
                <input global="false">
                  <from>description</from>
                </input>
              </replacement-token>
              <replacement-token name="catalog id">
                <input global="false">
                  <from>catalog id</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <items>
      <item>
        <parameters>
          <catalog-id>${catalog id}</catalog-id>
          <vdc-id>${vdc  id}</vdc-id>
          <import-ovf-as-vapp-template-params>
            <ovf-descriptor-file>${ovf descriptor file}</ovf-descriptor-file>
            <xsl:if test="'${vapp template name}' != ''">
              <vapp-template-name>${vapp template name}</vapp-template-name>
            </xsl:if>
            <xsl:if test="'${description}' != ''">
              <description>${description}</description>
            </xsl:if>
          </import-ovf-as-vapp-template-params>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Construct Item for Import Ovf As vApp Template</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>fa2c964c-b19c-4b6c-a096-21b360fda0de</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1354783403980</last-modified>
    </meta>
  </process-def>
</process>