            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="alternate username">
                <input global="false">
                  <from>alternate username</from>
                </input>
              </replacement-token>
              <replacement-token name="alternate password">
                <input global="false">
                  <from>alternate password</from>
                </input>
              </replacement-token>
              <replacement-token name="alternate vcloud organization">
                <input global="false">
                  <from>alternate vcloud organization</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <connection-parameters>
      <vcloud-organization>${alternate vcloud organization}</vcloud-organization>
      <user-name>${alternate username}</user-name>
      <password>${alternate password}</password>
    </connection-parameters>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
