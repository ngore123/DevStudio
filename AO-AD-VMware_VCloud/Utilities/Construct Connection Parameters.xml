<process draft="false" dev-studio-version="7.6.01.04">
  <process-def name=":AO-AD-VMware_VCloud:Utilities:Construct Connection Parameters">
    <parameters>
      <input global="false" required="false">
        <description>alternate username    [Optional]
            The username use to handle the request.</description>
        <from>alternate username</from>
        <to>alternate username</to>
      </input>
      <input global="false" required="false">
        <description>alternate password    [Optional]
            The password use to handle the request</description>
        <from>alternate password</from>
        <to>alternate password</to>
      </input>
      <input global="false" required="false">
        <description>alternate vcloud organization  [Optional]
            The vcloud organization used to execute the request</description>
        <from>alternate vcloud organization</from>
        <to>alternate vcloud organization</to>
      </input>
      <output global="false" required="false">
        <to>connection parameters</to>
        <from>connection parameters</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
