      <item>
        <parameters>
          <vdc-id>${vdc id}</vdc-id>
          <vapp-id>${vapp id}</vapp-id>
          <vapp-template-name>${vapp template name}</vapp-template-name>
          <vapp-template-description>${vapp template description}</vapp-template-description>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
