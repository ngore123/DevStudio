            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <items>
      <item>
        <parameters>
          <network-id>${network id}</network-id>
          <modify-organization-network-params>
            <network-name>${network name}</network-name>
            <description>${description}</description>
            <xsl:if test="'${is inherited}' != ''  or  '${parent network id}' != ''  or  '${fence mode}' != ''">
              <configuration>
                <xsl:if test="'${is inherited}' != ''">
                  <ip-scope>
                    <is-inherited>${is inherited}</is-inherited>
                    <xsl:if test="'${gateway}' != ''">
                      <gateway>${gateway}</gateway>
                    </xsl:if>
                    <xsl:if test="'${netmask}' != ''">
                      <netmask>${netmask}</netmask>
                    </xsl:if>
                    <xsl:if test="'${dns1}' != ''">
                      <dns1>${dns1}</dns1>
                    </xsl:if>
                    <xsl:if test="'${dns2}' != ''">
                      <dns2>${dns2}</dns2>
                    </xsl:if>
                    <xsl:if test="'${dns suffix}' != ''">
                      <dns-suffix>${dns suffix}</dns-suffix>
                    </xsl:if>
                    ${ip ranges}
                  </ip-scope>
                </xsl:if>
                <xsl:if test="'${parent network id}' != ''">
                  <parent-network-id>${parent network id}</parent-network-id>
                </xsl:if>
                <xsl:if test="'${fence mode}' != ''">
                  <fence-mode>${fence mode}</fence-mode>
                </xsl:if>
              </configuration>
            </xsl:if>
            <xsl:if test="'${network pool id}' != ''">
              <network-pool-id>${network pool id}</network-pool-id>
            </xsl:if>
          </modify-organization-network-params>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
