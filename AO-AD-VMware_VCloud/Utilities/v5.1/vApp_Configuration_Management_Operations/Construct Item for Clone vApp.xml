<process draft="false" dev-studio-version="7.6.02.05">
  <process-def name=":AO-AD-VMware_VCloud:Utilities:v5.1:vApp_Configuration_Management_Operations:Construct Item for Clone vApp">
    <parameters>
      <input global="false" required="true">
        <description>vapp id = Id of the vApp to be cloned</description>
        <from>vapp id</from>
        <to>vapp id</to>
      </input>
      <input global="false" required="true">
        <description>name of the cloned vApp</description>
        <from>name</from>
        <to>name</to>
      </input>
      <input global="false" required="true">
        <description>vdc id of the vApp Container</description>
        <from>vdc id</from>
        <to>vdc id</to>
      </input>
      <input global="false" required="false">
        <description>Boolean to sepcify if the cloned vApp should be deployed or not.</description>
        <from>deploy</from>
        <to>deploy</to>
      </input>
      <input global="false" required="false">
        <description>Boolean to specify if the cloned vApp should be powered on or not</description>
        <from>power on</from>
        <to>power on</to>
      </input>
      <input global="false" required="false">
        <description>Description of the cloned vApp</description>
        <from>description</from>
        <to>description</to>
      </input>
      <input global="false" required="false">
        <description>Represents the Instantiation parameters of the VMs of this vApp given in the following format:

&lt;sourced-vm-instantiation-params-list&gt;
	&lt;sourced-vm-instantiation-params&gt;
     		&lt;source&gt;sourceVM1&lt;/source&gt;
      	&lt;storage-profile&gt;teststorage1&lt;/storage-profile&gt;
	&lt;/sourced-vm-instantiation-params&gt;
     	&lt;sourced-vm-instantiation-params&gt;
     		&lt;source&gt;sourceVM2&lt;/source&gt;
        	&lt;storage-profile&gt;teststorage2&lt;/storage-profile&gt;
     &lt;/sourced-vm-instantiation-params&gt;
&lt;/sourced-vm-instantiation-params-list&gt;</description>
        <from>sourced vm instantiation params</from>
        <to>sourced vm instantiation params</to>
      </input>
      <output global="false" required="false">
        <to>items</to>
        <from>items</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="vdc id">
                <input global="false">
                  <from>vdc id</from>
                </input>
              </replacement-token>
              <replacement-token name="name">
                <input global="false">
                  <from>name</from>
                </input>
              </replacement-token>
              <replacement-token name="vapp id">
                <input global="false">
                  <from>vapp id</from>
                </input>
              </replacement-token>
              <replacement-token name="deploy">
                <input global="false">
                  <from>deploy</from>
                </input>
              </replacement-token>
              <replacement-token name="power on">
                <input global="false">
                  <from>power on</from>
                </input>
              </replacement-token>
              <replacement-token name="description">
                <input global="false">
                  <from>description</from>
                </input>
              </replacement-token>
              <replacement-token name="init params">
                <input global="false">
                  <from>sourced vm instantiation params</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <items>
      <item>
        <parameters>
          <vapp-id>${vapp id}</vapp-id>
          <vdc-id>${vdc id}</vdc-id>
          <clone-vapp-params>
            <name>${name}</name>
            <deploy>${deploy}</deploy>
            <power-on>${power on}</power-on>
            <description>${description}</description>
            ${init params}
          </clone-vapp-params>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
