                  </configuration>
                  <is-deployed>${is deployed}</is-deployed>
                </network-config>
              </network-config-section>
            </instantiation-params>
            ${vm init params}
          </instantiate-vapp-template-params>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
