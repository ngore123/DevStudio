<process draft="false" dev-studio-version="7.6.02.05">
  <process-def name=":AO-AD-VMware_VCloud:Utilities:v5.1:Template_Management_Operations:Construct Item for Clone vApp Template">
    <parameters>
      <input global="false" required="true">
        <description>vdc id of the vApp Container</description>
        <from>vdc id</from>
        <to>vdc id</to>
      </input>
      <input global="false" required="true">
        <description>vapp template id = Id of the vApp Template to be cloned</description>
        <from>vapp template id</from>
        <to>vapp template id</to>
      </input>
      <input global="false" required="false">
        <description>name of the cloned vApp Template
Default value : New Clone</description>
        <from>vapp template name</from>
        <to>vapp template name</to>
      </input>
      <input global="false" required="false">
        <description>Description of the cloned vApp Template</description>
        <from>description</from>
        <to>description</to>
      </input>
      <input global="false" required="false">
        <description>Boolean to specify if the source vApp Template will be deleted or not
Valid value: true, false
Default value : false</description>
        <from>delete source</from>
        <to>delete source</to>
      </input>
      <input global="false" required="false">
        <description>Id of the VDC storage profile to be used for storage.</description>
        <from>vdc storage profile</from>
        <to>vdc storage profile</to>
      </input>
      <output global="false" required="false">
        <to>items</to>
        <from>items</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="vdc id">
                <input global="false">
                  <from>vdc id</from>
                </input>
              </replacement-token>
              <replacement-token name="vapp template id">
                <input global="false">
                  <from>vapp template id</from>
                </input>
              </replacement-token>
              <replacement-token name="vapp template name">
                <input global="false">
                  <from>vapp template name</from>
                </input>
              </replacement-token>
              <replacement-token name="description">
                <input global="false">
                  <from>description</from>
                </input>
              </replacement-token>
              <replacement-token name="delete source">
                <input global="false">
                  <from>delete source</from>
                </input>
              </replacement-token>
              <replacement-token name="vdc storage profile">
                <input global="false">
                  <from>vdc storage profile</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <items>
      <item>
        <parameters>
          <vdc-id>${vdc id}</vdc-id>
          <clone-vapp-template-params>
            <vapp-template-id>${vapp template id}</vapp-template-id>
            <xsl:if test="'${vapp template name}' !=''">
              <vapp-template-name>${vapp template name}</vapp-template-name>
            </xsl:if>
            <xsl:if test="'${description}' != ''">
              <description>${description}</description>
            </xsl:if>
            <xsl:if test="'${delete source}' != ''">
              <is-source-delete>${delete source}</is-source-delete>
            </xsl:if>
            <xsl:if test="'${vdc storage profile}' != ''">
              <vdc-storage-profile>${vdc storage profile}</vdc-storage-profile>
            </xsl:if>
          </clone-vapp-template-params>
        </parameters>
      </item>
    </items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Construct item for clone vApp Template</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>7b4afac1-b093-4be9-85b5-b0a13b5bb61c</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1355479655313</last-modified>
    </meta>
  </process-def>
</process>