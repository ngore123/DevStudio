<process draft="false" dev-studio-version="20.02.00">
  <process-def name=":AutoPilot-OA-Incident_Management:Searches:Get Incidents by Assignee">
    <parameters>
      <input global="false" required="true">
        <description>String Value - The name of a configuration item defined in a configuration module. 

Example:BMC_ARS_System, HP_ServiceCenter, ServiceNow</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String Value - Assignee 

Remedy Example: Bob Backline

HP_ServiceCenter Example: HELPDESK 1

ServiceNow : Service Desk
</description>
        <from>assignee</from>
        <to>assignee</to>
      </input>
      <output global="false" required="false">
        <description>An XML document that contains detail for one or more incidents </description>
        <to>incidents</to>
        <from>incidents</from>
      </output>
