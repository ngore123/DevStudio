      <call-process>
        <process-name>:AutoPilot-OA-Incident_Management:Searches:Get Incidents</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <input>
            <value>assigned-to</value>
            <to>key 1</to>
          </input>
          <input>
            <from>assignee</from>
            <to>value 1</to>
          </input>
          <output>
            <from>incidents</from>
            <to>incidents</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
