<process draft="false" dev-studio-version="7.6.03">
  <process-def name=":AutoPilot-OA-Incident_Management:Searches:Get Incidents by Status">
    <parameters>
      <input global="false" required="true">
        <description>String Value - The name of a configuration item defined in a configuration module. 

Example: BMC_ARS_System, HP_ServiceCenter, ServiceNow</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String Value - Status 

BMC_AR_System

Valid Values: 

New 
Assigned 
In Progress 
Pending 
Resolved 
Closed 
Cancelled

HP_ServiceCenter

Valid Values:

Open
Pending other
Work in progress
Reject
Resolved
Pending vendor
Closed
Pending customer
Referred
Replaced Problem

</description>
        <from>status</from>
        <to>status</to>
      </input>
      <output global="false" required="false">
        <description>An XML document that contains detail for one or more incidents </description>
        <to>incidents</to>
        <from>incidents</from>
      </output>
      <output global="false" required="false">
        <description>An XML document that contains detail for failed incidents.</description>
        <to>failed incidents</to>
        <from>failed incidents</from>
      </output>
      <output global="false" required="false">
        <description>String Value - Count of incidents successfully created.</description>
        <to>success count</to>
        <from>success count</from>
      </output>
      <output global="false" required="false">
        <description>String Value - Count of failed incidents.</description>
        <to>failure count</to>
        <from>failure count</from>
      </output>
      <output global="false" required="true">
        <description>Indicates whether or not the process completed successfully.
Valid values: success, failure.
</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String Value - A process specific explanation for the value of success status.</description>
        <to>status message</to>
        <from>status message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-Incident_Management:Searches:Get Incidents</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <input>
            <value>status</value>
            <to>key 1</to>
          </input>
          <input>
            <from>status</from>
            <to>value 1</to>
          </input>
          <output>
            <from>incidents</from>
            <to>incidents</to>
          </output>
          <output>
            <from>failed incidents</from>
            <to>failed incidents</to>
          </output>
          <output>
            <from>success count</from>
            <to>success count</to>
          </output>
          <output>
            <from>failure count</from>
            <to>failure count</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>status message</from>
            <to>status message</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow finds an incident by a specified status.</description>
