<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":AutoPilot-OA-Problem_Management:Investigation:Custom:Find Items">
    <parameters>
      <input global="false" required="true">
        <description>String value - The name of a configuration item defined in a configuration module. 

Example: BMC_AR_System</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String value - The name or the id of the target field field to search.</description>
        <from>key 1</from>
        <to>key 1</to>
      </input>
      <input global="false" required="true">
        <description>String value - The value searched for in the key 1 field.</description>
        <from>value 1</from>
        <to>value 1</to>
      </input>
      <output global="false" required="true">
        <description>XML output that contains single or multiple Concept elements. A concept is an XML document containing specific elements used to assign multiple values to a single context item. Each value is associated with an element of the Concept. XML document containing elements necessary to interact with custom applications.
</description>
        <to>investigations</to>
        <from>investigations</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>items</to>
            <value>
              <items />
            </value>
          </output>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow will find a Problem Investigation Record.</description>
        <version>1.0</version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>90e39353-db2e-3e36-8a50-b3c9b5a449fa</process-id>
      <author></author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by></last-modified-by>
      <last-modified>1283164585496</last-modified>
    </meta>
  </process-def>
</process>