        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Assigned-Group-Pblm-Mgr">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <maxLength value="60" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Support-Organization-Pblm-Mgr">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <maxLength value="60" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Support-Company-Pblm-Mgr">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <maxLength value="60" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Target-Resolution-Date">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <maxLength value="30" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
