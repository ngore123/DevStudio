<process draft="false" dev-studio-version="7.6.03">
  <process-def name=":RBA-AD-Active_Directory:Utilities:Execute Command">
    <parameters>
      <input global="false" required="false">
        <description>adapter name    [Optional]
            The name of the adapter to use to handle the request. 

Default value: ActiveDirectory</description>
        <from>adapter name</from>
        <to>adapter name</to>
      </input>
      <input global="false" required="false">
        <description>This element is required if a target will be referenced or defined in the request.
In the absence of this element, the default target in the Adapter configuration will be used. 

Value takes the form:

&lt;target&gt;
		&lt;host&gt;&lt;/host&gt;
		&lt;user-name&gt;&lt;/user-name&gt;
		&lt;password&gt;&lt;/password&gt;
&lt;/target&gt;</description>
        <from>target</from>
        <to>target</to>
      </input>
      <input global="false" required="true">
        <description>The distinguished name of the user.</description>
        <from>object distinguished name</from>
        <to>object distinguished name</to>
      </input>
      <input global="false" required="false">
        <description>Contains the parameter list for the command.

Parameters for the command are:

&lt;parameters&gt;
          &lt;server&gt;&lt;/server&gt;
          &lt;domain&gt;&lt;/domain&gt;
          &lt;user-name&gt;&lt;/user-name&gt;
          &lt;password&gt;&lt;/password&gt;
&lt;/parameters&gt;</description>
        <from>parameters</from>
        <to>parameters</to>
      </input>
      <input global="false" required="true">
        <description>Active directory command</description>
        <from>command</from>
        <to>command</to>
      </input>
      <input global="false" required="true">
        <from>object class</from>
        <to>object class</to>
      </input>
      <output global="false" required="false">
        <to>adapter response</to>
        <from>adapter response</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:RBA-AD-Active_Directory:Utilities:Create Adapter Request</process-name>
        <parameters>
          <input>
            <from>target</from>
            <to>target</to>
          </input>
          <input>
            <from>command</from>
            <to>active directory command</to>
          </input>
          <input>
            <from>object class</from>
            <to>object class</to>
          </input>
          <input>
            <from>object distinguished name</from>
            <to>object distinguished name</to>
          </input>
          <input>
            <from>parameters</from>
            <to>parameters</to>
          </input>
          <output>
            <from>adapter request</from>
            <to>adapter request</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <call-process>
        <process-name>:RBA-AD-Active_Directory:Utilities:Invoke ActiveDirectory Adapter</process-name>
        <parameters>
          <input>
            <from>adapter name</from>
            <to>adapter name</to>
          </input>
          <input>
            <from>adapter request</from>
            <to>adapter request</to>
          </input>
          <output>
            <from>adapter response</from>
            <to>adapter response</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>*****************</value>
              </input>
              <input>
                <from>adapter request</from>
              </input>
              <input>
                <from>adapter response</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:call-process[2]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>17829bf7-fa76-49b6-aa2c-be363762bf69</process-id>
      <author>aoadmin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
