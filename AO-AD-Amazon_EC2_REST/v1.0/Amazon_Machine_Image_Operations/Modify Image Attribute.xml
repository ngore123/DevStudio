                    &lt;/item&gt;
                    &lt;item&gt;
                        &lt;group&gt;&lt;/group&gt;
                    &lt;/item&gt;
                &lt;/add&gt;
			&lt;remove&gt;
                    &lt;!--Zero or more repetition --&gt;
                    &lt;item&gt;
                        &lt;user-id&gt;&lt;/user-id&gt;
                    &lt;/item&gt;
                    &lt;item&gt;
                        &lt;group&gt;&lt;/group&gt;
