                    &lt;product-code&gt;&lt;/product-code&gt;
                &lt;/item&gt;
            &lt;/product-codes&gt;
            &lt;description&gt;
                &lt;value&gt;&lt;/value&gt;
            &lt;/description&gt;
        &lt;/parameters&gt;
    &lt;/item&gt;
&lt;/items&gt;

