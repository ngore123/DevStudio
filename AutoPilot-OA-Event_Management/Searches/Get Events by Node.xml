<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":AutoPilot-OA-Event_Management:Searches:Get Events by Node">
    <parameters>
      <input global="false" required="true">
        <description>String value - The name of a configuration item defined in a configuration module.

Example: Netcool_OMNIbus, HP_NNM, BMC_Event_Manager
</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String value - Enter the node name

Note: This function is not supported by HP NNM

BMC Event Manager: Value in the field 'Host Address'</description>
        <from>node</from>
        <to>node</to>
      </input>
      <output global="false" required="false">
        <description>An XML concept document containing events for the requested node</description>
        <to>events</to>
        <from>events</from>
      </output>
      <output global="false" required="true">
        <description>String value - status success / fail</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String value - verbose status message.</description>
        <to>success message</to>
        <from>success message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-Event_Management:Searches:Get Events</process-name>
        <parameters>
          <input>
            <value>node</value>
            <to>key 1</to>
          </input>
          <input>
            <from>node</from>
            <to>value 1</to>
          </input>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <output>
            <from>events</from>
            <to>events</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>success message</from>
            <to>success message</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow will find events by a node name.

Not supported on HPNNM</description>
        <version>1.0</version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>e95ef90b-3414-35ee-8973-7486559af557</process-id>
      <author></author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by></last-modified-by>
      <last-modified>1283171550614</last-modified>
    </meta>
  </process-def>
</process>