<process draft="false" dev-studio-version="7.6.03">
  <process-def name=":AutoPilot-OA-Event_Management:Searches:Get Event by Serial Number">
    <parameters>
      <input global="false" required="true">
        <description>String value - The name of a configuration item defined in a configuration module.

Example: Netcool_OMNIbus, HP_NNM, BMC_Event_Manager
</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String value - Enter the serial number of an event.

Note: This function is not supported by HP NNM

BMC Event Manager: The Serial number corresponds to the ID of the Event in BEM.</description>
        <from>serial number</from>
        <to>serial number</to>
      </input>
      <output global="false" required="false">
        <description>An XML concept document containing event detail for the serial number requested</description>
        <to>events</to>
        <from>events</from>
      </output>
      <output global="false" required="true">
        <description>String value - status success / fail</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String value - verbose status message.</description>
        <to>success message</to>
        <from>success message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <call-process>
              <process-name>:AutoPilot-OA-Event_Management:Searches:Get Events</process-name>
              <parameters>
                <input>
                  <from>configuration name</from>
                  <to>configuration name</to>
                </input>
                <input>
                  <value>Serial</value>
                  <to>key 1</to>
                </input>
                <input>
                  <from>serial number</from>
                  <to>value 1</to>
                </input>
                <output>
                  <from>events</from>
                  <to>events</to>
                </output>
                <output>
                  <from>success status</from>
                  <to>success status</to>
                </output>
                <output>
                  <from>success message</from>
                  <to>success message</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:call-process[1]</location>
            </call-process>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>configuration name</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[text() = "BMC_Event_Manager"]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <console-output>
              <type>INFO</type>
              <summary>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </summary>
              <detail>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </detail>
            </console-output>
            <call-process>
              <process-name>:AutoPilot-OA-Event_Management:Searches:Get Events</process-name>
              <parameters>
                <input>
                  <from>configuration name</from>
                  <to>configuration name</to>
                </input>
                <input>
                  <value>id</value>
                  <to>key 1</to>
                </input>
                <input>
                  <from>serial number</from>
                  <to>value 1</to>
                </input>
                <output>
                  <from>events</from>
                  <to>events</to>
                </output>
                <output>
                  <from>success status</from>
                  <to>success status</to>
                </output>
                <output>
                  <from>success message</from>
                  <to>success message</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <after-execution>
                <console-output>
                  <type>INFO</type>
                  <summary>
                    <concat>
                      <input>
                        <value></value>
                      </input>
                    </concat>
                  </summary>
                  <detail>
                    <concat>
                      <input>
                        <value></value>
                      </input>
                    </concat>
                  </detail>
                </console-output>
              </after-execution>
              <location>:switch[1]:sequence[2]:call-process[1]</location>
            </call-process>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow will find an event by serial number.

Not supported on HPNNM

BMC Event Manager: The Serial number corresponds to the ID of the Event in BEM.</description>
        <version>1.0</version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>88de5faf-7477-327f-9469-e674aa0c727f</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1457592022532</last-modified>
    </meta>
  </process-def>
</process>