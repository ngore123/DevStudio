<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":AutoPilot-OA-Event_Management:Assign Event to Assignee">
    <parameters>
      <input global="false" required="true">
        <description>String value - The name of a configuration item defined in a configuration module.

Example: Netcool_OMNIbus, HP_NNM, BMC_Event_Manager
</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String value - The serial number of the event.

Example: 10333

BMC Event Manager: The Serial number corresponds to the ID of the Event in BEM.</description>
        <from>serial number</from>
        <to>serial number</to>
      </input>
      <input global="false" required="true">
        <description>Numeric value - Enter the uid of the assignee

Example: 300


Note: This function is not supported by HP NNM</description>
        <from>assignee</from>
        <to>assignee</to>
      </input>
      <output global="false" required="false">
        <description>XML output that contains results returned by the adapter request. The contents of the response will vary by adapter.</description>
        <to>events</to>
        <from>events</from>
      </output>
      <output global="false" required="true">
        <description>String value - status success, failure</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String value - verbose status message.</description>
        <to>success message</to>
        <from>success message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <input>
                  <from>serial number</from>
                </input>
                <output>
                  <to>event</to>
                  <concept type="AutoPilot-OA-Event_Management:Event">
                    <attribute>serial</attribute>
                  </concept>
                </output>
              </assignment>
              <assignment>
                <input>
                  <from>assignee</from>
                </input>
                <output>
                  <to>event</to>
                  <concept type="AutoPilot-OA-Event_Management:Event">
                    <attribute>owner-id</attribute>
                  </concept>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>event</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>events</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens>
                    <replacement-token name="event">
                      <input global="false">
                        <from>event</from>
                      </input>
                    </replacement-token>
                  </stylesheet-replacement-tokens>
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" cdata-section-elements="location" />
  <xsl:template match="/">
    <Events>
      <xsl:choose>
        <xsl:when test="string-length(//event) &gt; 0">
          <xsl:copy-of select="//event" />
        </xsl:when>
        <xsl:otherwise>${event}</xsl:otherwise>
      </xsl:choose>
    </Events>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>configuration name</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[text() = "BMC_Event_Manager"]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <console-output>
              <type>INFO</type>
              <summary>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </summary>
              <detail>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </detail>
            </console-output>
            <assign>
              <assignment>
                <input>
                  <from>serial number</from>
                </input>
                <output>
                  <to>event</to>
                  <concept type="AutoPilot-OA-Event_Management:Event">
                    <attribute>id</attribute>
                  </concept>
                </output>
              </assignment>
              <assignment>
                <input>
                  <from>assignee</from>
                </input>
                <output>
                  <to>event</to>
                  <concept type="AutoPilot-OA-Event_Management:Event">
                    <attribute>owner-id</attribute>
                  </concept>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>event</to>
                  <value>assigned</value>
                  <concept type="AutoPilot-OA-Event_Management:Event">
                    <attribute>status</attribute>
                  </concept>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>event</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>events</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens>
                    <replacement-token name="event">
                      <input global="false">
                        <from>event</from>
                      </input>
                    </replacement-token>
                  </stylesheet-replacement-tokens>
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" cdata-section-elements="location" />
  <xsl:template match="/">
    <Events>
      <xsl:choose>
        <xsl:when test="string-length(//event) &gt; 0">
          <xsl:copy-of select="//event" />
        </xsl:when>
        <xsl:otherwise>${event}</xsl:otherwise>
      </xsl:choose>
    </Events>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
      <call-process>
        <process-name>:AutoPilot-OA-Event_Management:Update Events</process-name>
        <parameters>
          <input>
            <from>events</from>
            <to>events</to>
          </input>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <output>
            <from>events</from>
            <to>events</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>success message</from>
            <to>success message</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow will assign an event to a specified assignee.

Not supported by HPNNM</description>
        <version>1.0</version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>049b76f6-dd8b-3e4e-b339-63a57b306f14</process-id>
      <author></author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by></last-modified-by>
      <last-modified>1283171538534</last-modified>
    </meta>
  </process-def>
</process>