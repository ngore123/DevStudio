<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":AutoPilot-OA-Event_Management:Setup_Validation:Test Get Events by Node">
    <parameters>
      <input global="false" required="true">
        <description>String value - The name of a configuration item defined in a configuration module.

Example: Netcool_OMNIbus, HP_NNM, BMC_Event_Manager
</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>String value - The host or node name 

Note: This function is not supported by HP NNM


BMC Event Manager: The Node corresponds to the 'Host Address' of the Event in BEM.</description>
        <from>host</from>
        <to>host</to>
      </input>
      <output global="false" required="false">
        <description>An XML concept document containing events with the specified node</description>
        <to>events</to>
        <from>events</from>
      </output>
      <output global="false" required="true">
        <description>String value - status success, failure</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String value - verbose status message.</description>
        <to>success message</to>
        <from>success message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-Event_Management:Searches:Get Events by Node</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <input>
            <from>host</from>
            <to>node</to>
          </input>
          <output>
            <from>events</from>
            <to>events</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>success message</from>
            <to>success message</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow tests the find event by node.</description>
        <version>1.0</version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>507d50c1-427e-3a0e-a241-3f8203acbbe2</process-id>
      <author></author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by></last-modified-by>
      <last-modified>1283171556533</last-modified>
    </meta>
  </process-def>
</process>