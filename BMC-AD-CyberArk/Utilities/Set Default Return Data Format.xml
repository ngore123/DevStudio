<process draft="false" dev-studio-version="8.2.00">
  <process-def name=":BMC-AD-CyberArk:Utilities:Set Default Return Data Format">
    <parameters>
      <input global="false" required="false">
        <from>return data format</from>
        <to>return data format</to>
      </input>
      <output global="false" required="true">
        <description>default return data format</description>
        <to>return data format</to>
        <from>return data format</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence />
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>return data format</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[text()='']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <console-output>
              <type>INFO</type>
            </console-output>
            <assign>
              <assignment>
                <output>
                  <to>return data format</to>
                  <value>XML</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <after-execution>
                <log>
                  <level>debug</level>
                  <parameters>
                    <input>
                      <value>Done assigning default data format</value>
                    </input>
                    <input>
                      <from>return data format</from>
                    </input>
                  </parameters>
                </log>
              </after-execution>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Set Default Adapter Name 

Sets the default name for the adapter 
Inputs 
	adapter name 

		The adapter name that needs to be 			verified. 

Output 

	adapter name 

		The default adapter name 
</description>
        <version>1.0</version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>d052c5e5-f10c-4a9f-b8e5-b1429ed82929</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
