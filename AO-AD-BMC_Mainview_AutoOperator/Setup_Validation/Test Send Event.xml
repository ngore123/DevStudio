                    <slot name="mc_tool_suggestion">An example message for autooperator MVAO</slot>
                  </slot-values>
                </item>
              </items>
            </value>
            <to>slots</to>
          </input>
          <output>
            <from>adapter response</from>
            <to>adapter response</to>
          </output>
        </parameters>
        <meta>
