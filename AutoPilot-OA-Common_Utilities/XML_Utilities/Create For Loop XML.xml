<process draft="false" dev-studio-version="7.6.03">
  <process-def name=":AutoPilot-OA-Common_Utilities:XML_Utilities:Create For Loop XML">
    <parameters>
      <input global="false" required="true">
        <description>A whole number indicating the number of elements the loop should have</description>
        <from>loopcount</from>
        <to>loopcount</to>
      </input>
      <input global="false" required="false">
        <description>Name of the root element to be used in the output XML.
Optional - If none provided, a default value of 'loops' is used.</description>
        <from>root element name</from>
        <to>root element name</to>
      </input>
      <input global="false" required="false">
        <description>Name of the child element to be used in the output XML.
Optional - If none provided, a default value of 'loop' is used.</description>
        <from>child element name</from>
        <to>child element name</to>
      </input>
      <output global="false" required="false">
        <description>An XML document in this format:

&lt;loops&gt;
  &lt;loop&gt;1&lt;/loop&gt;
  &lt;loop&gt;2&lt;/loop&gt;
  (any number of iterations)
&lt;/loops&gt;</description>
        <to>loop</to>
        <from>loop</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>root element name</from>
            </input>
          </document>
          <result>
            <output>
              <to>root element name</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens />
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <value>
      <xsl:choose>
        <xsl:when test="string-length(normalize-space(.)) &gt; 0">
          <xsl:value-of select="." disable-output-escaping="no" />
        </xsl:when>
        <xsl:otherwise>
          <xsl:text disable-output-escaping="no">loops</xsl:text>
        </xsl:otherwise>
      </xsl:choose>
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <assignment>
          <document>
            <input>
              <from>child element name</from>
            </input>
          </document>
          <result>
            <output>
              <to>child element name</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens />
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <value>
      <xsl:choose>
        <xsl:when test="string-length(normalize-space(.)) &gt; 0">
          <xsl:value-of select="." disable-output-escaping="no" />
        </xsl:when>
        <xsl:otherwise>
          <xsl:text disable-output-escaping="no">loop</xsl:text>
        </xsl:otherwise>
      </xsl:choose>
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <assign>
        <assignment>
          <document>
            <input>
              <from>loopcount</from>
            </input>
          </document>
          <result>
            <output>
              <to>loops string</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="rootElem">
                <input global="false">
                  <from>root element name</from>
                </input>
              </replacement-token>
              <replacement-token name="childElem">
                <input global="false">
                  <from>child element name</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:tokenizer="com.realops.common.xml" version="1.0">
  <xsl:output indent="no" />
  <xsl:variable name="loopCount" select="." />
  <xsl:template match="/">
    <loops>
      <xsl:value-of select="tokenizer:StringXmlUtils.loopCountToXMLString($loopCount, '${rootElem}', '${childElem}')" disable-output-escaping="no" />
    </loops>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[2]</location>
      </assign>
      <utility>
        <string>
          <regex>
            <input>
              <from>loops string</from>
            </input>
            <expression>
              <search case-sensitive="false">&lt;\?xml[\s\t]*version=[\'\"]1.[0-9]+[\"\'][\s\t]*(encoding=[\'\"][\w\W]*[\'\"])*[\s\t]*(standalone=[\'\"][\w\W]*[\'\"])*[\s\t]*\?&gt;</search>
              <replace replace-all="true" />
            </expression>
          </regex>
        </string>
        <output global="false">
          <to>loops string</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:utility[1]</location>
      </utility>
      <utility>
        <string>
          <regex>
            <input>
              <from>loops string</from>
            </input>
            <expression>
              <search case-sensitive="false">&lt;!DOCTYPE.*?&gt;</search>
              <replace replace-all="true" />
            </expression>
          </regex>
        </string>
        <output global="false">
          <to>loops string</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:utility[2]</location>
      </utility>
      <assign>
        <assignment>
          <output>
            <to>XML</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="XML String">
                <input global="false">
                  <from>loops string</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <XML>${XML String}</XML>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <assignment>
          <document>
            <input>
              <from>XML</from>
            </input>
          </document>
          <result>
            <output>
              <to>loop</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="rootElem">
                <input global="false">
                  <from>root element name</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <xsl:copy-of select="//${rootElem}" />
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>XML Output</value>
              </input>
              <input>
                <from>loop</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[3]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>e752385e-c286-45f0-a097-cef43d7a722d</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1490969871633</last-modified>
    </meta>
  </process-def>
</process>