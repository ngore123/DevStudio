<process draft="false" dev-studio-version="20.02.00">
  <process-def name=":AutoPilot-OA-Common_Utilities:XML_Utilities:Convert JSON to XML">
    <parameters>
      <input global="false" required="true">
        <description>JSON</description>
        <from>JSON</from>
        <to>JSON</to>
      </input>
      <output global="false" required="false">
        <to>XML</to>
        <from>XML</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>JSON</from>
            </input>
          </document>
          <result>
            <output>
              <to>XML</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens />
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:json="org.json" version="1.0">
  <xsl:output indent="no" />
  <xsl:variable name="str" select="normalize-space(.)" />
  <xsl:template match="/">
    <XML>
      <xsl:choose>
        <xsl:when test="string-length($str) &gt; 0">
          <xsl:value-of select="json:XML.fromJSONString(., &quot;json&quot;)" disable-output-escaping="no" />
        </xsl:when>
        <xsl:otherwise>
          <xsl:value-of select="." disable-output-escaping="no" />
        </xsl:otherwise>
      </xsl:choose>
    </XML>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <from>JSON</from>
              </input>
            </parameters>
          </log>
        </before-execution>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <from>XML</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[1]</location>
      </assign>
      <call-process>
        <process-name>:AutoPilot-OA-Common_Utilities:XML_Utilities:XML String to XML</process-name>
        <parameters>
          <input>
            <from>XML</from>
            <to>XML String</to>
          </input>
          <output>
            <from>XML</from>
            <to>XML</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <call-process>
        <process-name>:AutoPilot-OA-Common_Utilities:XML_Utilities:Strip Namespaces</process-name>
        <parameters>
          <input>
            <from>XML</from>
            <to>XML</to>
          </input>
          <output>
            <from>XML</from>
            <to>XML</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[2]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Converts JSON to XML. This will require json.jar to be installed on your AO Studio and Peers.</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>cce230f3-6ce9-4e94-a0d4-89891e439dab</process-id>
      <author>rdevries</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1623934350760</last-modified>
    </meta>
  </process-def>
</process>