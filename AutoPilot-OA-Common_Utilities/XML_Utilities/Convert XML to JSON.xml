<process draft="false" dev-studio-version="8.2.00">
  <process-def name=":AutoPilot-OA-Common_Utilities:XML_Utilities:Convert XML to JSON">
    <parameters>
      <input global="false" required="true">
        <description>XML</description>
        <from>XML</from>
        <to>XML</to>
      </input>
      <output global="false" required="false">
        <description>JSON</description>
        <to>JSON</to>
        <from>JSON</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-OA-Common_Utilities:XML_Utilities:Strip Namespaces</process-name>
        <unique-id>c0d55b55-4649-44be-ad22-87501b1e94c3</unique-id>
        <parameters>
          <input>
            <from>XML</from>
            <to>XML</to>
          </input>
          <output>
            <from>XML</from>
            <to>XML</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <assign>
        <assignment>
          <output>
            <to>JSON</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="XML">
                <input global="false">
                  <from>XML</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:json="org.json" version="1.0">
  <xsl:output indent="no" />
  <xsl:variable name="xmlToken">$[XML]</xsl:variable>
  <xsl:template match="/">
    <xsl:variable name="json-string" select="json:XML.toJSONObject($xmlToken)" />
    <JSON>
      <xsl:value-of select="$json-string" disable-output-escaping="no" />
    </JSON>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>Input</value>
              </input>
              <input>
                <from>XML</from>
              </input>
            </parameters>
          </log>
        </before-execution>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>Output</value>
              </input>
              <input>
                <from>JSON</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Converts XML to JSON. This will require json.jar to be installed on your AO Studio and Peers.</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>8751f369-384d-4b50-b838-61a898b4c7ac</process-id>
      <author>rdevries</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1551373103426</last-modified>
    </meta>
  </process-def>
</process>