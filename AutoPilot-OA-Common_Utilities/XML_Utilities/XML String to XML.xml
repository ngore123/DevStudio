<process draft="false" dev-studio-version="8.0.00">
  <process-def name=":AutoPilot-OA-Common_Utilities:XML_Utilities:XML String to XML">
    <parameters>
      <input global="false" required="true">
        <description>XML Formatted String</description>
        <from>XML String</from>
        <to>XML String</to>
      </input>
      <output global="false" required="true">
        <description>XML Output</description>
        <to>XML</to>
        <from>XML</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <utility>
        <string>
          <regex>
            <input>
              <from>XML String</from>
            </input>
            <expression>
              <search case-sensitive="false">&lt;\?xml[\s\t]*version=[\'\"]1.[0-9]+[\"\'][\s\t]*(encoding=[\'\"][\w\W]*[\'\"])*[\s\t]*(standalone=[\'\"][\w\W]*[\'\"])*[\s\t]*\?&gt;</search>
              <replace replace-all="true" />
            </expression>
          </regex>
        </string>
        <output global="false">
          <to>XML String</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:utility[1]</location>
      </utility>
      <utility>
        <string>
          <regex>
            <input>
              <from>XML String</from>
            </input>
            <expression>
              <search case-sensitive="false">&lt;!DOCTYPE.*?&gt;</search>
              <replace replace-all="true" />
            </expression>
          </regex>
        </string>
        <output global="false">
          <to>XML String</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:utility[2]</location>
      </utility>
      <utility>
        <string>
          <regex>
            <input>
              <from>XML String</from>
            </input>
            <expression>
              <search case-sensitive="false">\\"</search>
              <replace replace-all="true">"</replace>
            </expression>
          </regex>
        </string>
        <output global="false">
          <to>XML String</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:utility[3]</location>
      </utility>
      <assign>
        <assignment>
          <output>
            <to>XML</to>
            <value>
              <empty />
            </value>
          </output>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="XML String">
                <input global="false">
                  <from>XML String</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <XML>${XML String}</XML>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
