        <error-message>
          <xsl:value-of select="./*[name() = 'value']/text()" disable-output-escaping="no" />
        </error-message>
      </xsl:when>
    </xsl:choose>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
