<process draft="false" dev-studio-version="8.0.00">
  <process-def name=":AutoPilot-OA-Common_Utilities:URLEncode">
    <parameters>
      <input global="false" required="true">
        <description>An input string that will be used in a URL with characters like &amp;, etc. like below

http://ahost:2000/webcontext?param1=value1&amp;param2=value2</description>
        <from>input</from>
        <to>input</to>
      </input>
      <output global="false" required="false">
        <to>output</to>
        <from>output</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>input</from>
            </input>
          </document>
          <result>
            <output>
              <to>output</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens />
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:encoder="java.net.URLEncoder" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <value>
      <xsl:value-of select="encoder:encode(., 'UTF-8')" disable-output-escaping="no" />
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <call-process>
        <process-name>:AutoPilot-OA-Common_Utilities:XML_Utilities:Strip Namespaces</process-name>
        <parameters>
          <input>
            <from>output</from>
            <to>XML</to>
          </input>
          <output>
            <from>XML</from>
            <to>output</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
        <consumer>OCP</consumer>
        <consumer>SOAP</consumer>
      </expose-to>
      <documentation>
        <description>URL Encode a string using Java.

Input:
An input string that will be used in a URL with characters like &amp;, etc. like below

http://ahost:2000/webcontext?param1=value1&amp;param2=value2


Output:
http://ahost:2000/webcontext?param1=value1&amp;mp;param2=value2</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>db8f2e67-9906-4c87-9c4b-b3b4756e33bb</process-id>
      <author>Ranganath Samudrala</author>
      <category>Common Utilities</category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>rsamudrala</last-modified-by>
      <last-modified>1525025150346</last-modified>
    </meta>
  </process-def>
</process>