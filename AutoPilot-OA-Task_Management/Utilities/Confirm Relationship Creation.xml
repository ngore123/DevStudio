<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":AutoPilot-OA-Task_Management:Utilities:Confirm Relationship Creation">
    <parameters>
      <input global="false" required="true">
        <description>String Value - The name of a configuration entry defined in a configuration module. 

Example: BCAN</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>This contains the value you of the configuration item reconciliation id. 

Example:  RE0050560C63F2rpytRAn_CoAAWI0A</description>
        <from>configuration item reconciliation id</from>
        <to>configuration item reconciliation id</to>
      </input>
      <input global="false" required="true">
        <description>This contains the value you of the task instance id. 

Example:  AS0050560C63F2rpytRAn_CoAAWI0A



</description>
        <from>task instance id</from>
        <to>task instance id</to>
      </input>
      <output global="false" required="false">
        <description>String Value - Login ID 

Example: 
adapteruser</description>
        <to>tasks</to>
        <from>tasks</from>
      </output>
      <output global="false" required="true">
        <description>String Value - Indicates whether or not the process completed successfully. 

Valid values: 0-success,1-failure
</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="true">
        <description>String Value â€“ A verbose message indicating the success or failure of the process</description>
        <to>status message</to>
        <from>status message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>task instance id</from>
            </input>
          </document>
          <result>
            <output>
              <to>formed query</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="task instance id">
                <input global="false">
                  <from>task instance id</from>
                </input>
              </replacement-token>
              <replacement-token name="configuration reconciliation id">
                <input global="false">
                  <from>configuration item reconciliation id</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" cdata-section-elements="value " />
  <xsl:template match="/">
    <value>
      <xsl:text disable-output-escaping="no">'1000000204' = "$[configuration reconciliation id]" and '1000000205' = "$[task instance id]"</xsl:text>
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <call-process>
        <process-name>:AutoPilot-OA-Task_Management:Searches:Get Relationships</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <input>
            <from>formed query</from>
            <to>query</to>
          </input>
          <output>
            <from>tasks</from>
            <to>tasks</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>status message</from>
            <to>status message</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-process[1]</location>
      </call-process>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>success</value>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>tasks</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>task id</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//task-id/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>task id</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>status message</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[concat("Data was retrieved successfully for Task ID: ", .)]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>success status</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[text() = 'failure']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>failure</value>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>task instance id</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>status message</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[concat("Unable to retrieve data for Task Instance ID: ", .)]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This workflow will confirm the creation of a relationship.

</description>
        <version>1.0</version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>ccdc0d03-346c-3b22-9479-e845bc4fb720</process-id>
      <author></author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by></last-modified-by>
      <last-modified>1283173303887</last-modified>
    </meta>
  </process-def>
</process>