<process draft="false" dev-studio-version="8.3.00">
  <process-def name=":BMC-SA-Event_Orchestration_PatrolAgent_Down:Utilities:Convert Target Connection Data to Host Connection Details">
    <parameters>
      <input global="false" required="true">
        <from>target connection data</from>
        <to>target connection data</to>
      </input>
      <output global="false" required="false">
        <to>connection details</to>
        <from>connection details</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>target connection data</from>
            </input>
          </document>
          <result>
            <output>
              <to>connection details</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens />
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <connection-details>
      <xsl:copy-of select="//invocation-mechanism" />
      <xsl:copy-of select="//adapter-name" />
      <hostname>
        <xsl:value-of select="//component/@name" disable-output-escaping="no" />
      </hostname>
      <xsl:copy-of select="//username" />
      <xsl:copy-of select="//password" />
      <xsl:apply-templates select="//adapter-options/*" mode="adapter-options" />
      <xsl:apply-templates select="//command-options/*" mode="command-options" />
    </connection-details>
  </xsl:template>
  <xsl:template match="@*|node()" mode="adapter-options">
    <xsl:copy-of select="." />
  </xsl:template>
  <xsl:template match="@*|node()" mode="command-options">
    <xsl:copy-of select="." />
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>=== incoming connection-details = target connection data===</value>
              </input>
              <input>
                <from>target connection data</from>
              </input>
            </parameters>
          </log>
        </before-execution>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>=== translated connection details=====</value>
              </input>
              <input>
                <from>connection details</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[1]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>51bc8fd2-cc18-41c8-aed5-9f5d068df392</process-id>
      <author>aoadmin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1571228980471</last-modified>
    </meta>
  </process-def>
</process>