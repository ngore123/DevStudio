<process draft="false" dev-studio-version="20.02.00">
  <process-def name=":BMC-SA-Event_Orchestration:Utilities:Truesight Login">
    <parameters>
      <input global="false" required="true">
        <from>all configurations</from>
        <to>all configurations</to>
      </input>
      <output global="false" required="false">
        <to>authentication token</to>
        <from>authentication token</from>
      </output>
      <output global="false" required="false">
        <to>status</to>
        <from>status</from>
      </output>
      <output global="false" required="false">
        <to>status code</to>
        <from>status code</from>
      </output>
      <output global="false" required="false">
        <to>error message</to>
        <from>error message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>authentication token</to>
            <value></value>
          </output>
        </assignment>
        <assignment>
          <document>
            <input global="true">
              <configuration>:BMC-SA-Event_Orchestration_Configuration</configuration>
            </input>
          </document>
          <result>
            <output>
              <to>event source type</to>
            </output>
          </result>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[//General/EventSourceType/text()]]></xpath>
          </xpath-expression>
        </assignment>
        <assignment>
          <output>
            <to>status</to>
            <value>success</value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>status code</to>
            <value>200</value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>error message</to>
            <value></value>
          </output>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>Event source type </value>
              </input>
              <input>
                <from>event source type</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[1]</location>
      </assign>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence />
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>all configurations</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[//Specifics/Defaults/BypassEventNotes/text() = 'false']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>all configurations</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>login configuration</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens>
                    <replacement-token name="event source type">
                      <input global="false">
                        <from>event source type</from>
                      </input>
                    </replacement-token>
                  </xpath-replacement-tokens>
                  <xpath><![CDATA[//*[name() = '${event source type}']/WebServiceConnectionData]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <after-execution>
                <log>
                  <level>debug</level>
                  <parameters>
                    <input>
                      <value>TSPS Login Data</value>
                    </input>
                    <input>
                      <from>login configuration</from>
                    </input>
                  </parameters>
                </log>
              </after-execution>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>login configuration</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>url</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//WebServiceConnectionData/BASE_URL/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>login configuration</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>loginuri</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//WebServiceConnectionData/LOGIN_URI/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <input>
                  <concat>
                    <input required="false">
                      <from>url</from>
                    </input>
                    <input required="false">
                      <from>loginuri</from>
                    </input>
                  </concat>
                </input>
                <output>
                  <to>login url</to>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>login configuration</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>user name</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//WebServiceConnectionData/UserName/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>login configuration</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>password</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens />
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <xsl:copy-of select="//WebServiceConnectionData/Password/*[name()='EncryptedData']" />
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>login configuration</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>tenant</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//WebServiceConnectionData/Tenant/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[2]</location>
            </assign>
            <call-process>
              <process-name>:BMC-SA-Event_Orchestration:Utilities:Login</process-name>
              <parameters>
                <input>
                  <from>all configurations</from>
                  <to>adapter name</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//Adapters/RESTAdapter/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>login url</from>
                  <to>url</to>
                </input>
                <input>
                  <from>user name</from>
                  <to>user name</to>
                </input>
                <input>
                  <from>password</from>
                  <to>password</to>
                </input>
                <input>
                  <from>tenant</from>
                  <to>tenant</to>
                </input>
                <output>
                  <from>authentication token</from>
                  <to>authentication token</to>
                </output>
                <output>
                  <from>status</from>
                  <to>status</to>
                </output>
                <output>
                  <from>status code</from>
                  <to>status code</to>
                </output>
                <output>
                  <from>status reason</from>
                  <to>status reason</to>
                </output>
                <output>
                  <from>error message</from>
                  <to>error message</to>
                </output>
                <output>
                  <from>error number</from>
                  <to>error number</to>
                </output>
                <output>
                  <from>remediation message</from>
                  <to>remediation message</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:call-process[1]</location>
            </call-process>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>3b718db4-03f0-4cdc-b5cb-647f47d0e643</process-id>
      <author>aoadmin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1597431187836</last-modified>
    </meta>
  </process-def>
</process>