<process draft="false" dev-studio-version="8.0.00">
  <process-def name=":BMC-SA-Event_Orchestration:Utilities:Extract Event Configuration Data">
    <parameters>
      <input global="false" required="false">
        <from>event type</from>
        <to>event type</to>
      </input>
      <output global="false" required="false">
        <to>event source type</to>
        <from>event source type</from>
      </output>
      <output global="false" required="false">
        <to>itsm type</to>
        <from>itsm type</from>
      </output>
      <output global="false" required="false">
        <to>all configurations</to>
        <from>all configurations</from>
      </output>
      <output global="false" required="false">
        <to>event configuration</to>
        <from>event configuration</from>
      </output>
      <output global="false" required="false">
        <to>status</to>
        <from>status</from>
      </output>
      <output global="false" required="false">
        <to>error message</to>
        <from>error message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>========Input=======</value>
              </input>
              <input>
                <from>event type</from>
              </input>
            </parameters>
          </log>
        </before-execution>
        <location>:assign[1]</location>
      </assign>
      <assign>
        <assignment>
          <input global="true">
            <configuration>:BMC-SA-Event_Orchestration_Configuration:General:EventSourceType</configuration>
          </input>
          <output>
            <to>event source type</to>
          </output>
        </assignment>
        <assignment>
          <input global="true">
            <configuration>:BMC-SA-Event_Orchestration_Configuration:General:ITSMType</configuration>
          </input>
          <output>
            <to>itsm type</to>
          </output>
        </assignment>
        <assignment>
          <input global="true">
            <configuration>:BMC-SA-Event_Orchestration_Configuration:Specifics</configuration>
          </input>
          <output>
            <to>all configurations</to>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>status</to>
            <value>success</value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>error message</to>
            <value></value>
          </output>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>===============step 1==================</value>
              </input>
              <input>
                <from>event source type</from>
              </input>
              <input>
                <from>itsm type</from>
              </input>
              <input>
                <from>all configurations</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[2]</location>
      </assign>
      <assign>
        <assignment>
          <document>
            <input>
              <from>all configurations</from>
            </input>
          </document>
          <result>
            <output>
              <to>event configuration</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="event type">
                <input global="false">
                  <from>event type</from>
                </input>
              </replacement-token>
              <replacement-token name="event source type">
                <input global="false">
                  <from>event source type</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:variable name="eventSourceType">${event source type}</xsl:variable>
  <xsl:template match="/">
    <xsl:copy-of select="//${event source type}/EventTypes/*[EventType/text() = '${event type}']" />
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <from>event source type</from>
              </input>
              <input>
                <from>event type</from>
              </input>
              <input>
                <from>all configurations</from>
              </input>
            </parameters>
          </log>
        </before-execution>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <from>event configuration</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[3]</location>
      </assign>
      <assign>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>==========Output========</value>
              </input>
              <input>
                <from>event source type</from>
              </input>
              <input>
                <from>itsm type</from>
              </input>
              <input>
                <from>all configurations</from>
              </input>
              <input>
                <from>event configuration</from>
              </input>
              <input>
                <from>status</from>
              </input>
              <input>
                <from>error message</from>
              </input>
            </parameters>
          </log>
        </before-execution>
        <location>:assign[4]</location>
      </assign>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
        <consumer>OCP</consumer>
        <consumer>SOAP</consumer>
      </expose-to>
      <documentation>
        <description>This a process template. It has a few basic things.
1. Log Input
2. Log Output
3. Atleast 2 basic output parameters - status and error message.</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>true</record-metrics>
      <process-id>8a56efc0-6663-4dcc-9d01-0aad061dae0c</process-id>
      <author>Ranganath Samudrala</author>
      <category>Event Orchestration.</category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1522373415909</last-modified>
    </meta>
  </process-def>
</process>