<process draft="false" dev-studio-version="20.02.00">
  <process-def name=":BMC-SA-Event_Orchestration:Utilities:ITSM API Login">
    <parameters>
      <input global="false" required="true">
        <description>All configurations for this solution</description>
        <from>all configurations</from>
        <to>all configurations</to>
      </input>
      <input global="false" required="true">
        <description>ITSM type  - should be BMC_AR_System. Otherwise, it will skip login attempt.</description>
        <from>itsm type</from>
        <to>itsm type</to>
      </input>
      <output global="false" required="false">
        <to>authentication token</to>
        <from>authentication token</from>
      </output>
      <output global="false" required="false">
        <to>status</to>
        <from>status</from>
      </output>
      <output global="false" required="false">
        <to>status code</to>
        <from>status code</from>
      </output>
      <output global="false" required="false">
        <to>error message</to>
        <from>error message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>authentication token</to>
            <value></value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>status</to>
            <value>success</value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>status code</to>
            <value></value>
          </output>
        </assignment>
        <assignment>
          <output>
            <to>error message</to>
            <value></value>
          </output>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <before-execution>
                <log>
                  <level>debug</level>
                  <parameters>
                    <input>
                      <value>===== Skipping ITSM API Login Process=====</value>
                    </input>
                    <input>
                      <value></value>
                    </input>
                  </parameters>
                </log>
              </before-execution>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>all configurations</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[//Specifics/Defaults/UseRemedyJavaAPI/text() = 'false']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <switch>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>true</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <execute-all-applicable-cases>false</execute-all-applicable-cases>
              <default>
                <sequence>
                  <assign>
                    <meta>
                      <documentation />
                      <checkpoint>false</checkpoint>
                    </meta>
                    <before-execution>
                      <log>
                        <level>debug</level>
                        <parameters>
                          <input>
                            <value>====Bypassing ITSM authentication==</value>
                          </input>
                          <input>
                            <value></value>
                          </input>
                        </parameters>
                      </log>
                    </before-execution>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[1]:assign[1]</location>
                  </assign>
                </sequence>
              </default>
              <case>
                <condition>
                  <document>
                    <input>
                      <from>all configurations</from>
                    </input>
                  </document>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//Specifics/Defaults/BypassITSM/text() = 'false']]></xpath>
                  </xpath-expression>
                </condition>
                <sequence>
                  <assign>
                    <assignment>
                      <output>
                        <to>itsm authorization token</to>
                        <value></value>
                      </output>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>all configurations</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>adapter name</to>
                        </output>
                      </result>
                      <xpath-expression>
                        <xpath-replacement-tokens />
                        <xpath><![CDATA[//Adapters/RESTAdapter/text()]]></xpath>
                      </xpath-expression>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>all configurations</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>web service connection data</to>
                        </output>
                      </result>
                      <xpath-expression>
                        <xpath-replacement-tokens>
                          <replacement-token name="itsm type">
                            <input global="false">
                              <from>itsm type</from>
                            </input>
                          </replacement-token>
                        </xpath-replacement-tokens>
                        <xpath><![CDATA[//*[name() = '${itsm type}']/WebServiceConnectionData]]></xpath>
                      </xpath-expression>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>web service connection data</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>url</to>
                        </output>
                      </result>
                      <xpath-expression>
                        <xpath-replacement-tokens />
                        <xpath><![CDATA[//RestURL/text()]]></xpath>
                      </xpath-expression>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>web service connection data</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>user name</to>
                        </output>
                      </result>
                      <xpath-expression>
                        <xpath-replacement-tokens />
                        <xpath><![CDATA[//UserName/text()]]></xpath>
                      </xpath-expression>
                    </assignment>
                    <assignment>
                      <document>
                        <input>
                          <from>web service connection data</from>
                        </input>
                      </document>
                      <result>
                        <output>
                          <to>password</to>
                        </output>
                      </result>
                      <xslt-stylesheet>
                        <stylesheet-replacement-tokens />
                        <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <xsl:choose>
      <xsl:when test="boolean(//Password/*[name() = 'EncryptedData']) ">
        <xsl:copy-of select="//Password/*[name() = 'EncryptedData']" />
      </xsl:when>
      <xsl:otherwise>
        <value>
          <xsl:value-of select="//Password/text()" disable-output-escaping="no" />
        </value>
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                      </xslt-stylesheet>
                    </assignment>
                    <meta>
                      <documentation />
                      <checkpoint>false</checkpoint>
                    </meta>
                    <after-execution>
                      <log>
                        <level>debug</level>
                        <parameters>
                          <input>
                            <value>=========ITSM API Login============</value>
                          </input>
                          <input>
                            <from>url</from>
                          </input>
                          <input>
                            <from>user name</from>
                          </input>
                          <input>
                            <from>password</from>
                          </input>
                        </parameters>
                      </log>
                    </after-execution>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[2]:assign[1]</location>
                  </assign>
                  <call-process>
                    <process-name>:AutoPilot-OA-Event_Orchestration:Utilities:Login</process-name>
                    <parameters>
                      <input>
                        <from>adapter name</from>
                        <to>adapter name</to>
                      </input>
                      <input>
                        <from>url</from>
                        <to>url</to>
                      </input>
                      <input>
                        <from>user name</from>
                        <to>user name</to>
                      </input>
                      <input>
                        <from>password</from>
                        <to>password</to>
                      </input>
                      <input>
                        <from>itsm type</from>
                        <to>itsm type</to>
                      </input>
                      <output>
                        <from>authentication token</from>
                        <to>authentication token</to>
                      </output>
                      <output>
                        <from>status</from>
                        <to>status</to>
                      </output>
                      <output>
                        <from>status code</from>
                        <to>status code</to>
                      </output>
                      <output>
                        <from>error message</from>
                        <to>error message</to>
                      </output>
                    </parameters>
                    <meta>
                      <documentation />
                      <run-mode-handling>
                        <run-mode>OCP</run-mode>
                        <allow-step-into>false</allow-step-into>
                      </run-mode-handling>
                      <checkpoint>false</checkpoint>
                    </meta>
                    <location>:switch[1]:sequence[2]:switch[1]:sequence[2]:call-process[1]</location>
                  </call-process>
                </sequence>
              </case>
              <location>:switch[1]:sequence[2]:switch[1]</location>
            </switch>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
        <consumer>OCP</consumer>
        <consumer>SOAP</consumer>
      </expose-to>
      <documentation>
        <description>Login into Remedy if the ITSM type is BMC_AR_System and REST_URL is set.</description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
