                <input>
                  <from>event data</from>
                  <to>event data</to>
                </input>
                <input>
                  <from>ts auth token</from>
                  <to>auth token</to>
                </input>
                <input>
                  <from>error message</from>
                  <to>notes</to>
                </input>
                <input>
                  <value>Log Error</value>
                  <to>action name</to>
                </input>
                <input>
                  <from>error message</from>
                  <to>message</to>
                </input>
                <input>
                  <value>MAJOR</value>
                  <to>severity</to>
                </input>
                <input>
                  <value></value>
                  <to>command output</to>
                </input>
                <input>
                  <value>1</value>
                  <to>exit code</to>
                </input>
                <input>
                  <from>error message</from>
                  <to>error notes</to>
