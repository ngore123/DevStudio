                </output>
              </assignment>
              <assignment>
                <output>
                  <to>status</to>
                  <value>failure</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>task id</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[string-length(.) > 0]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <call-process>
              <process-name>:AutoPilot-OA-Event_Orchestration:ITSM:Task_Management:Get Task by Task ID</process-name>
              <unique-id>4b36916c-5a81-4a48-8249-6e21e1e6955e</unique-id>
              <parameters>
                <input>
                  <from>task id</from>
                  <to>task id</to>
                </input>
                <input>
                  <from>itsm type</from>
                  <to>itsm type</to>
                </input>
                <input>
                  <from>all configurations</from>
                  <to>all configurations</to>
                </input>
                <input>
                  <from>event source type</from>
                  <to>event source type</to>
                </input>
                <output>
                  <from>task xml</from>
                  <to>task xml</to>
                </output>
                <output>
                  <from>status</from>
                  <to>status</to>
                </output>
                <output>
                  <from>error message</from>
                  <to>error message</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:call-process[1]</location>
            </call-process>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>task xml</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>event data</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[substring-after(//task-notes/text(), ":")]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
            <call-process>
              <process-name>:AutoPilot-OA-Common_Utilities:XML_Utilities:XML String to XML</process-name>
              <parameters>
                <input>
                  <from>event data</from>
                  <to>XML String</to>
                </input>
                <output>
                  <from>XML</from>
                  <to>event data</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>OCP</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:call-process[2]</location>
            </call-process>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
