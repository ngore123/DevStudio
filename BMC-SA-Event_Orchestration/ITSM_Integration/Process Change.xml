      <input global="false" required="true">
        <description>all configurations for this runbook</description>
        <from>all configurations</from>
        <to>all configurations</to>
      </input>
      <input global="false" required="true">
        <description>An event of common event model structure.</description>
        <from>event data</from>
        <to>event data</to>
      </input>
      <input global="false" required="true">
        <description>Like ServiceDown</description>
        <from>event type</from>
        <to>event type</to>
      </input>
      <input global="false" required="true">
        <description>Like:
&lt;flags&gt;
  &lt;continue-processing&gt;true&lt;/continue-processing&gt;
  &lt;status&gt;success&lt;/status&gt;
  &lt;change-enabled&gt;true&lt;/change-enabled&gt;
  &lt;incident-enabled&gt;true&lt;/incident-enabled&gt;
  &lt;remediation-enabled&gt;true&lt;/remediation-enabled&gt;
  &lt;remediation-required&gt;true&lt;/remediation-required&gt;
  &lt;incident-created&gt;false&lt;/incident-created&gt;
  &lt;incident-updated&gt;true&lt;/incident-updated&gt;
&lt;/flags&gt;</description>
        <from>flags</from>
        <to>flags</to>
      </input>
      <input global="false" required="true">
        <description>Like:

&lt;itsm-data&gt;
  &lt;itsm-incident-id&gt;INC000000000469&lt;/itsm-incident-id&gt;
&lt;/itsm-data&gt; </description>
        <from>itsm data</from>
        <to>itsm data</to>
      </input>
      <input global="false" required="true">
        <description>Like: BMC_TrueSight-ServiceDown-1
A combination of event source type, event type and configuration id for that event type.
[event-source-type]-[event-type]-[configuration id]</description>
        <from>action id</from>
        <to>action id</to>
      </input>
      <output global="false" required="false">
        <to>change</to>
        <from>change</from>
      </output>
      <output global="false" required="false">
        <to>itsm data</to>
        <from>itsm data</from>
      </output>
      <output global="false" required="false">
        <to>status</to>
        <from>status</from>
      </output>
      <output global="false" required="false">
        <to>error message</to>
        <from>error message</from>
      </output>
