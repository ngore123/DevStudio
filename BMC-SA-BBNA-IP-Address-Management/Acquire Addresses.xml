<process draft="false" dev-studio-version="7.7.01">
  <process-def name=":BMC-SA-BBNA-IP-Address-Management:Acquire Addresses">
    <parameters>
      <input global="false" required="true">
        <description>Pool id of the pool.
In QIP pool id is nothing but subnet address.</description>
        <from>pool id</from>
        <to>pool id</to>
      </input>
      <input global="false" required="true">
        <description>Addresses to be acquired from pool.
Format is as below...  
   &lt;addresses&gt;
            &lt;ip-address&gt;a.b.c.d&lt;/ip-address&gt;
            &lt;ip-address&gt;w.x.y.z&lt;/ip-address&gt;
            &lt;ip-address&gt;p.q.r.s&lt;/ip-address&gt;
     &lt;/addresses&gt;
</description>
        <from>addresses</from>
        <to>addresses</to>
      </input>
      <input global="false" required="false">
        <description>Scope can have twos possible values
1. Any string : It is a organization name same as pod name. 
2. null : in this case we will use default organization.</description>
        <from>scope</from>
        <to>scope</to>
      </input>
      <input global="false" required="false">
        <description>hostname of the server for which the IP address is being acquired.</description>
        <from>hostname</from>
        <to>hostname</to>
      </input>
      <input global="false" required="false">
        <description>transactionId of the CLM process that triggered this call to acquire IP address</description>
        <from>transactionId</from>
        <to>transactionId</to>
      </input>
      <input global="false" required="false">
        <description>nicId of the server for which the IP address is being acquired.</description>
        <from>nicId</from>
        <to>nicId</to>
      </input>
      <output global="false" required="false">
        <description>operation status.
valid values: true, false.</description>
        <to>success</to>
        <from>success</from>
      </output>
      <output global="false" required="false">
        <description>Operation result
Format:
&lt;operation-result&gt;
     &lt;message&gt;error message incase of any error&lt;/message&gt;
     &lt;info&gt;xxxx&lt;/info&gt;
&lt;/operation-result&gt;
</description>
        <to>operation result</to>
        <from>operation result</from>
      </output>
    </parameters>
