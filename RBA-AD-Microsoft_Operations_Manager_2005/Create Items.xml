        <to>items</to>
      </input>
      <input global="false" required="true">
        <description>item-type
The item-type that makes the judgement on what wrapper must be executed.
The valid values are alert</description>
        <from>item type</from>
        <to>item type</to>
      </input>
      <input global="false" required="true">
        <description>version
The MOM application version. The current supported version is v2005</description>
        <from>version</from>
        <to>version</to>
      </input>
      <output global="false" required="false">
        <to>adapter response</to>
        <from>adapter response</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <switch>
        <meta>
