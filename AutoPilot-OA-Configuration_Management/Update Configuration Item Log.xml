  <xsl:template match="/">
    <Configuration-Items>
      <xsl:choose>
        <xsl:when test="string-length(//Configuration-Item) &gt; 0">
          <xsl:copy-of select="//Configuration-Item" />
        </xsl:when>
        <xsl:otherwise>${configuration item}</xsl:otherwise>
      </xsl:choose>
    </Configuration-Items>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
