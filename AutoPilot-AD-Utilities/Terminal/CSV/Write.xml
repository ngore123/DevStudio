<process draft="false" dev-studio-version="7.6.02.06">
  <process-def name=":AutoPilot-AD-Utilities:Terminal:CSV:Write">
    <parameters>
      <input global="false" required="false">
        <description>The name of the Adapter to use for this Process. 

Default Value: CSVAdapter</description>
        <from>adapter name</from>
        <to>adapter name</to>
      </input>
      <input global="false" required="true">
        <description>The fully qualified pathname of the CSV file to be written.</description>
        <from>file name</from>
        <to>file name</to>
      </input>
      <input global="false" required="true">
        <description>Specifies the data to be written into the CSV file. Each &lt;row&gt; tag represents a line and &lt;column&gt; tag represents a field in CSV file.

                &lt;rows&gt;
			    &lt;row&gt;
			      &lt;column&gt;&lt;/column&gt;
			      &lt;column&gt;&lt;/column&gt;
			    &lt;/row&gt;
			    &lt;row&gt;
			      &lt;column&gt;&lt;/column&gt;
			      &lt;column&gt;&lt;/column&gt;
			    &lt;/row&gt;
			    .
			    .
			    .
			    .
			&lt;/rows&gt;</description>
        <from>rows</from>
        <to>rows</to>
      </input>
      <input global="false" required="false">
        <description>Used to indicate the end of one field and the beginning of the next.

Default Value: ,</description>
        <from>delimiter</from>
        <to>delimiter</to>
      </input>
      <input global="false" required="false">
        <description>Used to differentiate field data from delimiter. Each field's data will be between the text qualifier. If, for example, field data contains a comma and a comma is used as a record delimiter, then text qualifier must be used to distinguish between the comma’s in data and the comma’s used to delimit fields.

Default Value: "</description>
        <from>text qualifier</from>
        <to>text qualifier</to>
      </input>
      <input global="false" required="false">
        <description>Specifies whether to preserve leading and trailing whitespace in this column of data.</description>
        <from>preserve spaces</from>
        <to>preserve spaces</to>
      </input>
      <input global="false" required="false">
        <description>Specifies the supporting charset. Character set that are identifiers describing a series of universal characters.</description>
        <from>charset</from>
        <to>charset</to>
      </input>
      <output global="false" required="false">
        <to>adapter response</to>
        <from>adapter response</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence />
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>adapter name</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[string-length(.)=0]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>adapter name</to>
                  <value>CSVAdapter</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
      <assign>
        <assignment>
          <document>
            <input>
              <from>rows</from>
            </input>
          </document>
          <result>
            <output>
              <to>adapter request</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="file name">
                <input global="false">
                  <from>file name</from>
                </input>
              </replacement-token>
              <replacement-token name="delimiter">
                <input global="false">
                  <from>delimiter</from>
                </input>
              </replacement-token>
              <replacement-token name="text qualifier">
                <input global="false">
                  <from>text qualifier</from>
                </input>
              </replacement-token>
              <replacement-token name="charset">
                <input global="false">
                  <from>charset</from>
                </input>
              </replacement-token>
              <replacement-token name="preserve spaces">
                <input global="false">
                  <from>preserve spaces</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <request-data>
      <csv-adapter-request>
        <operation-name>
          <xsl:text disable-output-escaping="no">write</xsl:text>
        </operation-name>
        <file-name>${file name}</file-name>
        <delimiter>
          <xsl:text disable-output-escaping="no">$[delimiter]</xsl:text>
        </delimiter>
        <text-qualifier>
          <xsl:text disable-output-escaping="no">$[text qualifier]</xsl:text>
        </text-qualifier>
        <preserve-spaces>${preserve spaces}</preserve-spaces>
        <charset>${charset}</charset>
        <xsl:copy-of select="//rows" />
      </csv-adapter-request>
    </request-data>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <from>adapter request</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:assign[1]</location>
      </assign>
      <call-adapter>
        <parameters>
          <input global="false">
            <from>adapter name</from>
          </input>
          <input>
            <value>
              <adapter-request>
                <peer-location>
                  <location>any</location>
                </peer-location>
              </adapter-request>
            </value>
          </input>
          <input global="false">
            <from>adapter request</from>
          </input>
          <input global="false">
            <from>adapter request</from>
          </input>
          <output global="false">
            <to>adapter response</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <from>adapter response</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:call-adapter[1]</location>
      </call-adapter>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
        <consumer>SOAP</consumer>
      </expose-to>
      <documentation>
        <description>Writes the data provided in the &lt;rows&gt; element to the specified file in Comma Separated Values (CSV) file format.</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>ed47dea7-8e1d-4651-94ae-538f52874156</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>admin</last-modified-by>
      <last-modified>1387443462385</last-modified>
    </meta>
  </process-def>
</process>