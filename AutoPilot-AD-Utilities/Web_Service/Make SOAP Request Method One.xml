            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:escapeUtils="org.apache.commons.lang.StringEscapeUtils" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <request-data>
      <soap-rpc-request>
        <use-ssl-certificate>${use ssl certificate}</use-ssl-certificate>
        <install-certificate>${install certificate}</install-certificate>
        <allow-unsigned-certificate>${allow unsigned certificate}</allow-unsigned-certificate>
        <xsl:if test="string-length('$[truststore file]')!=0">
          <truststore-file>${truststore file}</truststore-file>
        </xsl:if>
        <passphrase>
          <xsl:choose>
            <xsl:when test="string('$[is truststore passphrase secure]') = &quot;true&quot;">${pass phrase}</xsl:when>
            <xsl:otherwise>
              <xsl:choose>
                <xsl:when test="string('$[truststore passphrase not provided]') = &quot;false&quot;">
                  <xsl:text disable-output-escaping="no">$[pass phrase]</xsl:text>
                </xsl:when>
                <xsl:otherwise>
                  <xsl:text disable-output-escaping="no">changeit</xsl:text>
                </xsl:otherwise>
              </xsl:choose>
            </xsl:otherwise>
          </xsl:choose>
        </passphrase>
        <xsl:if test="string-length('$[keystore file]')!=0">
          <keystore-file>${keystore file}</keystore-file>
          <keystore-passphrase>
            <xsl:choose>
              <xsl:when test="string('${is keystore passphrase secure}') = &quot;true&quot;">${keystore pass phrase}</xsl:when>
              <xsl:otherwise>
                <xsl:choose>
                  <xsl:when test="string('$[keystore passphrase not provided]') = &quot;false&quot;">
                    <xsl:text disable-output-escaping="no">$[keystore pass phrase]</xsl:text>
                  </xsl:when>
                  <xsl:otherwise>
                    <xsl:text disable-output-escaping="no">changeit</xsl:text>
                  </xsl:otherwise>
                </xsl:choose>
              </xsl:otherwise>
            </xsl:choose>
          </keystore-passphrase>
          <client-alias>${client alias}</client-alias>
        </xsl:if>
        <xsl:choose>
          <xsl:when test="${properties length} &gt; 0">${connection properties}</xsl:when>
          <xsl:otherwise />
        </xsl:choose>
        <http-connection-properties>
          <xsl:choose>
            <xsl:when test="string-length('$[soap action]')!=0">
              <SoapAction>$[soap action]</SoapAction>
            </xsl:when>
            <xsl:otherwise>
              <SoapAction>
                <xsl:text disable-output-escaping="no">POST</xsl:text>
              </SoapAction>
            </xsl:otherwise>
          </xsl:choose>
          <xsl:if test="string-length('$[content type]')!=0">
            <Content-Type>$[content type]</Content-Type>
          </xsl:if>
        </http-connection-properties>
        <xsl:if test="string-length('$[username]')!=0">
          <username>$[username]</username>
          <password>
            <xsl:choose>
              <xsl:when test="string('$[is password secure]') = &quot;true&quot;">${password}</xsl:when>
              <xsl:otherwise>$[password]</xsl:otherwise>
            </xsl:choose>
          </password>
          <xsl:if test="string-length('${ntlmauthentication}')!=0">
            <ntlm-authentication>${ntlmauthentication}</ntlm-authentication>
          </xsl:if>
        </xsl:if>
        <xsl:choose>
          <xsl:when test="string-length('${transport}')!=0">
            <transport>${transport}</transport>
          </xsl:when>
          <xsl:otherwise>
            <transport>
              <xsl:text>http-rpc</xsl:text>
            </transport>
          </xsl:otherwise>
        </xsl:choose>
        <soap-url>$[soap url]</soap-url>
        <character-set>${character-set}</character-set>
        <download-attachments>
          <xsl:choose>
            <xsl:when test="string-length('$[download attachments]')!=0">${download attachments}</xsl:when>
            <xsl:otherwise>
