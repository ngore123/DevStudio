<process draft="false" dev-studio-version="8.2.00">
  <process-def name=":AutoPilot-AD-Utilities:Common:Remove Braces from JSON">
    <parameters>
      <input global="false" required="true">
        <from>input json</from>
        <to>input json</to>
      </input>
      <input global="false" required="true">
        <from>initial index</from>
        <to>initial index</to>
      </input>
      <output global="false" required="false">
        <to>output json</to>
        <from>output json</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>input json</from>
            </input>
          </document>
          <result>
            <output>
              <to>input json</to>
            </output>
          </result>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[normalize-space(.)]]></xpath>
          </xpath-expression>
        </assignment>
        <assignment>
          <document>
            <input>
              <from>input json</from>
            </input>
          </document>
          <result>
            <output>
              <to>input json length</to>
            </output>
          </result>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[string-length(.)]]></xpath>
          </xpath-expression>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens />
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:java="java" version="1.0"><xsl:output indent="no" /><xsl:template match="/"><xsl:variable name="formatterIn"  select="java:text.NumberFormat.getInstance(  )"/><xsl:variable name="numericIn"  select="java:parse($formatterIn,string(.))"/><xsl:variable name="formatterOut"  select="java:text.DecimalFormat.new(  )"/><xsl:variable name="patternOut"  select='"#.##########"'/><xsl:variable name="emptyOut"  select="java:applyPattern($formatterOut,$patternOut)"/><xsl:variable name="numericOut"  select="java:format($formatterOut,$numericIn)"/><value><xsl:value-of select="$numericOut"/></value></xsl:template> </xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <assignment>
          <document>
            <input>
              <from>input json length</from>
            </input>
          </document>
          <result>
            <output>
              <to>last index</to>
            </output>
          </result>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[number(.) - 1]]></xpath>
          </xpath-expression>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[substring-before(., ".")]]></xpath>
          </xpath-expression>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <utility>
        <string>
          <substring>
            <input>
              <from>input json</from>
            </input>
            <start-index>
              <input>
                <from>initial index</from>
              </input>
            </start-index>
            <end-index>
              <input>
                <from>last index</from>
              </input>
            </end-index>
          </substring>
        </string>
        <output global="false">
          <to>output json</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:utility[1]</location>
      </utility>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>9d303cf7-5467-4212-8103-742166e35266</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
