            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <xsl:param name="wsdl-content">$[wsdl content]</xsl:param>
    <request-data>
      <soap-rpc-request>
        <soap-url>$[soap url]</soap-url>
        <xsl:if test="string-length('${connection name}')!=0">
          <connection>
            <name>$[connection name]</name>
            <xsl:if test="string-length('${terminate on exit}')!=0">
              <terminate-on-exit>${terminate on exit}</terminate-on-exit>
            </xsl:if>
          </connection>
        </xsl:if>
        <xsl:if test="string-length('${username}')!=0">
          <username>${username}</username>
          <xsl:if test="string-length('${password}')!=0">
            <password>$[password]</password>
          </xsl:if>
        </xsl:if>
        <wsdl>
          <xsl:choose>
            <xsl:when test="string-length($wsdl-content)!=0">
              <content>${wsdl content}</content>
            </xsl:when>
            <xsl:otherwise>
              <reference>${wsdl url}</reference>
            </xsl:otherwise>
          </xsl:choose>
        </wsdl>
        <xsl:if test="string-length('$[generator version]')!=0">
          <xsl:choose>
            <xsl:when test="${generator version}= 1.0">
              <generator-version>${generator version}</generator-version>
            </xsl:when>
            <xsl:otherwise>
              <generator-version>
                <xsl:text disable-output-escaping="no">1.0</xsl:text>
              </generator-version>
            </xsl:otherwise>
          </xsl:choose>
        </xsl:if>
        <request>
          <service>
            <xsl:if test="string-length('${request service namespace}')!=0">
              <namespace>$[request service namespace]</namespace>
            </xsl:if>
            <xsl:if test="string-length('${request service name}')!=0">
              <name>${request service name}</name>
            </xsl:if>
          </service>
          <port>
