            </xsl:if>
          </port>
          <xsl:if test="string-length('${request operation name}')!=0">
            <operation-name>${request operation name}</operation-name>
          </xsl:if>
          <xsl:if test="string-length('$[request parameters]')=0">
            <parameters />
          </xsl:if>
          ${request parameters}
        </request>
      </soap-rpc-request>
    </request-data>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
