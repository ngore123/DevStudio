            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <export-info>
      <xsl:for-each select="//dataset-member-info">
        <xsl:if test="contains(concat(string(./member-name), &quot; &quot;), concat(&quot;/&quot;, concat(string('${member}'), &quot; &quot;)))">
          <xsl:if test="string(./nfs-export-name)  != ''">
            <nfs-export-name>
              <xsl:value-of select="string(./nfs-export-name)" disable-output-escaping="no" />
            </nfs-export-name>
          </xsl:if>
          <cifs-share>
            <xsl:value-of select="string(//cifs-share-names/cifs-share-name)" disable-output-escaping="no" />
          </cifs-share>
        </xsl:if>
      </xsl:for-each>
    </export-info>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
