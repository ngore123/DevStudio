<process draft="false" dev-studio-version="8.0.01">
  <process-def name=":AutoPilot-OA-ITSM_Automation:Utilities:Merge Change XMLs">
    <parameters>
      <input global="false" required="true">
        <from>ChangesOutboundXML</from>
        <to>ChangesOutboundXML</to>
      </input>
      <input global="false" required="true">
        <from>ChangesTransactionalXML</from>
        <to>ChangesTransactionalXML</to>
      </input>
      <input global="false" required="false">
        <from>Changes</from>
        <to>Changes</to>
      </input>
      <output global="false" required="false">
        <to>Changes</to>
        <from>Changes</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <for-each>
        <document>
          <input>
            <from>ChangesOutboundXML</from>
          </input>
        </document>
        <result>
          <output>
            <to>change</to>
          </output>
        </result>
        <xpath-expression>
          <xpath-replacement-tokens />
          <xpath><![CDATA[//change]]></xpath>
        </xpath-expression>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <sequence>
          <assign>
            <assignment>
              <document>
                <input>
                  <from>change</from>
                </input>
              </document>
              <result>
                <output>
                  <to>ChangeID</to>
                </output>
              </result>
              <xpath-expression>
                <xpath-replacement-tokens />
                <xpath><![CDATA[//change-id/text()]]></xpath>
              </xpath-expression>
            </assignment>
            <assignment>
              <document>
                <input>
                  <from>ChangesTransactionalXML</from>
                </input>
              </document>
              <result>
                <output>
                  <to>WorkLog</to>
                </output>
              </result>
              <xpath-expression>
                <xpath-replacement-tokens>
                  <replacement-token name="changeID">
                    <input global="false">
                      <from>ChangeID</from>
                    </input>
                  </replacement-token>
                </xpath-replacement-tokens>
                <xpath><![CDATA[/changes/change[contains(field-value,'Failed to create associations') and change-id='${changeID}' ]/field-value/text()]]></xpath>
              </xpath-expression>
            </assignment>
            <assignment>
              <document>
                <input>
                  <from>WorkLog</from>
                </input>
              </document>
              <result>
                <output>
                  <to>WorkLogList</to>
                </output>
              </result>
              <xpath-expression>
                <xpath-replacement-tokens />
                <xpath><![CDATA[substring-after(., "CIs not found in the CMDB.")]]></xpath>
              </xpath-expression>
            </assignment>
            <assignment>
              <document>
                <input>
                  <from>WorkLogList</from>
                </input>
              </document>
              <result>
                <output>
                  <to>WorkLog</to>
                </output>
              </result>
              <xpath-expression>
                <xpath-replacement-tokens />
                <xpath><![CDATA[normalize-space(.)]]></xpath>
              </xpath-expression>
            </assignment>
            <meta>
              <documentation />
              <checkpoint>false</checkpoint>
            </meta>
            <before-execution>
              <log>
                <level>debug</level>
                <parameters>
                  <input>
                    <value>worklog</value>
                  </input>
                  <input>
                    <from>WorkLog</from>
                  </input>
                </parameters>
              </log>
            </before-execution>
            <after-execution>
              <log>
                <level>debug</level>
                <parameters>
                  <input>
                    <value>Extracted worklog</value>
                  </input>
                  <input>
                    <from>ChangeID</from>
                  </input>
                  <input>
                    <from>WorkLog</from>
                  </input>
                </parameters>
              </log>
            </after-execution>
            <location>:assign[1]</location>
          </assign>
          <utility>
            <string>
              <regex>
                <input>
                  <from>WorkLog</from>
                </input>
                <expression>
                  <search case-sensitive="false">\s</search>
                  <replace replace-all="true">;</replace>
                </expression>
              </regex>
            </string>
            <output global="false">
              <to>WorkLog</to>
            </output>
            <meta>
              <documentation />
              <checkpoint>false</checkpoint>
            </meta>
            <location>:utility[1]</location>
          </utility>
          <assign>
            <assignment>
              <document>
                <input>
                  <from>change</from>
                </input>
              </document>
              <result>
                <output>
                  <to>change</to>
                </output>
              </result>
              <xslt-stylesheet>
                <stylesheet-replacement-tokens>
                  <replacement-token name="worklog">
                    <input global="false">
                      <from>WorkLog</from>
                    </input>
                  </replacement-token>
                </stylesheet-replacement-tokens>
                <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="*">
    <xsl:copy>
      <xsl:copy-of select="@*|node()" />
    </xsl:copy>
  </xsl:template>
  <xsl:template match="change">
    <xsl:copy>
      <xsl:copy-of select="@*|node()" />
      <failed-ci-list>${worklog}</failed-ci-list>
    </xsl:copy>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
              </xslt-stylesheet>
            </assignment>
            <assignment>
              <document>
                <input>
                  <from>Changes</from>
                </input>
              </document>
              <result>
                <output>
                  <to>Changes</to>
                </output>
              </result>
              <xslt-stylesheet>
                <stylesheet-replacement-tokens>
                  <replacement-token name="newChange">
                    <input global="false">
                      <from>change</from>
                    </input>
                  </replacement-token>
                </stylesheet-replacement-tokens>
                <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <changes>
      <xsl:copy-of select="node()/*" />
      ${newChange}
    </changes>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
              </xslt-stylesheet>
            </assignment>
            <meta>
              <documentation />
              <checkpoint>false</checkpoint>
            </meta>
            <after-execution>
              <log>
                <level>debug</level>
                <parameters>
                  <input>
                    <value>Extracted worklog</value>
                  </input>
                  <input>
                    <from>ChangeID</from>
                  </input>
                  <input>
                    <from>WorkLog</from>
                  </input>
                </parameters>
              </log>
            </after-execution>
            <location>:assign[2]</location>
          </assign>
        </sequence>
        <location>:for-each[1]</location>
      </for-each>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description></description>
        <version></version>
      </documentation>
      <logging-options>
        <is-enabled>false</is-enabled>
        <log-file-name></log-file-name>
      </logging-options>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>573c0981-8dfb-47f7-b8ed-1686a6dc3236</process-id>
      <author>admin</author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by>aoadmin</last-modified-by>
      <last-modified>1526291412669</last-modified>
    </meta>
  </process-def>
</process>