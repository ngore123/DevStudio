            <value>http-rpc</value>
            <to>transport</to>
          </input>
          <input>
            <from>soapurl</from>
            <to>soap url</to>
          </input>
          <input>
            <from>soap request</from>
            <to>soap message</to>
          </input>
          <input>
            <from>UserName</from>
            <to>username</to>
          </input>
          <input>
            <from>Password</from>
            <to>password</to>
          </input>
          <input>
            <value>true</value>
            <to>use ssl certificate</to>
          </input>
          <input>
            <value>true</value>
            <to>install certificate</to>
          </input>
