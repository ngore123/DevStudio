<?xml version="1.0" encoding="UTF-8"?>
<process>
   <process-def name=":BMC-SA-SDA_Fulfillment_Cases:rules">
      <sequence>
         <switch>
            <meta>
               <peer-location>
                  <location>any</location>
               </peer-location>
            </meta>
            <execute-all-applicable-cases>true</execute-all-applicable-cases>
            <case name="Extend Mailbox Size" applies-if-any-condition-true="false">
               <condition>
                  <document>
                     <input>
                        <from>inputevent</from>
                     </input>
                  </document>
                  <xpath-expression>
                     <xpath><![CDATA[normalize-space(substring-before(substring-after(//text/text(), "ActionType="), ","))="Extend Mailbox Size"='true']]></xpath>
                  </xpath-expression>
                  <gui-data>
                     <from><![CDATA[normalize-space(substring-before(substring-after(//text/text(), "ActionType="), ","))="Extend Mailbox Size"]]></from>
                     <fromKey><![CDATA[8694]]></fromKey>
                     <to><![CDATA['true']]></to>
                     <operator><![CDATA[=]]></operator>
                     <type><![CDATA[String]]></type>
                  </gui-data>
               </condition>
               <sequence>
                  <schedule-job>
                     <parameters>
                        <input required="false">
                           <from>inputevent</from>
                           <to>inputevent</to>
                        </input>
                     </parameters>
                     <group>BMC-SA-SDA_Fulfillment_Cases</group>
                     <name-prefix>Extend Mailbox Size</name-prefix>
                     <process-name>:BMC-SA-SDA_Fulfillment_Cases:Fulfillment:Mailbox:Extend:Process Extend Mailbox Event</process-name>
                  </schedule-job>
               </sequence>
            </case>
            <case name="Procecc new employee" applies-if-any-condition-true="false">
               <condition>
                  <document>
                     <input>
                        <from>inputevent</from>
                     </input>
                  </document>
                  <xpath-expression>
                     <xpath><![CDATA[normalize-space(substring-before(substring-after(//text/text(), "ActionType="), ","))="Onboarding New Employee"='true']]></xpath>
                  </xpath-expression>
                  <gui-data>
                     <from><![CDATA[normalize-space(substring-before(substring-after(//text/text(), "ActionType="), ","))="Onboarding New Employee"]]></from>
                     <fromKey><![CDATA[69]]></fromKey>
                     <to><![CDATA['true']]></to>
                     <operator><![CDATA[=]]></operator>
                     <type><![CDATA[String]]></type>
                  </gui-data>
               </condition>
               <sequence>
                  <schedule-job>
                     <parameters>
                        <input required="false">
                           <from>inputevent</from>
                           <to>inputevent</to>
                        </input>
                     </parameters>
                     <group>BMC-SA-SDA_Fulfillment_Cases</group>
                     <name-prefix>Procecc new employee</name-prefix>
                     <process-name>:BMC-SA-SDA_Fulfillment_Cases:Fulfillment:Onboarding:Process User Onboarding Event</process-name>
                  </schedule-job>
               </sequence>
            </case>
            <case name="Process Offboard employee" applies-if-any-condition-true="false">
               <condition>
                  <document>
                     <input>
                        <from>inputevent</from>
                     </input>
                  </document>
                  <xpath-expression>
                     <xpath><![CDATA[normalize-space(substring-before(substring-after(//text/text(), "ActionType="), ","))="Offboard Employee"='true']]></xpath>
                  </xpath-expression>
                  <gui-data>
                     <from><![CDATA[normalize-space(substring-before(substring-after(//text/text(), "ActionType="), ","))="Offboard Employee"]]></from>
                     <fromKey><![CDATA[8696]]></fromKey>
                     <to><![CDATA['true']]></to>
                     <operator><![CDATA[=]]></operator>
                     <type><![CDATA[String]]></type>
                  </gui-data>
               </condition>
               <sequence>
                  <schedule-job>
                     <parameters>
                        <input required="false">
                           <from>inputevent</from>
                           <to>inputevent</to>
                        </input>
                     </parameters>
                     <group>BMC-SA-SDA_Fulfillment_Cases</group>
                     <name-prefix>Process Offboard employee</name-prefix>
                     <process-name>:BMC-SA-SDA_Fulfillment_Cases:Fulfillment:Offboarding:Process User Offboarding Event</process-name>
                  </schedule-job>
               </sequence>
            </case>
            <case name="Process Unlock Account Event" applies-if-any-condition-true="false">
               <condition>
                  <document>
                     <input>
                        <from>inputevent</from>
                     </input>
                  </document>
                  <xpath-expression>
                     <xpath><![CDATA[normalize-space(substring-before(substring-after(//text/text(), "ActionType="), ","))="Unlock Account"='true']]></xpath>
                  </xpath-expression>
                  <gui-data>
                     <from><![CDATA[normalize-space(substring-before(substring-after(//text/text(), "ActionType="), ","))="Unlock Account"]]></from>
                     <fromKey><![CDATA[8636]]></fromKey>
                     <to><![CDATA['true']]></to>
                     <operator><![CDATA[=]]></operator>
                     <type><![CDATA[String]]></type>
                  </gui-data>
               </condition>
               <sequence>
                  <schedule-job>
                     <parameters>
                        <input required="false">
                           <from>inputevent</from>
                           <to>inputevent</to>
                        </input>
                     </parameters>
                     <group>BMC-SA-SDA_Fulfillment_Cases</group>
                     <name-prefix>Process Unlock Account Event</name-prefix>
                     <process-name>:BMC-SA-SDA_Fulfillment_Cases:Fulfillment:Account:Unlock:Process Unlock Event</process-name>
                  </schedule-job>
               </sequence>
            </case>
         </switch>
      </sequence>
      <meta>
         <peer-location>
            <location>any</location>
         </peer-location>
      </meta>
      <parameters>
         <input>
            <from>inputevent</from>
            <to>inputevent</to>
         </input>
      </parameters>
   </process-def>
</process>

