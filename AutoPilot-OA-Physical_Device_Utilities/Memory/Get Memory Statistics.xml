              <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <fat-command>
      <command os-id="Linux">
        <xsl:text>free -l</xsl:text>
        <xsl:if test="string-length('${units}')!=0">
          <xsl:choose>
            <xsl:when test="'${units}' = 'bytes'">
              <xsl:text> -b</xsl:text>
            </xsl:when>
            <xsl:when test="'${units}' = 'kilobytes'">
              <xsl:text> -k</xsl:text>
            </xsl:when>
            <xsl:when test="'${units}' = 'megabytes'">
              <xsl:text> -m</xsl:text>
            </xsl:when>
            <xsl:when test="'${units}' = 'gigabytes'">
              <xsl:text> -g</xsl:text>
            </xsl:when>
            <xsl:otherwise>
              <xsl:text> -b</xsl:text>
            </xsl:otherwise>
          </xsl:choose>
        </xsl:if>
        <xsl:if test="string-length('${delay}')!=0">
          <xsl:if test="string-length('${count}')!=0">
            <xsl:text> -s ${delay} -c ${count}</xsl:text>
          </xsl:if>
        </xsl:if>
      </command>
      <command os-id="SunOS">
        <xsl:text disable-output-escaping="no">/usr/bin/vmstat</xsl:text>
        <xsl:if test="string-length('${delay}')!=0">
          <xsl:text disable-output-escaping="no"> $[delay]</xsl:text>
        </xsl:if>
        <xsl:if test="string-length('${count}')!=0">
          <xsl:text disable-output-escaping="no"> $[count]</xsl:text>
        </xsl:if>
      </command>
      <command os-id="Windows Default">
        <xsl:text disable-output-escaping="no">systeminfo | find "Memory"</xsl:text>
      </command>
      <command os-id="Cisco IOS">
        <xsl:text disable-output-escaping="no">show memory summary</xsl:text>
      </command>
    </fat-command>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
