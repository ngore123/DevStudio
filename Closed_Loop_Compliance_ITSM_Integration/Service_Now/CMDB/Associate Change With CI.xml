<process draft="false" dev-studio-version="7.6.03">
  <process-def name=":Closed_Loop_Compliance_ITSM_Integration:Service_Now:CMDB:Associate Change With CI">
    <parameters>
      <input global="false" required="true">
        <description>List of configuration items to associate with change.

Sample XML:

&lt;Configuration-Items&gt;
      &lt;Configuration-Item&gt;
         &lt;name&gt;configuration-item-name&lt;/name&gt;
       &lt;/Configuration-Item&gt;
       &lt;Configuration-Item&gt;
         &lt;name&gt;configuration-item-name&lt;/name&gt;
       &lt;/Configuration-Item&gt;
&lt;/Configuration-Items&gt;</description>
        <from>configuration items</from>
        <to>configuration items</to>
      </input>
      <input global="false" required="true">
        <description>Change id to which configuration items will be associated</description>
        <from>change id</from>
        <to>change id</to>
      </input>
      <output global="false" required="false">
        <to>change success status</to>
        <from>change success status</from>
      </output>
      <output global="false" required="false">
        <to>change status message</to>
        <from>change status message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <output>
            <to>itsm platform</to>
            <value>ServiceNow</value>
          </output>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <assign>
        <assignment>
          <document>
            <input>
              <from>configuration items</from>
            </input>
          </document>
          <result>
            <output>
              <to>associated change</to>
              <concept type="AutoPilot-OA-Change_Management:Change">
                <attribute>configuration-item</attribute>
              </concept>
            </output>
          </result>
          <xpath-expression>
            <xpath-replacement-tokens />
            <xpath><![CDATA[//name/text()]]></xpath>
          </xpath-expression>
        </assignment>
        <assignment>
          <input>
            <from>change id</from>
          </input>
          <output>
            <to>associated change</to>
            <concept type="AutoPilot-OA-Change_Management:Change">
              <attribute>change-id</attribute>
            </concept>
          </output>
        </assignment>
        <assignment>
          <document>
            <input>
              <from>associated change</from>
            </input>
          </document>
          <result>
            <output>
              <to>associated changes</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="change">
                <input global="false">
                  <from>associated change</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" cdata-section-elements="location" />
  <xsl:template match="/">
    <Changes>
      <xsl:choose>
        <xsl:when test="string-length(//change) &gt; 0">
          <xsl:copy-of select="//change" />
        </xsl:when>
        <xsl:otherwise>${change}</xsl:otherwise>
      </xsl:choose>
    </Changes>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[2]</location>
      </assign>
      <call-process>
        <process-name>:AutoPilot-OA-Change_Management:Create Change Association</process-name>
        <parameters>
          <input>
            <from>itsm platform</from>
            <to>configuration name</to>
          </input>
          <input>
            <from>associated changes</from>
            <to>changes</to>
          </input>
          <output>
            <from>changes</from>
            <to>changes</to>
          </output>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>status message</from>
            <to>status message</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <before-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <from>associated changes</from>
              </input>
            </parameters>
          </log>
        </before-execution>
        <location>:call-process[1]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
