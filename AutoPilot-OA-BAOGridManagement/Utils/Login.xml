            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xalan="http://xml.apache.org/xalan" version="1.0">
  <xsl:output indent="no" />
  <xsl:variable name="tokenInput">${config password}</xsl:variable>
  <xsl:variable name="tokenXml" select="xalan:nodeset($tokenInput)" />
  <xsl:template match="/">
    <xsl:choose>
      <xsl:when test="boolean(//*[name() = 'EncryptedData']) or string-length(normalize-space(.)) &gt; 0">
        <xsl:apply-templates select="." mode="context" />
      </xsl:when>
      <xsl:otherwise>
        <xsl:apply-templates select="$tokenXml" mode="context" />
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>
  <xsl:template match="@*|node()" mode="context">
    <xsl:choose>
      <xsl:when test="boolean(//*[name() = 'EncryptedData'])">
        <xsl:copy-of select="//*[name() = 'EncryptedData']" />
      </xsl:when>
      <xsl:otherwise>
        <value>
          <xsl:value-of select="." disable-output-escaping="no" />
        </value>
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>
  <xsl:template match="@*|node()" mode="token">
    <xsl:copy-of select="//*[name() = 'EncryptedData']" />
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
