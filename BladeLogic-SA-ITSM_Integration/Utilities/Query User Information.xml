<process draft="false" dev-studio-version="7.6.01.00">
  <process-def name=":BladeLogic-SA-ITSM_Integration:Utilities:Query User Information">
    <parameters>
      <input global="false" required="true">
        <description>configuration name

Name of the module configuration group for which the user is to be found.

Allowed values: BLADELOGIC</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>username

username who's information is to be retrieved from ITSM.</description>
        <from>username</from>
        <to>username</to>
      </input>
      <output global="false" required="false">
        <description>String Value - Indicates whether or not the process completed successfully.  

Valid values: 0-success,1-failure 
</description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="false">
        <description>String Value â€“ A verbose message indicating the success or failure of the process</description>
        <to>status message</to>
        <from>status message</from>
      </output>
      <output global="false" required="false">
        <to>login ID</to>
        <from>login ID</from>
      </output>
      <output global="false" required="false">
        <description>String Value - First name 

Example: adapter</description>
        <to>first name</to>
        <from>first name</from>
      </output>
      <output global="false" required="false">
        <description>String Value - Last name  

Example: user</description>
        <to>last name</to>
        <from>last name</from>
      </output>
      <output global="false" required="false">
        <description>String Value - Company name 

Example: Calbro Financial Services</description>
        <to>company</to>
        <from>company</from>
      </output>
      <output global="false" required="false">
        <description>String Value - Company location 

Example: Calbro Financial Services</description>
        <to>company location</to>
        <from>company location</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:BladeLogic-SA-ITSM_Integration:Utilities:Extract Login ID Details</process-name>
        <parameters>
          <input>
            <from>configuration name</from>
            <to>configuration name</to>
          </input>
          <input>
            <from>username</from>
            <to>login id</to>
          </input>
          <output>
            <from>success status</from>
            <to>success status</to>
          </output>
          <output>
            <from>status message</from>
            <to>status message</to>
          </output>
          <output>
            <from>login id</from>
            <to>login id</to>
          </output>
          <output>
            <from>first name</from>
            <to>first name</to>
          </output>
          <output>
            <from>last name</from>
            <to>last name</to>
          </output>
          <output>
            <from>company</from>
            <to>company</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <console-output>
            <type>INFO</type>
            <summary>
              <concat>
                <input>
                  <value></value>
                </input>
              </concat>
            </summary>
            <detail>
              <concat>
                <input>
                  <value></value>
                </input>
              </concat>
            </detail>
          </console-output>
        </after-execution>
        <location>:call-process[1]</location>
      </call-process>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <input>
                  <from>company</from>
                </input>
                <output>
                  <to>company location</to>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>success</value>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>status message</to>
                  <value>username  found.</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[2]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>success status</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[text() = 'failure']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>failure</value>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>status message</to>
                  <value>Authentication failure: invalid username.</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This process will retrieve detail for a specified user id.</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>dae30e36-dc86-3915-a72a-972cfddbcea4</process-id>
      <author></author>
      <category></category>
      <status></status>
      <keywords></keywords>
      <systems></systems>
      <last-modified-by></last-modified-by>
