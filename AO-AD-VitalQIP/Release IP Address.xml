<process draft="false" dev-studio-version="7.6.01.04">
  <process-def name=":AO-AD-VitalQIP:Release IP Address">
    <parameters>
      <input global="false" required="false">
        <description>The name of the Adapter to use for this Process. </description>
        <from>adapter name</from>
        <to>adapter name</to>
      </input>
      <input global="false" required="true">
        <description>The user name name that will be used to execute the command on QIP.</description>
        <from>user name</from>
        <to>user name</to>
      </input>
      <input global="false" required="true">
        <description>The password for the user name that will be used to execute the command on the QIP.</description>
        <from>password</from>
        <to>password</to>
      </input>
      <input global="false" required="true">
        <description>The QIP organization name that will be used while executing the command on the QIP.</description>
        <from>organization</from>
        <to>organization</to>
      </input>
      <input global="false" required="true">
        <description>Ip address to be released.</description>
        <from>ip-address</from>
        <to>ip-address</to>
      </input>
      <input global="false" required="false">
        <description>Scope can have twos possible values
1. Any string : It is a organization name same as pod name. 
2. null : in this case we will use default organization.</description>
        <from>scope</from>
        <to>scope</to>
      </input>
      <output global="false" required="false">
        <description>incase of any error, error message goes here.  </description>
        <to>error message</to>
        <from>error message</from>
      </output>
      <output global="false" required="false">
        <description>command status.
valid values: true, false.</description>
        <to>success</to>
        <from>Success</from>
      </output>
      <output global="false" required="false">
        <description>Command output as it is. </description>
        <to>output</to>
        <from>output</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>user name</from>
            </input>
          </document>
          <result>
            <output>
              <to>Adapter request</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="user">
                <input global="false">
                  <from>user name</from>
                </input>
              </replacement-token>
              <replacement-token name="pass">
                <input global="false">
                  <from>password</from>
                </input>
              </replacement-token>
              <replacement-token name="org">
                <input global="false">
                  <from>organization</from>
                </input>
              </replacement-token>
              <replacement-token name="address">
                <input global="false">
                  <from>ip-address</from>
                </input>
              </replacement-token>
              <replacement-token name="scope">
                <input global="false">
                  <from>scope</from>
                </input>
              </replacement-token>
            </stylesheet-replacement-tokens>
            <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <request-data>
      <command-request>
        <xsl:if test="string-length(&quot;${scope}&quot;) &gt; 0">
          <commands>
            <command>
              <xsl:text disable-output-escaping="no">qip-ungetipaddr -u "${user}" -p "${pass}" -o "${scope}" -a ${address}</xsl:text>
            </command>
          </commands>
        </xsl:if>
        <xsl:if test="string-length(&quot;${scope}&quot;) = 0">
          <commands>
            <command>
              <xsl:text disable-output-escaping="no">qip-ungetipaddr -u "${user}" -p "${pass}" -o "${org}" -a ${address}</xsl:text>
            </command>
          </commands>
        </xsl:if>
      </command-request>
    </request-data>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
          </xslt-stylesheet>
        </assignment>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:assign[1]</location>
      </assign>
      <call-adapter>
        <parameters>
          <input global="false">
            <from>adapter name</from>
          </input>
          <input>
            <value>
              <adapter-request>
                <peer-location>
                  <location>any</location>
                </peer-location>
              </adapter-request>
            </value>
          </input>
          <input global="false">
            <from>Adapter request</from>
          </input>
          <input global="false">
            <from>Adapter request</from>
          </input>
          <output global="false">
            <to>Adapter Response</to>
          </output>
        </parameters>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <location>:call-adapter[1]</location>
      </call-adapter>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>OCP</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>Adapter Response</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>output</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//output]]></xpath>
                </xpath-expression>
              </assignment>
              <assignment>
                <output>
                  <to>Success</to>
                  <value>true</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case applies-if-any-condition-true="true">
          <condition>
            <document>
              <input>
                <from>Adapter Response</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[boolean(//error/text())]]></xpath>
            </xpath-expression>
          </condition>
          <condition>
            <document>
              <input>
                <from>Adapter Response</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[lower-case(string(//command-output/metadata/status)) = "error"]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <console-output>
              <type>INFO</type>
              <summary>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </summary>
              <detail>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </detail>
            </console-output>
            <assign>
              <assignment>
                <output>
                  <to>Success</to>
                  <value>false</value>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>Adapter Response</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>error message</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//line/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Release IP Address:
Release the ip address which was selected (acquired) previously.</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
      <process-id>7ccbd8cb-0361-4241-8d1c-6cd749a637a7</process-id>
