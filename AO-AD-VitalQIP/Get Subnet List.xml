          <condition>
            <document>
              <input>
                <from>Adapter Response</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[lower-case(string(//command-output/metadata/status)) = "error"]]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <console-output>
              <type>INFO</type>
              <summary>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </summary>
              <detail>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </detail>
            </console-output>
            <assign>
              <assignment>
                <output>
                  <to>Success</to>
                  <value>false</value>
                </output>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>Adapter Response</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>error message</to>
                  </output>
                </result>
                <xpath-expression>
                  <xpath-replacement-tokens />
                  <xpath><![CDATA[//line/text()]]></xpath>
                </xpath-expression>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[2]:assign[1]</location>
            </assign>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Get Subnet List:
Retrieves a list of all subnets the specified domain ([name]) manages from the VitalQIP database.</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
