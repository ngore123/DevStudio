        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <document>
                  <input>
                    <from>Adapter Response</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>pools</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens />
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="yes" />
  <xsl:template match="/">
    <start>
      <xsl:for-each select="//line">
        <data>
          <xsl:call-template name="tokenizer">
            <xsl:with-param name="string" select="string(.)" />
            <xsl:with-param name="delimiter" select="'&quot;'" />
          </xsl:call-template>
        </data>
      </xsl:for-each>
    </start>
  </xsl:template>
  <xsl:template name="tokenizer">
    <xsl:param name="string" />
    <xsl:param name="delimiter" select="' '" />
    <xsl:choose>
      <xsl:when test="$delimiter and contains($string, $delimiter)">
        <xyz>
          <xsl:value-of select="substring-before($string,$delimiter)" disable-output-escaping="no" />
        </xyz>
        <xsl:call-template name="tokenizer">
          <xsl:with-param name="string" select="substring-after($string,$delimiter)" />
          <xsl:with-param name="delimiter" select="$delimiter" />
        </xsl:call-template>
      </xsl:when>
      <xsl:otherwise>
        <xyz>
          <xsl:value-of select="$string" disable-output-escaping="no" />
        </xyz>
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
              </assignment>
              <assignment>
                <document>
                  <input>
                    <from>pools</from>
                  </input>
                </document>
                <result>
                  <output>
                    <to>subnet-list</to>
                  </output>
                </result>
                <xslt-stylesheet>
                  <stylesheet-replacement-tokens />
                  <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <pools>
      <xsl:for-each select="//data">
        <pool>
          <pool-id>
            <xsl:value-of select=".//xyz[ position() =4]" disable-output-escaping="no" />
          </pool-id>
          <pool-address>
            <xsl:value-of select=".//xyz[ position() =4]" disable-output-escaping="no" />
          </pool-address>
          <subnet-mask>
            <xsl:value-of select=".//xyz[ position() = 16]" disable-output-escaping="no" />
          </subnet-mask>
          <reserved>
            <xsl:text disable-output-escaping="no">false</xsl:text>
          </reserved>
        </pool>
      </xsl:for-each>
    </pools>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
                </xslt-stylesheet>
              </assignment>
              <assignment>
                <output>
                  <to>Success</to>
                  <value>true</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
