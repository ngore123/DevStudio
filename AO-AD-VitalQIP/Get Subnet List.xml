<process draft="false" dev-studio-version="7.6.01.04">
  <process-def name=":AO-AD-VitalQIP:Get Subnet List">
    <parameters>
      <input global="false" required="false">
        <description>The name of the Adapter to use for this Process. </description>
        <from>adapter name</from>
        <to>adapter name</to>
      </input>
      <input global="false" required="true">
        <description>The user name name that will be used to execute the command on QIP.</description>
        <from>user name</from>
        <to>user name</to>
      </input>
      <input global="false" required="true">
        <description>The password for the user name that will be used to execute the command on the QIP.</description>
        <from>password</from>
        <to>password</to>
      </input>
      <input global="false" required="true">
        <description>The QIP organization name that will be used while executing the command on the QIP.</description>
        <from>organization</from>
        <to>organization</to>
      </input>
      <input global="false" required="true">
        <description>Valid values are:
Subnetorg
Corp
Domain
OSPF
Network

</description>
        <from>owner type</from>
        <to>owner type</to>
      </input>
      <input global="false" required="true">
        <description>Corresponding value of owner type</description>
        <from>onwer value</from>
        <to>onwer value</to>
      </input>
      <input global="false" required="false">
        <description>Scope can have twos possible values
1. Any string : It is a organization name same as pod name. 
2. null : in this case we will use default organization.</description>
        <from>scope</from>
        <to>scope</to>
      </input>
      <output global="false" required="false">
        <description>incase of any error, error message goes here.  </description>
        <to>error message</to>
        <from>error message</from>
      </output>
      <output global="false" required="false">
        <description>command status.
valid values: true, false.</description>
        <to>success</to>
        <from>Success</from>
      </output>
      <output global="false" required="false">
        <description>List of all the availabe subnet from the QIP.
Format must be as below...
&lt;pools&gt;
   &lt;pool&gt;
        &lt;network-address&gt;a.b.c.d&lt;/network-address&gt;
        &lt;subnet-address&gt;l.m.n.o&lt;/subnet-address&gt;
        &lt;subnet-mask&gt;p.q.r.s&lt;/subnet-mask&gt;
   &lt;/pool&gt;
   &lt;pool&gt;
        &lt;network-address&gt;w.x.y.z&lt;/network-address&gt;
        &lt;subnet-address&gt;s.t.u.v&lt;/subnet-address&gt;
        &lt;subnet-mask&gt;p.q.r.s&lt;/subnet-mask&gt;
   &lt;/pool&gt;
&lt;/pools&gt;
</description>
        <to>subnet-list</to>
        <from>subnet-list</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <assign>
        <assignment>
          <document>
            <input>
              <from>user name</from>
            </input>
          </document>
          <result>
            <output>
              <to>Adapter request</to>
            </output>
          </result>
          <xslt-stylesheet>
            <stylesheet-replacement-tokens>
              <replacement-token name="user">
                <input global="false">
                  <from>user name</from>
                </input>
              </replacement-token>
