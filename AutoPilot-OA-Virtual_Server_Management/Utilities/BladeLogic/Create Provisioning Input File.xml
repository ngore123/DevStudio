<process draft="false" dev-studio-version="7.5.02.02">
  <process-def name=":AutoPilot-OA-Virtual_Server_Management:Utilities:BladeLogic:Create Provisioning Input File">
    <parameters>
      <input global="false" required="false">
        <description>File Adapter Name</description>
        <from>adapter name</from>
        <to>adapter name</to>
      </input>
      <input global="false" required="true">
        <from>file name</from>
        <to>file name</to>
      </input>
      <input global="false" required="true">
        <from>xml doc name</from>
        <to>xml doc name</to>
      </input>
      <output global="false" required="true">
        <to>success status</to>
        <from>success status</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <call-process>
        <process-name>:AutoPilot-AD-Utilities:Terminal:File:Write</process-name>
        <parameters>
          <input>
            <from>adapter name</from>
            <to>adapter name</to>
          </input>
          <input>
            <from>file name</from>
            <to>file name</to>
          </input>
          <input>
            <value>xml</value>
            <to>file type</to>
          </input>
          <input>
            <from>xml doc name</from>
            <to>xml doc</to>
          </input>
          <output>
            <from>adapter response</from>
            <to>success status</to>
            <xslt-stylesheet>
              <stylesheet-replacement-tokens />
              <stylesheet><![CDATA[<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output indent="no" />
  <xsl:template match="/">
    <value>
      <xsl:choose>
        <xsl:when test="//status/text()='success'">
          <xsl:text disable-output-escaping="no">true</xsl:text>
        </xsl:when>
        <xsl:otherwise>
          <xsl:text disable-output-escaping="no">false</xsl:text>
        </xsl:otherwise>
      </xsl:choose>
    </value>
  </xsl:template>
</xsl:stylesheet>]]></stylesheet>
            </xslt-stylesheet>
          </output>
        </parameters>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>SEMI_AUTOMATIC</run-mode>
            <allow-step-into>false</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <console-output>
            <type>INFO</type>
            <summary>
              <concat>
                <input>
                  <value />
                </input>
              </concat>
            </summary>
            <detail>
              <concat>
                <input>
                  <value />
                </input>
              </concat>
            </detail>
          </console-output>
        </after-execution>
        <location>:call-process[1]</location>
      </call-process>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description> </description>
        <version />
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
    </meta>
  </process-def>
</process>