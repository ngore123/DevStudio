<process draft="false" dev-studio-version="7.5.02.04">
  <process-def name=":AutoPilot-OA-Virtual_Server_Management:Create VM">
    <parameters>
      <input global="false" required="true">
        <description>String Value - The name of a configuration entry defined in a configuration module. 
</description>
        <from>configuration name</from>
        <to>configuration name</to>
      </input>
      <input global="false" required="true">
        <description>An XML document providing the data template for single or multiple Concept elements.The virtual server concept is an XML document containing specific elements used to assign multiple values to a single context item. Each value is associated with an element of the Concept.

Sample:

&lt;Virtual-Server&gt;
  &lt;ip-address/&gt;
  &lt;mac-address /&gt;
  &lt;power-status/&gt;
  &lt;name/&gt;
  &lt;datacenter/&gt;
  &lt;datastore/&gt;
  &lt;host-name/&gt;
  &lt;resource-pool/&gt;
  &lt;disk-info/&gt;
  &lt;expiry-date/&gt;
  &lt;package-id/&gt;
  &lt;destination/&gt;
  &lt;srm-id/&gt;
  &lt;change-id/&gt;
  &lt;task-id/&gt;
  &lt;owner/&gt;
  &lt;network-info/&gt;
  &lt;file-repository/&gt;
  &lt;boot-image-id/&gt;
  &lt;approval-type/&gt;
  &lt;change-type/&gt;
  &lt;description/&gt;
  &lt;impact-id/&gt;
  &lt;risk-level/&gt;
  &lt;property-name/&gt;
  &lt;server-type/&gt;
  &lt;approval-id/&gt;
  &lt;package-key/&gt;
  &lt;job-key/&gt;
  &lt;template-id/&gt;
  &lt;file-adapter-name/&gt;
  &lt;vm-details/&gt;
  &lt;job-folder-id/&gt;
  &lt;job-name/&gt;
  &lt;timeout/&gt;
  &lt;user-role/&gt;
  &lt;etKey/&gt;
 &lt;/Virtual-Server&gt;</description>
        <from>virtual server</from>
        <to>virtual server</to>
      </input>
      <output global="false" required="false">
        <description>String Value - Indicates whether or not the process completed successfully. </description>
        <to>success status</to>
        <from>success status</from>
      </output>
      <output global="false" required="false">
        <description>String Value - A process specific explanation for the value of success status.</description>
        <to>status message</to>
        <from>status message</from>
      </output>
      <output global="false" required="false">
        <description>String Value - Indicates whether or not the process completed successfully. </description>
        <to>file maintenance success status</to>
        <from>file maintenance success status</from>
      </output>
      <output global="false" required="false">
        <description>String Value - A process specific explanation for the value of success status.</description>
        <to>file maintenance status message</to>
        <from>file maintenance status message</from>
      </output>
    </parameters>
    <access />
    <sequence>
      <switch>
        <meta>
          <documentation />
          <run-mode-handling>
            <run-mode>SEMI_AUTOMATIC</run-mode>
            <allow-step-into>true</allow-step-into>
          </run-mode-handling>
          <checkpoint>false</checkpoint>
        </meta>
        <execute-all-applicable-cases>false</execute-all-applicable-cases>
        <default>
          <sequence>
            <assign>
              <assignment>
                <output>
                  <to>success status</to>
                  <value>failure</value>
                </output>
              </assignment>
              <assignment>
                <output>
                  <to>status message</to>
                  <value>Given system is not supported</value>
                </output>
              </assignment>
              <meta>
                <documentation />
                <checkpoint>false</checkpoint>
              </meta>
              <location>:switch[1]:sequence[1]:assign[1]</location>
            </assign>
          </sequence>
        </default>
        <case>
          <condition>
            <document>
              <input>
                <from>configuration name</from>
              </input>
            </document>
            <xpath-expression>
              <xpath-replacement-tokens />
              <xpath><![CDATA[string(.) = 'BladeLogic']]></xpath>
            </xpath-expression>
          </condition>
          <sequence>
            <console-output>
              <type>INFO</type>
              <summary>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </summary>
              <detail>
                <concat>
                  <input>
                    <value></value>
                  </input>
                </concat>
              </detail>
            </console-output>
            <call-process>
              <process-name>:AutoPilot-OA-Virtual_Server_Management:Utilities:BladeLogic:Create VM</process-name>
              <parameters>
                <input>
                  <from>virtual server</from>
                  <to>file adapter name</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//file-adapter-name/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>virtual server</from>
                  <to>srm id</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//srm-id/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>virtual server</from>
                  <to>change id</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//change-id/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>virtual server</from>
                  <to>task id</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//task-id/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>virtual server</from>
                  <to>job folder ID</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//job-folder-id/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>virtual server</from>
                  <to>job name</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//job-name/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>virtual server</from>
                  <to>vm details</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//vm-details/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>virtual server</from>
                  <to>timeout</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//timeout/text()]]></xpath>
                  </xpath-expression>
                </input>
                <input>
                  <from>virtual server</from>
                  <to>user role</to>
                  <xpath-expression>
                    <xpath-replacement-tokens />
                    <xpath><![CDATA[//user-role/text()]]></xpath>
                  </xpath-expression>
                </input>
                <output>
                  <from>success status</from>
                  <to>success status</to>
                </output>
                <output>
                  <from>status message</from>
                  <to>status message</to>
                </output>
                <output>
                  <from>file maintenance success status</from>
                  <to>file maintenance success status</to>
                </output>
                <output>
                  <from>file maintenance status message</from>
                  <to>file maintenance status message</to>
                </output>
              </parameters>
              <meta>
                <documentation />
                <run-mode-handling>
                  <run-mode>SEMI_AUTOMATIC</run-mode>
                  <allow-step-into>false</allow-step-into>
                </run-mode-handling>
                <checkpoint>false</checkpoint>
              </meta>
              <after-execution>
                <console-output>
                  <type>INFO</type>
                  <summary>
                    <concat>
                      <input>
                        <value></value>
                      </input>
                    </concat>
                  </summary>
                  <detail>
                    <concat>
                      <input>
                        <value></value>
                      </input>
                    </concat>
                  </detail>
                </console-output>
              </after-execution>
              <location>:switch[1]:sequence[2]:call-process[1]</location>
            </call-process>
          </sequence>
        </case>
        <location>:switch[1]</location>
      </switch>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>This action creates a virtual server</description>
        <version></version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
    </meta>
  </process-def>
</process>