<process draft="false" dev-studio-version="7.6.01.02">
  <process-def name=":AO-AD-HP_ServiceCenter:Utilities:Operation Name Error Reporting">
    <parameters>
      <input global="false" required="false">
        <description>The correct operation name. 
</description>
        <from>operation name</from>
        <to>operation name</to>
      </input>
      <input global="false" required="false">
        <description>This item xml is taken for future use. For example if we want to display what operation name the user had provided. 
</description>
        <from>items</from>
        <to>items</to>
      </input>
    </parameters>
    <access />
    <sequence>
      <utility>
        <string>
          <concat>
            <input>
              <concat>
                <input>
                  <value>  Incorrect operation name specified in the input. Operation name which is valid for this wrapper is: </value>
                </input>
              </concat>
            </input>
            <input>
              <from>operation name</from>
            </input>
            <input>
              <concat>
                <input>
                  <value>.</value>
                </input>
              </concat>
            </input>
          </concat>
        </string>
        <output global="false">
          <to>errorstring</to>
        </output>
        <meta>
          <documentation />
          <checkpoint>false</checkpoint>
        </meta>
        <after-execution>
          <log>
            <level>debug</level>
            <parameters>
              <input>
                <value>ErrorString:</value>
              </input>
              <input>
                <from>errorstring</from>
              </input>
            </parameters>
          </log>
        </after-execution>
        <location>:utility[1]</location>
      </utility>
    </sequence>
    <meta>
      <expose-to>
        <consumer>RULE</consumer>
        <consumer>SCHEDULE</consumer>
      </expose-to>
      <documentation>
        <description>Operation Name Error Reporting 

Sets and debugs the error string in case an unsupported operation name is provided in the input </description>
        <version>1.0</version>
      </documentation>
      <peer-location>
        <location>any</location>
      </peer-location>
      <record-metrics>false</record-metrics>
